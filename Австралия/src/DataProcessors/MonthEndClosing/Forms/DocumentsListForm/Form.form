    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DocumentMonthEndClosing.Ref AS Ref,
	DocumentMonthEndClosing.DeletionMark AS DeletionMark,
	DocumentMonthEndClosing.Number AS Number,
	DocumentMonthEndClosing.Date AS Date,
	DocumentMonthEndClosing.Posted AS Posted,
	DocumentMonthEndClosing.Company AS Company,
	DocumentMonthEndClosing.Comment AS Comment,
	DocumentMonthEndClosing.Author AS Author,
	DocumentMonthEndClosing.PointInTime AS PointInTime,
	&amp;StringDocumentMonthEndClosing AS DocumentType,
	CASE
		WHEN DocumentMonthEndClosing.Posted
			THEN 1
		WHEN DocumentMonthEndClosing.DeletionMark
			THEN 3
		ELSE 0
	END AS NumberDocumentStatus
FROM
	Document.MonthEndClosing AS DocumentMonthEndClosing

UNION ALL

SELECT
	FixedAssetsDepreciation.Ref,
	FixedAssetsDepreciation.DeletionMark,
	FixedAssetsDepreciation.Number,
	FixedAssetsDepreciation.Date,
	FixedAssetsDepreciation.Posted,
	FixedAssetsDepreciation.Company,
	FixedAssetsDepreciation.Comment,
	FixedAssetsDepreciation.Author,
	FixedAssetsDepreciation.PointInTime,
	&amp;StringFixedAssetsDepreciation,
	CASE
		WHEN FixedAssetsDepreciation.Posted
			THEN 1 WHEN FixedAssetsDepreciation.DeletionMark
			THEN 3
		ELSE 0
	END
FROM
	Document.FixedAssetsDepreciation AS FixedAssetsDepreciation</queryText>
