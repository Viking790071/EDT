    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT ALLOWED TOP 1
	SalesInvoiceSerialNumbers.Ref AS Ref
INTO SerialNumbersTable
FROM
	Document.SalesInvoice.SerialNumbers AS SalesInvoiceSerialNumbers
WHERE
	SalesInvoiceSerialNumbers.SerialNumber = &amp;SerialNumber

UNION ALL

SELECT TOP 1
	SalesSlipSerialNumbers.Ref
FROM
	Document.SalesSlip.SerialNumbers AS SalesSlipSerialNumbers
WHERE
	SalesSlipSerialNumbers.SerialNumber = &amp;SerialNumber
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	UNDEFINED AS Ref,
	DATETIME(1, 1, 1) AS Date,
	&quot;00000000000&quot; AS Number,
	VALUE(Catalog.Companies.EmptyRef) AS Company,
	VALUE(Catalog.Counterparties.EmptyRef) AS Counterparty,
	VALUE(Catalog.CounterpartyContracts.EmptyRef) AS Contract,
	0 AS Amount,
	VALUE(Catalog.Currencies.EmptyRef) AS Currency,
	UNDEFINED AS Type
WHERE
	FALSE

UNION ALL

SELECT DISTINCT
	SalesInvoice.Ref,
	SalesInvoice.Date,
	SalesInvoice.Number,
	SalesInvoice.Company,
	SalesInvoice.Counterparty,
	SalesInvoice.Contract,
	SalesInvoice.DocumentAmount,
	SalesInvoice.DocumentCurrency,
	TYPE(Document.SalesInvoice)
FROM
	Document.SalesInvoice AS SalesInvoice
		INNER JOIN Document.SalesInvoice.Inventory AS SalesInvoiceInventory
		ON SalesInvoice.Ref = SalesInvoiceInventory.Ref
		LEFT JOIN SerialNumbersTable AS SerialNumbersTable
		ON SalesInvoice.Ref = SerialNumbersTable.Ref
WHERE
	SalesInvoiceInventory.Products = &amp;Product
	AND SalesInvoiceInventory.Characteristic = &amp;Characteristic
	AND (&amp;SerialNumber = VALUE(Catalog.SerialNumbers.EmptyRef)
			OR NOT SerialNumbersTable.Ref IS NULL)

UNION ALL

SELECT DISTINCT
	SalesSlip.Ref,
	SalesSlip.Date,
	SalesSlip.Number,
	SalesSlip.Company,
	VALUE(Catalog.Counterparties.EmptyRef),
	VALUE(Catalog.CounterpartyContracts.EmptyRef),
	SalesSlip.DocumentAmount,
	SalesSlip.DocumentCurrency,
	TYPE(Document.SalesSlip)
FROM
	Document.SalesSlip AS SalesSlip
		INNER JOIN Document.SalesSlip.Inventory AS SalesSlipInventory
		ON SalesSlip.Ref = SalesSlipInventory.Ref
		LEFT JOIN SerialNumbersTable AS SerialNumbersTable
		ON SalesSlip.Ref = SerialNumbersTable.Ref
WHERE
	SalesSlipInventory.Products = &amp;Product
	AND SalesSlipInventory.Characteristic = &amp;Characteristic
	AND (&amp;SerialNumber = VALUE(Catalog.SerialNumbers.EmptyRef)
			OR NOT SerialNumbersTable.Ref IS NULL)</queryText>
