    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CatalogPeripherals.Ref AS Ref,
	CatalogPeripherals.Ref AS Peripherals,
	CatalogPeripherals.InfobaseNode.ExportDate AS ExportDate,
	CatalogPeripherals.InfobaseNode.ImportDate AS ImportDate,
	CatalogPeripherals.InfobaseNode.ExportCompleted AS ExportCompleted,
	CatalogPeripherals.InfobaseNode.ImportCompleted AS ImportCompleted,
	CASE
		WHEN CatalogPeripherals.InfobaseNode.ExportCompleted
			THEN 1
		ELSE CASE
				WHEN CatalogPeripherals.InfobaseNode.ExportDate > DATETIME(1, 1, 1)
					THEN 0
				ELSE 1
			END
	END AS ExportingPictureIndex,
	CatalogPeripherals.ExchangeRule AS ExchangeRule,
	CashRegisters.Ref AS CashCR,
	CASE
		WHEN 1 In
				(SELECT
					1 AS Field1
				FROM
					InformationRegister.ProductsCodesPeripheralOffline.Changes AS ProductCodes
				WHERE
					ProductCodes.ExchangeRule = CatalogPeripherals.ExchangeRule
					AND ProductCodes.Node = CatalogPeripherals.InfobaseNode)
			THEN 0
		ELSE 1
	END AS Changes,
	CatalogPeripherals.ExchangeRule.StructuralUnit AS Warehouse,
	CASE
		WHEN CatalogPeripherals.Workplace = &amp;CurrentWorksPlace
			THEN TRUE
		ELSE FALSE
	END AS ConnectedToCurrentWorksplace
	FROM
	Catalog.Peripherals AS CatalogPeripherals
		INNER JOIN Catalog.CashRegisters AS CashRegisters
		ON (CashRegisters.Peripherals = CatalogPeripherals.Ref)
	WHERE
	CatalogPeripherals.EquipmentType = VALUE(Enum.PeripheralTypes.CashRegistersOffline)
		AND CatalogPeripherals.DeviceIsInUse</queryText>
