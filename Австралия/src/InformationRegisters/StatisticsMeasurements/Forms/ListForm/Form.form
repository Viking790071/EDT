    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	InformationRegisterStatisticsMeasurements.RecordPeriod,
	InformationRegisterStatisticsMeasurements.EntryType,
	InformationRegisterStatisticsMeasurements.Key,
	InformationRegisterStatisticsMeasurements.StatisticsOperation,
	InformationRegisterStatisticsOperations.Description AS StatisticsOperationDescription,
	InformationRegisterStatisticsMeasurements.DeletionID,
	InformationRegisterStatisticsMeasurements.OperationValue
FROM
	InformationRegister.StatisticsMeasurements AS InformationRegisterStatisticsMeasurements
LEFT JOIN
	InformationRegister.StatisticsOperations AS InformationRegisterStatisticsOperations
ON
	InformationRegisterStatisticsOperations.OperationID = InformationRegisterStatisticsMeasurements.StatisticsOperation 	</queryText>
      <mainTable>InformationRegister.StatisticsMeasurements</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
