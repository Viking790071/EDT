    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	InformationRegisterConfigurationStatistics.StatisticsOperation,
	InformationRegisterConfigurationStatistics.Value,
	CASE InformationRegisterConfigurationStatistics.StatisticsKind
		WHEN 0 THEN InformationRegisterConfigurationStatistics.Value
		WHEN 1 THEN
			CASE InformationRegisterConfigurationStatistics.Value
				WHEN 0 THEN FALSE
				WHEN 1 THEN TRUE
			END
	END AS ValuePresentation,
	StatisticsOperations.Description,
	InformationRegisterConfigurationStatistics.StatisticsKind AS StatisticsKind,
	CASE InformationRegisterConfigurationStatistics.StatisticsKind
		WHEN 0 THEN &amp;Metadata
		WHEN 1 THEN &amp;FunctionalOption
	END AS StatisticsKindPresentation,
	InformationRegisterConfigurationStatistics.StatisticsAreaID,
	StatisticsAreas.Description AS AreaDescription
FROM
	InformationRegister.ConfigurationStatistics AS InformationRegisterConfigurationStatistics
		LEFT JOIN InformationRegister.StatisticsOperations AS StatisticsOperations
		ON InformationRegisterConfigurationStatistics.StatisticsOperation = StatisticsOperations.OperationID
		LEFT JOIN InformationRegister.StatisticsAreas AS StatisticsAreas
		ON InformationRegisterConfigurationStatistics.StatisticsAreaID = StatisticsAreas.AreaID</queryText>
      <mainTable>InformationRegister.ConfigurationStatistics</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
