    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Period</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	InformationRegisterAccountingPolicy.Period AS Period,
	InformationRegisterAccountingPolicy.Company AS Company,
	InformationRegisterAccountingPolicy.RegisteredForVAT AS RegisteredForVAT,
	InformationRegisterAccountingPolicy.RegisteredForVAT
		AND InformationRegisterAccountingPolicy.PostVATEntriesBySourceDocuments AS PostVATEntriesBySourceDocuments,
	InformationRegisterAccountingPolicy.RegisteredForVAT
		AND InformationRegisterAccountingPolicy.PostAdvancePaymentsBySourceDocuments AS PostAdvancePaymentsBySourceDocuments,
	InformationRegisterAccountingPolicy.DefaultVATRate AS DefaultVATRate,
	InformationRegisterAccountingPolicy.CashMethodOfAccounting AS CashMethodOfAccounting,
	InformationRegisterAccountingPolicy.InventoryValuationMethod AS InventoryValuationMethod,
	CASE
		WHEN InformationRegisterAccountingPolicy.UseGoodsReturnFromCustomer
			THEN &amp;CreditNoteAndGoodsReceipt
		ELSE &amp;CreditNote
	END AS SalesReturnFlow,
	CASE
		WHEN InformationRegisterAccountingPolicy.UseGoodsReturnToSupplier
			THEN &amp;DebitNoteAndGoodsIssue
		ELSE &amp;DebitNote
	END AS PurchaseReturnFlow,
	InformationRegisterAccountingPolicy.RegisteredForSalesTax AS RegisteredForSalesTax,
	InformationRegisterAccountingPolicy.RegisterDeliveryDateInInvoices AS RegisterDeliveryDateInInvoices,
	InformationRegisterAccountingPolicy.InventoryDispatchingStrategy AS InventoryDispatchingStrategy,
	InformationRegisterAccountingPolicy.RegisterAccountingEntries AS RegisterAccountingEntries,
	InformationRegisterAccountingPolicy.UseTemplates AS UseTemplates
FROM
	InformationRegister.AccountingPolicy AS InformationRegisterAccountingPolicy</queryText>
      <mainTable>InformationRegister.AccountingPolicy</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
