  <name>UseOverdraft</name>
  <synonym>
    <key>en</key>
    <value>Use overdraft</value>
  </synonym>
  <synonym>
    <key>ru</key>
    <value>Использовать овердрафт</value>
  </synonym>
  <synonym>
    <key>pl</key>
    <value>Używaj przekroczenia stanu rachunku</value>
  </synonym>
  <synonym>
    <key>es_ES</key>
    <value>Utilizar el sobregiro</value>
  </synonym>
  <synonym>
    <key>es_CO</key>
    <value>Utilizar el sobregiro</value>
  </synonym>
  <synonym>
    <key>tr</key>
    <value>Fazla para çekme kullan</value>
  </synonym>
  <synonym>
    <key>it</key>
    <value>Utilizzare scoperto</value>
  </synonym>
  <synonym>
