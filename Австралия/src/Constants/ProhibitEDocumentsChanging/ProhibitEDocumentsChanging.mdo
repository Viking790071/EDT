  <name>ProhibitEDocumentsChanging</name>
  <synonym>
    <key>en</key>
    <value>Deny changing e-documents</value>
  </synonym>
  <synonym>
    <key>ru</key>
    <value>Запретить изменения электронных документов</value>
  </synonym>
  <synonym>
    <key>pl</key>
    <value>Zabroń zmianę dokumentów elektronicznych</value>
  </synonym>
  <synonym>
    <key>es_ES</key>
    <value>Denegar el cambio de documentos electrónicos</value>
  </synonym>
  <synonym>
