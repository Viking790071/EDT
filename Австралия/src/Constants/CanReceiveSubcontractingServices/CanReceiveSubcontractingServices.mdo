  <name>CanReceiveSubcontractingServices</name>
  <synonym>
    <key>en</key>
    <value>Receive subcontracting services</value>
  </synonym>
  <synonym>
    <key>ru</key>
    <value>Получать услуги переработки</value>
  </synonym>
  <synonym>
    <key>pl</key>
    <value>Otrzymuj usługi podwykonawstwa</value>
  </synonym>
  <synonym>
    <key>es_ES</key>
    <value>Recibir servicios de subcontratación</value>
  </synonym>
  <synonym>
    <key>es_CO</key>
    <value>Recibir servicios de subcontratación</value>
  </synonym>
