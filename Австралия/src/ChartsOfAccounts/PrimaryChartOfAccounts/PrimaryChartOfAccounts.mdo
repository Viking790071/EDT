    <name>TypeOfAccount</name>
    <synonym>
      <key>en</key>
      <value>Account type</value>
    </synonym>
    <synonym>
      <key>ru</key>
      <value>Тип счета</value>
    </synonym>
    <synonym>
      <key>pl</key>
      <value>Rodzaj konta</value>
    </synonym>
    <synonym>
      <key>es_ES</key>
      <value>Tipo de cuenta</value>
    </synonym>
    <synonym>
      <key>es_CO</key>
      <value>Tipo de cuenta</value>
    </synonym>
    <synonym>
      <key>tr</key>
      <value>Hesap türü</value>
    </synonym>
    <synonym>
      <key>it</key>
      <value>Tipologia di conto</value>
    </synonym>
    <synonym>
