    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DocumentJournalAccountingDocuments.Ref AS Ref,
	DocumentJournalAccountingDocuments.Date AS Date,
	DocumentJournalAccountingDocuments.DeletionMark AS DeletionMark,
	DocumentJournalAccountingDocuments.Number AS Number,
	DocumentJournalAccountingDocuments.Posted AS Posted,
	DocumentJournalAccountingDocuments.Type AS Type,
	DocumentAccountingEntriesStatuses.Company AS Company,
	DocumentAccountingEntriesStatuses.Status AS Status,
	DocumentAccountingEntriesStatuses.Status = VALUE(Enum.AccountingEntriesStatus.Approved) AS Approved,
	DocumentAccountingEntriesStatuses.AdjustedManually AS AdjustedManually,
	DocumentAccountingEntriesStatuses.ApprovalDate AS ApprovalDate,
	DocumentAccountingEntriesStatuses.Counterparty AS Counterparty,
	DocumentAccountingEntriesStatuses.ChartOfAccounts AS ChartOfAccounts,
	DocumentAccountingEntriesStatuses.DocumentCurrency AS Currency,
	DocumentAccountingEntriesStatuses.DocumentAmount AS Amount,
	DocumentAccountingEntriesStatuses.OperationKind AS OperationKind,
	DocumentAccountingEntriesStatuses.Author AS Author,
	DocumentAccountingEntriesStatuses.Comment AS Comment,
	CAST(NULL AS NUMBER(1, 0)) AS HasFiles
FROM
	DocumentJournal.SourceDocuments AS DocumentJournalAccountingDocuments
		INNER JOIN InformationRegister.DocumentAccountingEntriesStatuses AS DocumentAccountingEntriesStatuses
		ON DocumentJournalAccountingDocuments.Ref = DocumentAccountingEntriesStatuses.Recorder
WHERE
	DocumentAccountingEntriesStatuses.TypeOfAccounting = VALUE(Catalog.TypesOfAccounting.EmptyRef)
				AND 
			DocumentAccountingEntriesStatuses.ChartOfAccounts = VALUE(Catalog.ChartsOfAccounts.EmptyRef)</queryText>
      <mainTable>DocumentJournal.SourceDocuments</mainTable>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
