    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	Tasks.Ref,
	Tasks.DeletionMark,
	Tasks.Number,
	Tasks.Date,
	Tasks.BusinessProcess,
	Tasks.RoutePoint,
	Tasks.Description,
	Tasks.Executed,
	Tasks.Importance,
	CASE
		WHEN Tasks.Importance = VALUE(Enum.TaskImportanceOptions.Low)
			THEN 0
		WHEN Tasks.Importance = VALUE(Enum.TaskImportanceOptions.High)
			THEN 2
		ELSE 1
	END AS ImportancePicture,
	Tasks.CompletionDate,
	Tasks.Author,
	Tasks.Details,
	Tasks.DueDate,
	Tasks.ExecutionResult,
	Tasks.Performer,
	Tasks.PerformerRole,
	Tasks.MainAddressingObject,
	Tasks.AdditionalAddressingObject,
	Tasks.Presentation,
	Tasks.AcceptedForExecution,
	CASE
		WHEN Tasks.BusinessProcessState = VALUE(Enum.BusinessProcessStates.Stopped)
			THEN True
		ELSE False
	END AS Stopped
FROM
	Task.PerformerTask AS Tasks
	
WHERE Tasks.Topic = &amp;FilterParameter 
	AND Tasks.BusinessProcessState = VALUE(Enum.BusinessProcessStates.Running)</queryText>
