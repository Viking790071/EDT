		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN InventoryBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE InventoryBalanceAndTurnovers.Recorder
	END AS Recorder,
	InventoryBalanceAndTurnovers.SecondPeriod AS SecondPeriod,
	InventoryBalanceAndTurnovers.MinutePeriod AS MinutePeriod,
	InventoryBalanceAndTurnovers.HourPeriod AS HourPeriod,
	InventoryBalanceAndTurnovers.DayPeriod AS DayPeriod,
	InventoryBalanceAndTurnovers.WeekPeriod AS WeekPeriod,
	InventoryBalanceAndTurnovers.TenDaysPeriod AS TenDaysPeriod,
	InventoryBalanceAndTurnovers.MonthPeriod AS MonthPeriod,
	InventoryBalanceAndTurnovers.QuarterPeriod AS QuarterPeriod,
	InventoryBalanceAndTurnovers.YearPeriod AS YearPeriod,
	InventoryBalanceAndTurnovers.Company AS Company,
	InventoryBalanceAndTurnovers.PresentationCurrency AS PresentationCurrency,
	InventoryBalanceAndTurnovers.StructuralUnit AS StructuralUnit,
	InventoryBalanceAndTurnovers.Products AS Products,
	InventoryBalanceAndTurnovers.Characteristic AS Characteristic,
	InventoryBalanceAndTurnovers.Batch AS Batch,
	InventoryBalanceAndTurnovers.Ownership AS Ownership,
	InventoryBalanceAndTurnovers.QuantityOpeningBalance AS QuantityOpeningBalance,
	InventoryBalanceAndTurnovers.QuantityClosingBalance AS QuantityClosingBalance,
	InventoryBalanceAndTurnovers.QuantityTurnover AS QuantityTurnover,
	InventoryBalanceAndTurnovers.QuantityReceipt AS QuantityReceipt,
	InventoryBalanceAndTurnovers.QuantityExpense AS QuantityExpense,
	InventoryBalanceAndTurnovers.AmountOpeningBalance AS AmountOpeningBalance,
	InventoryBalanceAndTurnovers.AmountClosingBalance AS AmountClosingBalance,
	InventoryBalanceAndTurnovers.AmountTurnover AS AmountTurnover,
	InventoryBalanceAndTurnovers.AmountReceipt AS AmountReceipt,
	InventoryBalanceAndTurnovers.AmountExpense AS AmountExpense,
	InventoryBalanceAndTurnovers.Products.MeasurementUnit AS ProductsMeasurementUnit,
	CASE
		WHEN InventoryBalanceAndTurnovers.Products.ConversionRate = 0
			THEN InventoryBalanceAndTurnovers.QuantityOpeningBalance
		ELSE InventoryBalanceAndTurnovers.QuantityOpeningBalance / InventoryBalanceAndTurnovers.Products.ConversionRate
	END AS ReportOpeningBalance,
	CASE
		WHEN InventoryBalanceAndTurnovers.Products.ConversionRate = 0
			THEN InventoryBalanceAndTurnovers.QuantityClosingBalance
		ELSE InventoryBalanceAndTurnovers.QuantityClosingBalance / InventoryBalanceAndTurnovers.Products.ConversionRate
	END AS ReportClosingBalance,
	CASE
		WHEN InventoryBalanceAndTurnovers.Products.ConversionRate = 0
			THEN InventoryBalanceAndTurnovers.QuantityTurnover
		ELSE InventoryBalanceAndTurnovers.QuantityTurnover / InventoryBalanceAndTurnovers.Products.ConversionRate
	END AS ReportTurnover,
	CASE
		WHEN InventoryBalanceAndTurnovers.Products.ConversionRate = 0
			THEN InventoryBalanceAndTurnovers.QuantityReceipt
		ELSE InventoryBalanceAndTurnovers.QuantityReceipt / InventoryBalanceAndTurnovers.Products.ConversionRate
	END AS ReportReceipt,
	CASE
		WHEN InventoryBalanceAndTurnovers.Products.ConversionRate = 0
			THEN InventoryBalanceAndTurnovers.QuantityExpense
		ELSE InventoryBalanceAndTurnovers.QuantityExpense / InventoryBalanceAndTurnovers.Products.ConversionRate
	END AS ReportExpense,
	InventoryBalanceAndTurnovers.Products.ReportUOM AS ProductsReportUOM,
	InventoryBalanceAndTurnovers.InventoryAccountType AS InventoryAccountType
FROM
	AccumulationRegister.Inventory.BalanceAndTurnovers(
			,
			,
			Auto,
			,
			VALUETYPE(StructuralUnit) = TYPE(Catalog.BusinessUnits)
				AND StructuralUnit &lt;&gt; VALUE(Catalog.BusinessUnits.DropShipping)) AS InventoryBalanceAndTurnovers</query>
	</dataSet>
