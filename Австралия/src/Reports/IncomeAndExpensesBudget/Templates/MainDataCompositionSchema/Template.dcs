		<expression>0</expression>
		<group>GLAccount</group>
		<group>IncomeAndExpenseItem</group>
	</totalField>
	<totalField>
		<dataPath>ProfitLossFact</dataPath>
		<expression>SUM(AmountIncomeTurnoverFact)-SUM(AmountExpenseTurnoverFact)</expression>
		<group>SalesOrder</group>
		<group>BusinessLine</group>
		<group>Company</group>
		<group>Deviation</group>
		<group>PlanningPeriod</group>
		<group>YearPeriod</group>
		<group>DayPeriod</group>
		<group>QuarterPeriod</group>
		<group>MonthPeriod</group>
		<group>MinutePeriod</group>
		<group>WeekPeriod</group>
		<group>SecondPeriod</group>
		<group>HourPeriod</group>
		<group>Percent</group>
		<group>Recorder</group>
		<group>StructuralUnit</group>
		<group>Overall</group>
	</totalField>
	<totalField>
		<dataPath>Percent</dataPath>
		<expression>CASE
 WHEN SUM(AmountIncomeTurnover) IS NULL OR SUM(AmountIncomeTurnover)-SUM(AmountExpenseTurnover) = 0
 THEN "-"
 ELSE (ISNULL(SUM(AmountIncomeTurnoverFact), 0) - ISNULL(SUM(AmountExpenseTurnoverFact), 0) - (SUM(AmountIncomeTurnover)-SUM(AmountExpenseTurnover)) ) * 100 / ( SUM(AmountIncomeTurnover)-SUM(AmountExpenseTurnover) ) END</expression>
		<group>SalesOrder</group>
		<group>BusinessLine</group>
		<group>Company</group>
		<group>PlanningPeriod</group>
		<group>YearPeriod</group>
		<group>DayPeriod</group>
		<group>QuarterPeriod</group>
		<group>MonthPeriod</group>
		<group>MinutePeriod</group>
		<group>WeekPeriod</group>
		<group>SecondPeriod</group>
		<group>HourPeriod</group>
		<group>Recorder</group>
		<group>StructuralUnit</group>
		<group>Overall</group>
	</totalField>
	<totalField>
		<dataPath>Deviation</dataPath>
		<expression>ISNULL(SUM(AmountIncomeTurnoverFact)- SUM(AmountExpenseTurnoverFact), 0) - ISNULL(SUM(AmountIncomeTurnover)- SUM(AmountExpenseTurnover), 0)</expression>
		<group>SalesOrder</group>
		<group>BusinessLine</group>
		<group>Company</group>
		<group>PlanningPeriod</group>
		<group>YearPeriod</group>
		<group>DayPeriod</group>
		<group>QuarterPeriod</group>
		<group>MonthPeriod</group>
		<group>MinutePeriod</group>
		<group>WeekPeriod</group>
		<group>SecondPeriod</group>
		<group>HourPeriod</group>
		<group>Recorder</group>
		<group>StructuralUnit</group>
		<group>Overall</group>
	</totalField>
	<totalField>
		<dataPath>IncomePercent</dataPath>
		<expression>CASE WHEN SUM(AmountIncomeTurnover) IS NULL OR SUM(AmountIncomeTurnover) = 0
 THEN "-" ELSE (ISNULL (SUM(AmountIncomeTurnoverFact), 0) - SUM(AmountIncomeTurnover)) * 100 / SUM(AmountIncomeTurnover) END</expression>
	</totalField>
	<totalField>
		<dataPath>IncomeRejection</dataPath>
		<expression>ISNULL(SUM(AmountIncomeTurnoverFact), 0) - ISNULL(SUM(AmountIncomeTurnover), 0)</expression>
	</totalField>
	<totalField>
		<dataPath>ExpensePercent</dataPath>
		<expression>CASE WHEN SUM(AmountExpenseTurnover) IS NULL OR SUM(AmountExpenseTurnover)
 = 0 THEN
 "-" ELSE (ISNULL (SUM(AmountExpenseTurnoverFact), 0) - SUM(AmountExpenseTurnover)) * 100 / SUM(AmountExpenseTurnover) END</expression>
	</totalField>
