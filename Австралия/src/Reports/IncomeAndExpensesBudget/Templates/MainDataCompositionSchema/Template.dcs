			<query>SELECT ALLOWED
	IncomeAndExpensesBudget.Recorder AS Recorder,
	DAY(IncomeAndExpensesBudget.Period) AS DayPeriod,
	MONTH(IncomeAndExpensesBudget.Period) AS MonthPeriod,
	IncomeAndExpensesBudget.Company AS Company,
	Companies.PresentationCurrency AS PresentationCurrency,
	IncomeAndExpensesBudget.StructuralUnit AS StructuralUnit,
	IncomeAndExpensesBudget.SalesOrder AS SalesOrder,
	IncomeAndExpensesBudget.BusinessLine AS BusinessLine,
	IncomeAndExpensesBudget.GLAccount AS GLAccount,
	IncomeAndExpensesBudget.IncomeAndExpenseItem AS IncomeAndExpenseItem,
	IncomeAndExpensesBudget.AmountIncome AS AmountIncomeTurnover,
	IncomeAndExpensesBudget.AmountExpense AS AmountExpenseTurnover,
	SECOND(IncomeAndExpensesBudget.Period) AS SecondPeriod,
	MINUTE(IncomeAndExpensesBudget.Period) AS MinutePeriod,
	HOUR(IncomeAndExpensesBudget.Period) AS HourPeriod,
	WEEK(IncomeAndExpensesBudget.Period) AS WeekPeriod,
	QUARTER(IncomeAndExpensesBudget.Period) AS QuarterPeriod,
	YEAR(IncomeAndExpensesBudget.Period) AS YearPeriod,
	0 AS ProfitLoss,
	IncomeAndExpensesBudget.PlanningPeriod AS PlanningPeriod,
	0 AS Deviation,
	0 AS Percent,
	0 AS IncomeRejection,
	0 AS ExpenseVariance,
	0 AS IncomePercent,
	0 AS ExpensePercent
FROM
	AccumulationRegister.IncomeAndExpensesBudget AS IncomeAndExpensesBudget
		LEFT JOIN Catalog.Companies AS Companies
		ON IncomeAndExpensesBudget.Company = Companies.Ref
WHERE
	(IncomeAndExpensesBudget.PlanningPeriod = &amp;PlanningPeriod
			OR &amp;PlanningPeriod = VALUE(Catalog.PlanningPeriods.EmptyRef))
{WHERE
	(IncomeAndExpensesBudget.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod)}</query>
