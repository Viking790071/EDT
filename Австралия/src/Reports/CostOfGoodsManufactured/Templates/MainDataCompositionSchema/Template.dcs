			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	ProductRelease.Company AS CompanyProducts,
	Manufacturing.StructuralUnit AS ProductionSubdepartment,
	ProductRelease.Products AS Products,
	ProductRelease.Characteristic AS ProductCharacteristic,
	ProductRelease.Batch AS BatchProducts,
	ProductRelease.SalesOrder AS SalesOrderProducts,
	ProductRelease.Specification AS ProductionSpecification,
	ProductRelease.Quantity AS ProductsQuantity,
	ProductRelease.Products.MeasurementUnit AS UnOfMeasProducts,
	BillsOfMaterials.Quantity AS BOMQuantity,
	CASE
		WHEN BillsOfMaterials.Quantity &gt; 1
			THEN CASE
					WHEN (CAST(ProductRelease.Quantity / BillsOfMaterials.Quantity AS NUMBER(15, 0))) = ProductRelease.Quantity / BillsOfMaterials.Quantity
						THEN ProductRelease.Quantity
					ELSE ((CAST(ProductRelease.Quantity / BillsOfMaterials.Quantity - 0.5 AS NUMBER(15, 0))) + 1) * BillsOfMaterials.Quantity
				END
		ELSE ProductRelease.Quantity
	END AS RoundedQuantity,
	ProductRelease.Recorder AS Production
INTO TT_ProductionWithBOM
FROM
	AccumulationRegister.ProductRelease AS ProductRelease
		INNER JOIN Catalog.BillsOfMaterials AS BillsOfMaterials
		ON ProductRelease.Specification = BillsOfMaterials.Ref
		INNER JOIN Document.Manufacturing AS Manufacturing
		ON ProductRelease.Recorder = Manufacturing.Ref
WHERE
	ProductRelease.Quantity &lt;&gt; 0
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN ProductRelease.Period &gt;= &amp;BeginOfPeriod
			ELSE ProductRelease.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END
	AND Manufacturing.OperationKind = VALUE(Enum.OperationTypesProduction.Assembly)
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_ProductionWithBOM.ProductionSpecification AS ProductionSpecification,
	BillsOfMaterialsContent.Products AS Products,
	CASE
		WHEN BillsOfMaterialsContent.CalculationMethod = VALUE(Enum.BOMContentCalculationMethod.Proportional)
			THEN BillsOfMaterialsContent.Quantity / TT_ProductionWithBOM.BOMQuantity * ISNULL(ProductUOM.Factor, 1) * TT_ProductionWithBOM.ProductsQuantity
		ELSE BillsOfMaterialsContent.Quantity / TT_ProductionWithBOM.BOMQuantity * ISNULL(ProductUOM.Factor, 1) * TT_ProductionWithBOM.RoundedQuantity
	END AS PlannedQuantity,
	BillsOfMaterialsContent.Characteristic AS Characteristic,
	TT_ProductionWithBOM.Production AS Production,
	TT_ProductionWithBOM.ProductionSubdepartment AS ProductionSubdepartment,
	TT_ProductionWithBOM.CompanyProducts AS CompanyProducts
INTO TT_PlannedQuantity
FROM
	TT_ProductionWithBOM AS TT_ProductionWithBOM
		INNER JOIN Catalog.BillsOfMaterials.Content AS BillsOfMaterialsContent
			LEFT JOIN Catalog.UOM AS ProductUOM
			ON BillsOfMaterialsContent.MeasurementUnit = ProductUOM.Ref
		ON TT_ProductionWithBOM.ProductionSpecification = BillsOfMaterialsContent.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	Inventory.ProductsCorr AS ProductsCorr,
	Inventory.CharacteristicCorr AS ProductCharacteristic,
	Inventory.BatchCorr AS BatchProducts,
	Inventory.CorrGLAccount AS ProductsGLAccount,
	Inventory.CustomerCorrOrder AS SalesOrderProducts,
	ISNULL(Inventory.SpecificationCorr, TT_PlannedQuantity.ProductionSpecification) AS ProductionSpecification,
	TT_PlannedQuantity.ProductionSubdepartment AS ProductionSubdepartment,
	ISNULL(Inventory.Company, TT_PlannedQuantity.CompanyProducts) AS CompanyProducts,
	Inventory.PresentationCurrency AS PresentationCurrency,
	ISNULL(Inventory.Products, TT_PlannedQuantity.Products) AS Products,
	ISNULL(Inventory.Characteristic, TT_PlannedQuantity.Characteristic) AS Characteristic,
	Inventory.Batch AS Batch,
	Inventory.GLAccount AS GLAccount,
	Inventory.Specification AS Specification,
	ISNULL(Inventory.Products.MeasurementUnit, TT_PlannedQuantity.Products.MeasurementUnit) AS UnOfMeas,
	Inventory.ProductsCorr.MeasurementUnit AS UnOfMeasProducts,
	TT_PlannedQuantity.PlannedQuantity AS PlannedQuantity,
	ISNULL(Inventory.Recorder, TT_PlannedQuantity.Production) AS Production
FROM
	TT_PlannedQuantity AS TT_PlannedQuantity
		LEFT JOIN AccumulationRegister.Inventory AS Inventory
		ON TT_PlannedQuantity.ProductionSpecification = Inventory.SpecificationCorr
			AND TT_PlannedQuantity.Products = Inventory.Products
			AND TT_PlannedQuantity.Characteristic = Inventory.Characteristic
			AND TT_PlannedQuantity.Production = Inventory.Recorder
			AND (Inventory.RecordType = VALUE(AccumulationRecordType.Expense))
			AND (CASE
				WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
					THEN Inventory.Period &gt;= &amp;BeginOfPeriod
				ELSE Inventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
			END)

GROUP BY
	Inventory.ProductsCorr,
	Inventory.CorrGLAccount,
	ISNULL(Inventory.SpecificationCorr, TT_PlannedQuantity.ProductionSpecification),
	TT_PlannedQuantity.ProductionSubdepartment,
	Inventory.PresentationCurrency,
	ISNULL(Inventory.Products, TT_PlannedQuantity.Products),
	Inventory.CharacteristicCorr,
	ISNULL(Inventory.Company, TT_PlannedQuantity.CompanyProducts),
	Inventory.BatchCorr,
	Inventory.CustomerCorrOrder,
	ISNULL(Inventory.Recorder, TT_PlannedQuantity.Production),
	Inventory.GLAccount,
	Inventory.Batch,
	ISNULL(Inventory.Products.MeasurementUnit, TT_PlannedQuantity.Products.MeasurementUnit),
	ISNULL(Inventory.Characteristic, TT_PlannedQuantity.Characteristic),
	Inventory.Specification,
	Inventory.ProductsCorr.MeasurementUnit,
	TT_PlannedQuantity.PlannedQuantity</query>
		</item>
