			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	InventoryReg.ProductsCorr AS ProductsCorr,
	InventoryReg.CharacteristicCorr AS ProductCharacteristic,
	InventoryReg.BatchCorr AS BatchProducts,
	InventoryReg.CorrGLAccount AS ProductsGLAccount,
	InventoryReg.CustomerCorrOrder AS SalesOrderProducts,
	InventoryReg.SpecificationCorr AS ProductionSpecification,
	Manufacturing.StructuralUnit AS ProductionSubdepartment,
	InventoryReg.Company AS CompanyProducts,
	InventoryReg.PresentationCurrency AS PresentationCurrency,
	InventoryReg.Products AS Products,
	InventoryReg.Characteristic AS Characteristic,
	InventoryReg.Batch AS Batch,
	InventoryReg.GLAccount AS GLAccount,
	SUM(InventoryReg.Amount) AS Amount,
	SUM(InventoryReg.Quantity) AS Quantity,
	SUM(0) AS ProductsQuantity,
	InventoryReg.Specification AS Specification,
	InventoryReg.Products.MeasurementUnit AS UnOfMeas,
	InventoryReg.ProductsCorr.MeasurementUnit AS UnOfMeasProducts,
	InventoryReg.Recorder AS Production
FROM
	AccumulationRegister.Inventory AS InventoryReg
		INNER JOIN Document.Manufacturing AS Manufacturing
		ON InventoryReg.Recorder = Manufacturing.Ref
WHERE
	InventoryReg.RecordType = VALUE(AccumulationRecordType.Expense)
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN InventoryReg.Period &gt;= &amp;BeginOfPeriod
			ELSE InventoryReg.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END
	AND Manufacturing.OperationKind = VALUE(Enum.OperationTypesProduction.Assembly)

GROUP BY
	InventoryReg.CharacteristicCorr,
	InventoryReg.CorrGLAccount,
	InventoryReg.CustomerCorrOrder,
	InventoryReg.ProductsCorr,
	InventoryReg.BatchCorr,
	InventoryReg.Batch,
	InventoryReg.Characteristic,
	InventoryReg.Recorder,
	InventoryReg.SpecificationCorr,
	InventoryReg.Products,
	Manufacturing.StructuralUnit,
	InventoryReg.PresentationCurrency,
	InventoryReg.Specification,
	InventoryReg.GLAccount,
	InventoryReg.Company,
	InventoryReg.Products.MeasurementUnit,
	InventoryReg.ProductsCorr.MeasurementUnit</query>
		</item>
