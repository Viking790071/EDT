		<dataSource>DataSource1</dataSource>
		<query>SELECT
	LeadActivitiesSliceLast.Lead AS Lead,
	LeadActivitiesSliceLast.SalesRep AS SalesRep,
	LeadActivitiesSliceLast.Activity AS Activity,
	Leads.Potential AS Potential,
	&amp;StrFirstPeriod AS Type
INTO LeadsTable
FROM
	InformationRegister.LeadActivities.SliceLast(ENDOFPERIOD(&amp;FirstPeriod, DAY), ) AS LeadActivitiesSliceLast
		LEFT JOIN Catalog.Leads AS Leads
		ON LeadActivitiesSliceLast.Lead = Leads.Ref
WHERE
	LeadActivitiesSliceLast.Campaign = &amp;Campaign
	AND NOT Leads.DeletionMark
	AND (Leads.ClosureResult = VALUE(Enum.LeadClosureResult.EmptyRef)
			OR Leads.ClosureDate &gt; ENDOFPERIOD(&amp;FirstPeriod, DAY))

UNION ALL

SELECT
	LeadActivitiesSliceLast.Lead,
	LeadActivitiesSliceLast.SalesRep,
	LeadActivitiesSliceLast.Activity,
	Leads.Potential,
	&amp;StrSecondPeriod
FROM
	InformationRegister.LeadActivities.SliceLast(ENDOFPERIOD(&amp;SecondPeriod, DAY), ) AS LeadActivitiesSliceLast
		LEFT JOIN Catalog.Leads AS Leads
		ON LeadActivitiesSliceLast.Lead = Leads.Ref
WHERE
	LeadActivitiesSliceLast.Campaign = &amp;Campaign
	AND NOT Leads.DeletionMark
	AND (Leads.ClosureResult = VALUE(Enum.LeadClosureResult.EmptyRef)
			OR Leads.ClosureDate &gt; ENDOFPERIOD(&amp;SecondPeriod, DAY))
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	LeadsTable.SalesRep AS SalesRep
INTO SalesReps
FROM
	LeadsTable AS LeadsTable

GROUP BY
	LeadsTable.SalesRep
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	CampaignsActivities.LineNumber AS LineNumber,
	CampaignsActivities.Activity AS Activity,
	SalesReps.SalesRep AS SalesRep
INTO RepsActivities
FROM
	Catalog.Campaigns.Activities AS CampaignsActivities,
	SalesReps AS SalesReps
WHERE
	CampaignsActivities.Ref = &amp;Campaign
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	RepsActivities.LineNumber AS LineNumber,
	RepsActivities.Activity AS Activity,
	RepsActivities.SalesRep AS SalesRep,
	LeadsTable.Lead AS Lead,
	ISNULL(LeadsTable.Potential, 0) AS Potential,
	ISNULL(LeadsTable.Type, &amp;StrFirstPeriod) AS Type,
	ISNULL(LeadsTable.Potential, 0) AS PotentialPresentation,
	LeadsTable.Lead AS LeadRef
FROM
	RepsActivities AS RepsActivities
		LEFT JOIN LeadsTable AS LeadsTable
		ON RepsActivities.Activity = LeadsTable.Activity
			AND RepsActivities.SalesRep = LeadsTable.SalesRep</query>
