	</totalField>
	<totalField>
		<dataPath>PotentialPresentation</dataPath>
		<expression>StringFunctionsClientServer.SubstituteParametersToString(&amp;TextPotential, String(Sum(Potential)))</expression>
	</totalField>
	<totalField>
		<dataPath>Lead</dataPath>
		<expression>Count(Distinct Lead)</expression>
	</totalField>
