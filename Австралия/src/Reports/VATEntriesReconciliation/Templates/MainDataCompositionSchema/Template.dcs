		<dataSource>DataSource1</dataSource>
		<query>SELECT
	AccountingJournalEntriesDrCrTurnovers.Recorder AS ShipmentDocument,
	AccountingJournalEntriesDrCrTurnovers.Recorder AS Recorder,
	AccountingJournalEntriesDrCrTurnovers.AccountDr AS Details,
	AccountingJournalEntriesDrCrTurnovers.AmountTurnover AS AmountDr,
	0 AS AmountCr,
	0 AS AmountInput,
	0 AS AmountOutput
INTO TT_Turnovers
FROM
	AccountingRegister.AccountingJournalEntries.DrCrTurnovers(, , Recorder, AccountDr IN (&amp;VATInputGLAccounts), , , , Company = &amp;Company) AS AccountingJournalEntriesDrCrTurnovers

UNION ALL

SELECT
	AccountingJournalEntriesDrCrTurnovers.Recorder,
	AccountingJournalEntriesDrCrTurnovers.Recorder,
	AccountingJournalEntriesDrCrTurnovers.AccountCr,
	0,
	AccountingJournalEntriesDrCrTurnovers.AmountTurnover,
	0,
	0
FROM
	AccountingRegister.AccountingJournalEntries.DrCrTurnovers(, , Recorder, , , AccountCr IN (&amp;VATOutputGLAccounts), , Company = &amp;Company) AS AccountingJournalEntriesDrCrTurnovers

UNION ALL

SELECT
	AccountingJournalEntriesDrCrTurnovers.Recorder,
	AccountingJournalEntriesDrCrTurnovers.Recorder,
	AccountingJournalEntriesDrCrTurnovers.AccountCr,
	0,
	- AccountingJournalEntriesDrCrTurnovers.AmountTurnover,
	0,
	0
FROM
	AccountingRegister.AccountingJournalEntries.DrCrTurnovers(, , Recorder, , , AccountCr IN (&amp;VATInputGLAccounts), , Company = &amp;Company) AS AccountingJournalEntriesDrCrTurnovers

UNION ALL

SELECT
	AccountingJournalEntriesDrCrTurnovers.Recorder,
	AccountingJournalEntriesDrCrTurnovers.Recorder,
	AccountingJournalEntriesDrCrTurnovers.AccountDr,
	AccountingJournalEntriesDrCrTurnovers.AmountTurnover,
	0,
	0,
	0
FROM
	AccountingRegister.AccountingJournalEntries.DrCrTurnovers(, , Recorder, AccountDr IN (&amp;VATOutputGLAccounts), , , , Company = &amp;Company) AS AccountingJournalEntriesDrCrTurnovers

UNION ALL

SELECT
	VATInputTurnovers.ShipmentDocument,
	VATInputTurnovers.Recorder,
	&amp;VATInputText,
	0,
	0,
	VATInputTurnovers.VATAmountTurnover,
	0
FROM
	AccumulationRegister.VATInput.Turnovers(, , Recorder, Company = &amp;Company) AS VATInputTurnovers

UNION ALL

SELECT
	VATIncurredTurnovers.ShipmentDocument,
	VATIncurredTurnovers.Recorder,
	&amp;VATIncurredText,
	0,
	0,
	VATIncurredTurnovers.VATAmountTurnover,
	0
FROM
	AccumulationRegister.VATIncurred.Turnovers(, , Recorder, Company = &amp;Company) AS VATIncurredTurnovers

UNION ALL

SELECT
	VATOutputTurnovers.ShipmentDocument,
	VATOutputTurnovers.Recorder,
	&amp;VATOutputText,
	0,
	0,
	0,
	VATOutputTurnovers.VATAmountTurnover
FROM
	AccumulationRegister.VATOutput.Turnovers(, , Recorder, Company = &amp;Company) AS VATOutputTurnovers
;

