SELECT
	AccountingJournalEntries.Company AS Company,
	AccountingJournalEntries.AccountDr AS AccountDr,
	AccountingJournalEntries.AccountCr AS AccountCr,
	AccountingJournalEntries.Amount AS Amount
FROM
	AccountingRegister.AccountingJournalEntries AS AccountingJournalEntries
		INNER JOIN AccountsVAT AS AccountsVATDr
		ON AccountingJournalEntries.AccountDr = AccountsVATDr.GLAccounts
		INNER JOIN AccountsVAT AS AccountsVATCr
		ON AccountingJournalEntries.AccountCr = AccountsVATCr.GLAccounts
WHERE
	VALUETYPE(AccountingJournalEntries.Recorder) = TYPE(Document.MonthEndClosing)
	AND AccountingJournalEntries.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod</query>
