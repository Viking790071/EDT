	FinancialResultTurnovers.SecondPeriod,
	FinancialResultTurnovers.MinutePeriod,
	FinancialResultTurnovers.HourPeriod,
	FinancialResultTurnovers.DayPeriod,
	FinancialResultTurnovers.WeekPeriod,
	FinancialResultTurnovers.TenDaysPeriod,
	FinancialResultTurnovers.MonthPeriod,
	FinancialResultTurnovers.QuarterPeriod,
	FinancialResultTurnovers.HalfYearPeriod,
	FinancialResultTurnovers.YearPeriod,
	FinancialResultTurnovers.Company,
	FinancialResultTurnovers.StructuralUnit,
	FinancialResultTurnovers.BusinessLine,
	FinancialResultTurnovers.SalesOrder,
	FinancialResultTurnovers.IncomeAndExpenseItem AS Item,
	FinancialResultTurnovers.AmountIncomeTurnover,
	FinancialResultTurnovers.AmountExpenseTurnover,
	0 AS ProfitLoss
	FROM
		AccumulationRegister.FinancialResult.Turnovers(, , Auto, ) AS FinancialResultTurnovers</query>
