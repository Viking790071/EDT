					<dataSource>DataSource1</dataSource>
					<query>SELECT ALLOWED
	CASE
		WHEN POSSummary.Recorder = UNDEFINED
			THEN NULL
		ELSE POSSummary.Recorder
	END AS Recorder,
	POSSummary.Company,
	POSSummary.StructuralUnit,
	POSSummary.Currency AS Currency,
	POSSummary.AmountCur AS AmountCurReceipt,
	POSSummary.Amount AS AmountReceipt,
	POSSummary.Cost AS CostReceipt,
	NULL AS AmountCurExpense,
	NULL AS AmountExpense,
	NULL AS CostExpense,
	0 AS AmountCurOpeningBalance,
	0 AS AmountOpeningBalance,
	0 AS CostOpeningBalance,
	0 AS AmountCurClosingBalance,
	0 AS AmountClosingBalance,
	0 AS CostClosingBalance,
	BEGINOFPERIOD(POSSummary.Period, Day) AS DayPeriod,
	BEGINOFPERIOD(POSSummary.Period, WEEK) AS WeekPeriod,
	BEGINOFPERIOD(POSSummary.Period, TENDAYS) AS TenDaysPeriod,
	BEGINOFPERIOD(POSSummary.Period, MONTH) AS MonthPeriod,
	BEGINOFPERIOD(POSSummary.Period, QUARTER) AS QuarterPeriod,
	BEGINOFPERIOD(POSSummary.Period, YEAR) AS YearPeriod
	FROM
	AccumulationRegister.POSSummary AS POSSummary
	WHERE
	POSSummary.RecordType = VALUE(AccumulationRecordType.Receipt)
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN POSSummary.Period &gt;= &amp;BeginOfPeriod
			ELSE POSSummary.Period between &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END

	UNION ALL

	SELECT
	POSSummary.Recorder,
	POSSummary.Company,
	POSSummary.StructuralUnit,
	POSSummary.Currency,
	NULL,
	NULL,
	NULL,
	POSSummary.AmountCur,
	POSSummary.Amount,
	POSSummary.Cost,
	0,
	0,
	0,
	0,
	0,
	0,
	BEGINOFPERIOD(POSSummary.Period, Day),
	BEGINOFPERIOD(POSSummary.Period, WEEK),
	BEGINOFPERIOD(POSSummary.Period, TENDAYS),
	BEGINOFPERIOD(POSSummary.Period, MONTH),
	BEGINOFPERIOD(POSSummary.Period, QUARTER),
	BEGINOFPERIOD(POSSummary.Period, YEAR)
	FROM
	AccumulationRegister.POSSummary AS POSSummary
	WHERE
	POSSummary.RecordType = VALUE(AccumulationRecordType.Expense)
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN POSSummary.Period &gt;= &amp;BeginOfPeriod
			ELSE POSSummary.Period between &amp;BeginOfPeriod AND &amp;EndOfPeriod
			END</query>
