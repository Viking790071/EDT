	FixedAssetsBalanceAndTurnovers.FixedAsset.Code AS Code,
	SUM(FixedAssetsBalanceAndTurnovers.DepreciationReceipt) AS DepreciationReceipt,
	SUM(FixedAssetsBalanceAndTurnovers.CostClosingBalance) AS CostClosingBalance,
	FixedAssetsBalanceAndTurnovers.FixedAsset.DepreciationMethod AS DepreciationMethod,
	FixedAssetsBalanceAndTurnovers.Company
FROM
	AccumulationRegister.FixedAssets.BalanceAndTurnovers(&amp;BeginOfPeriod, &amp;EndOfPeriod, Recorder, , ) AS FixedAssetsBalanceAndTurnovers
WHERE
	VALUETYPE(FixedAssetsBalanceAndTurnovers.Recorder) = Type(Document.FixedAssetsDepreciation)

GROUP BY
	FixedAssetsBalanceAndTurnovers.FixedAsset,
