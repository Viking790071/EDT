SELECT ALLOWED
	Inventory.Products AS Products,
	Inventory.Characteristic AS ProductCharacteristic,
	Inventory.Batch AS BatchProducts,
	Inventory.GLAccount AS ProductsGLAccount,
	Inventory.SalesOrder AS SalesOrderProducts,
	Inventory.Specification AS ProductionSpecification,
	Inventory.StructuralUnit AS ProductionSubdepartment,
	Inventory.Company AS CompanyProducts,
	"Intangible costs" AS ProductsCorr,
	"Processing services" AS GLAccount,
	Inventory.Amount AS Amount,
	Inventory.Quantity AS Quantity,
	0 AS ProductsQuantity,
	Inventory.Products.MeasurementUnit AS UnOfMeasProducts
FROM
	AccumulationRegister.Inventory AS Inventory
WHERE
	Inventory.Recorder IN
			(SELECT
				SupplierInvoices.Ref AS Ref
			FROM
				SupplierInvoices AS SupplierInvoices)
	AND Inventory.FixedCost
	AND Inventory.StructuralUnit.StructuralUnitType = VALUE(Enum.BusinessUnitsTypes.Warehouse)
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN Inventory.Period &gt;= &amp;BeginOfPeriod
			ELSE Inventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END</query>
