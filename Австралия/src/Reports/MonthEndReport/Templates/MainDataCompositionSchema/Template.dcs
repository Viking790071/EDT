	InformationRegister.ProductGLAccounts AS ProductGLAccounts
WHERE
	ProductGLAccounts.VATInput &lt;&gt; VALUE(Catalog.DefaultGLAccounts.EmptyRef)

UNION ALL

SELECT DISTINCT
	ProductGLAccounts.VATOutput
FROM
	InformationRegister.ProductGLAccounts AS ProductGLAccounts
WHERE
	ProductGLAccounts.VATOutput &lt;&gt; VALUE(Catalog.DefaultGLAccounts.EmptyRef)

UNION ALL

SELECT
	&amp;VATPayable

UNION ALL

SELECT
	&amp;VATTaxRefund

UNION ALL

SELECT DISTINCT
	VATInput.GLAccount
FROM
	AccumulationRegister.VATInput AS VATInput
WHERE
	VATInput.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod

UNION ALL

SELECT DISTINCT
	VATOutput.GLAccount
FROM
	AccumulationRegister.VATOutput AS VATOutput
WHERE
	VATOutput.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
;

