	MonthEndErrors.Active AS Active,
	MonthEndErrors.OperationKind AS OperationKind,
	MonthEndErrors.ErrorDescription AS ErrorDescription,
	MonthEndErrors.Company AS Company,
	MonthEndErrors.Analytics AS Analytics,
	CASE
		WHEN MonthEndErrors.OperationKind = "AccrueDepreciation"
			THEN "Accrue depreciation"
		WHEN MonthEndErrors.OperationKind = "DirectCostCalculation"
			THEN "Direct cost calculation"
		WHEN MonthEndErrors.OperationKind = "ActualCostCalculation"
			THEN "Actual cost calculation"
		WHEN MonthEndErrors.OperationKind = "CostAllocation"
