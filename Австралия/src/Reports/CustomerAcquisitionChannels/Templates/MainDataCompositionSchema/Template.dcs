	<totalField>
		<dataPath>NewLeadsPercent</dataPath>
		<expression>Case When EvalExpression("Sum(NewLeads)", "Overall") = 0 Then 0 Else Sum(NewLeads) / EvalExpression("Sum(NewLeads)", "Overall") * 100 End</expression>
	</totalField>
	<totalField>
		<dataPath>NewCustomersPercent</dataPath>
		<expression>Case When EvalExpression("Sum(NewCustomers)", "Overall") = 0 Then 0 Else Sum(NewCustomers) / EvalExpression("Sum(NewCustomers)", "Overall") * 100 End</expression>
	</totalField>
	<totalField>
