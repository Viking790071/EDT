		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	Leads.AcquisitionChannel AS AcquisitionChannel,
	Leads.Ref AS Lead,
	VALUE(Catalog.Counterparties.EmptyRef) AS Counterparty,
	1 AS NewLeads,
	0 AS NewCustomers
FROM
	Catalog.Leads AS Leads
WHERE
	NOT Leads.DeletionMark
	AND Leads.ClosureResult &lt;&gt; VALUE(Enum.LeadClosureResult.ConvertedIntoCustomer)
	AND Leads.Created BETWEEN &amp;Date1 AND &amp;Date2

UNION ALL

SELECT
	Counterparties.CustomerAcquisitionChannel,
	VALUE(Catalog.Leads.EmptyRef),
	Counterparties.Ref,
	0,
	1
FROM
	Catalog.Counterparties AS Counterparties
WHERE
	NOT Counterparties.DeletionMark
	AND Counterparties.CreationDate BETWEEN &amp;Date1 AND &amp;Date2
	AND (Counterparties.Customer
			OR Counterparties.CustomerAcquisitionChannel &lt;&gt; VALUE(Catalog.CustomerAcquisitionChannels.EmptyRef))</query>
