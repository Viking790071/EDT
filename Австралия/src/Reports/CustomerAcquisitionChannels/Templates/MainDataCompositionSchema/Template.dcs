	</totalField>
	<totalField>
		<dataPath>TotalPercent</dataPath>
		<expression>Case When EvalExpression("Sum(NewLeads + NewCustomers)", "Overall") = 0 Then 0 Else Sum(NewLeads + NewCustomers) / EvalExpression("Sum(NewLeads + NewCustomers)", "Overall") * 100 End</expression>
