			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	POSSummaryBalancesAndTurnovers.Company,
	POSSummaryBalancesAndTurnovers.StructuralUnit,
	POSSummaryBalancesAndTurnovers.Currency AS Currency,
	POSSummaryBalancesAndTurnovers.AmountOpeningBalance,
	POSSummaryBalancesAndTurnovers.AmountClosingBalance,
	0 AS AmountReceipt,
	0 AS AmountExpense,
	POSSummaryBalancesAndTurnovers.AmountCurOpeningBalance,
	POSSummaryBalancesAndTurnovers.AmountCurClosingBalance,
	0 AS AmountCurReceipt,
	0 AS AmountCurExpense,
	POSSummaryBalancesAndTurnovers.CostOpeningBalance,
	POSSummaryBalancesAndTurnovers.CostClosingBalance,
	0 AS CostReceipt,
	0 AS CostExpense,
	POSSummaryBalancesAndTurnovers.DayPeriod,
	POSSummaryBalancesAndTurnovers.WeekPeriod,
	POSSummaryBalancesAndTurnovers.TenDaysPeriod,
	POSSummaryBalancesAndTurnovers.MonthPeriod,
	POSSummaryBalancesAndTurnovers.QuarterPeriod,
	POSSummaryBalancesAndTurnovers.HalfYearPeriod,
	POSSummaryBalancesAndTurnovers.YearPeriod,
	CASE
		WHEN POSSummaryBalancesAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE POSSummaryBalancesAndTurnovers.Recorder
	END AS Recorder
	FROM
		AccumulationRegister.POSSummary.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS POSSummaryBalancesAndTurnovers</query>
		</item>
