		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		WHEN AccountsPayableBalancesAndTurnovers.AmountTurnover = 0
			THEN NULL
		ELSE AccountsPayableBalancesAndTurnovers.Recorder
	END AS Recorder,
	AccountsPayableBalancesAndTurnovers.SecondPeriod AS SecondPeriod,
	AccountsPayableBalancesAndTurnovers.MinutePeriod AS MinutePeriod,
	AccountsPayableBalancesAndTurnovers.HourPeriod AS HourPeriod,
	AccountsPayableBalancesAndTurnovers.DayPeriod AS DayPeriod,
	AccountsPayableBalancesAndTurnovers.WeekPeriod AS WeekPeriod,
	AccountsPayableBalancesAndTurnovers.TenDaysPeriod AS TenDaysPeriod,
	AccountsPayableBalancesAndTurnovers.MonthPeriod AS MonthPeriod,
	AccountsPayableBalancesAndTurnovers.QuarterPeriod AS QuarterPeriod,
	AccountsPayableBalancesAndTurnovers.HalfYearPeriod AS HalfYearPeriod,
	AccountsPayableBalancesAndTurnovers.YearPeriod AS YearPeriod,
	AccountsPayableBalancesAndTurnovers.Company AS Company,
	AccountsPayableBalancesAndTurnovers.SettlementsType AS SettlementsType,
	AccountsPayableBalancesAndTurnovers.Counterparty AS Counterparty,
	AccountsPayableBalancesAndTurnovers.Contract AS Contract,
	AccountsPayableBalancesAndTurnovers.Document AS Document,
	AccountsPayableBalancesAndTurnovers.Order AS Order,
	AccountsPayableBalancesAndTurnovers.Contract.SettlementsCurrency AS Currency,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountOpeningBalance
		ELSE 0
	END AS AmountOpeningBalanceDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsPayableBalancesAndTurnovers.AmountOpeningBalance
		ELSE 0
	END AS AmountOpeningBalanceAdvance,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountOpeningBalance
		ELSE 0
	END - CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsPayableBalancesAndTurnovers.AmountOpeningBalance
		ELSE 0
	END AS AmountOpeningBalanceSettlements,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountClosingBalance
		ELSE 0
	END AS AmountClosingBalanceDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsPayableBalancesAndTurnovers.AmountClosingBalance
		ELSE 0
	END AS AmountClosingBalanceAdvance,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountClosingBalance
		ELSE 0
	END - CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsPayableBalancesAndTurnovers.AmountClosingBalance
		ELSE 0
	END AS AmountClosingBalanceSettlements,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountReceipt
		ELSE 0
	END AS AmountReceiptDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsPayableBalancesAndTurnovers.AmountReceipt
		ELSE 0
	END AS AmountReceiptAdvance,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountTurnover
		ELSE 0
	END AS AmountTurnoverDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsPayableBalancesAndTurnovers.AmountTurnover
		ELSE 0
	END AS AmountAdvanceTurnover,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountExpense
		ELSE 0
	END AS AmountExpenseDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsPayableBalancesAndTurnovers.AmountExpense
		ELSE 0
	END AS AmountExpenseAdvance,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurOpeningBalance
		ELSE 0
	END AS AmountCurOpeningBalanceDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsPayableBalancesAndTurnovers.AmountCurOpeningBalance
		ELSE 0
	END AS AmountCurOpeningBalanceAdvance,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurOpeningBalance
		ELSE 0
	END - CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsPayableBalancesAndTurnovers.AmountCurOpeningBalance
		ELSE 0
	END AS AmountCurOpeningBalanceSettlements,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurClosingBalance
		ELSE 0
	END AS AmountCurClosingBalanceDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsPayableBalancesAndTurnovers.AmountCurClosingBalance
		ELSE 0
	END AS AmountCurClosingBalanceAdvance,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurClosingBalance
		ELSE 0
	END - CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsPayableBalancesAndTurnovers.AmountCurClosingBalance
		ELSE 0
	END AS AmountCurClosingBalanceCalculations,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurTurnover
		ELSE 0
	END AS AmountCurTurnoverDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurTurnover
		ELSE 0
	END AS CurAmountTurnoverAdvance,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurReceipt
		ELSE 0
	END AS AmountCurReceiptDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurReceipt
		ELSE 0
	END AS AmountCurReceiptPrepayment,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurExpense
		ELSE 0
	END AS AmountCurExpenseDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsPayableBalancesAndTurnovers.AmountCurExpense
		ELSE 0
	END AS AmountCurExpenseAdvance,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
				AND ISNULL(UsingPaymentTermsInDocuments.UsingPaymentTerms, FALSE)
			THEN AccountsPayableBalancesAndTurnovers.AmountForPaymentClosingBalance
		ELSE 0
	END AS AmountForPaymentClosingBalanceDebt,
	CASE
		WHEN AccountsPayableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
				AND ISNULL(UsingPaymentTermsInDocuments.UsingPaymentTerms, FALSE)
			THEN AccountsPayableBalancesAndTurnovers.AmountForPaymentCurClosingBalance
		ELSE 0
	END AS AmountForPaymentCurClosingBalanceDebt
FROM
	AccumulationRegister.AccountsPayable.BalanceAndTurnovers(, , Auto, , ) AS AccountsPayableBalancesAndTurnovers
		LEFT JOIN InformationRegister.UsingPaymentTermsInDocuments AS UsingPaymentTermsInDocuments
		ON AccountsPayableBalancesAndTurnovers.Document = UsingPaymentTermsInDocuments.Document</query>
	</dataSet>
