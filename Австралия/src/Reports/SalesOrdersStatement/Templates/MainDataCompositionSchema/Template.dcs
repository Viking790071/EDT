		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN SalesOrdersBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE SalesOrdersBalanceAndTurnovers.Recorder
	END AS Recorder,
	SalesOrdersBalanceAndTurnovers.SecondPeriod AS SecondPeriod,
	SalesOrdersBalanceAndTurnovers.MinutePeriod AS MinutePeriod,
	SalesOrdersBalanceAndTurnovers.HourPeriod AS HourPeriod,
	SalesOrdersBalanceAndTurnovers.DayPeriod AS DayPeriod,
	SalesOrdersBalanceAndTurnovers.WeekPeriod AS WeekPeriod,
	SalesOrdersBalanceAndTurnovers.TenDaysPeriod AS TenDaysPeriod,
	SalesOrdersBalanceAndTurnovers.MonthPeriod AS MonthPeriod,
	SalesOrdersBalanceAndTurnovers.QuarterPeriod AS QuarterPeriod,
	SalesOrdersBalanceAndTurnovers.HalfYearPeriod AS HalfYearPeriod,
	SalesOrdersBalanceAndTurnovers.YearPeriod AS YearPeriod,
	SalesOrdersBalanceAndTurnovers.Company AS Company,
	SalesOrdersBalanceAndTurnovers.SalesOrder AS SalesOrder,
	SalesOrdersBalanceAndTurnovers.Products AS Products,
	SalesOrdersBalanceAndTurnovers.Characteristic AS Characteristic,
	SalesOrdersBalanceAndTurnovers.QuantityOpeningBalance AS QuantityOpeningBalance,
	SalesOrdersBalanceAndTurnovers.QuantityClosingBalance AS QuantityClosingBalance,
	SalesOrdersBalanceAndTurnovers.QuantityTurnover AS QuantityTurnover,
	SalesOrdersBalanceAndTurnovers.QuantityReceipt AS QuantityReceipt,
	SalesOrdersBalanceAndTurnovers.QuantityExpense AS QuantityExpense,
	CASE
		WHEN SalesOrdersBalanceAndTurnovers.Products.ConversionRate = 0
			THEN SalesOrdersBalanceAndTurnovers.QuantityOpeningBalance
		ELSE SalesOrdersBalanceAndTurnovers.QuantityOpeningBalance / SalesOrdersBalanceAndTurnovers.Products.ConversionRate
	END AS ReportOpeningBalance,
	CASE
		WHEN SalesOrdersBalanceAndTurnovers.Products.ConversionRate = 0
			THEN SalesOrdersBalanceAndTurnovers.QuantityClosingBalance
		ELSE SalesOrdersBalanceAndTurnovers.QuantityClosingBalance / SalesOrdersBalanceAndTurnovers.Products.ConversionRate
	END AS ReportClosingBalance,
	CASE
		WHEN SalesOrdersBalanceAndTurnovers.Products.ConversionRate = 0
			THEN SalesOrdersBalanceAndTurnovers.QuantityTurnover
		ELSE SalesOrdersBalanceAndTurnovers.QuantityTurnover / SalesOrdersBalanceAndTurnovers.Products.ConversionRate
	END AS ReportTurnover,
	CASE
		WHEN SalesOrdersBalanceAndTurnovers.Products.ConversionRate = 0
			THEN SalesOrdersBalanceAndTurnovers.QuantityReceipt
		ELSE SalesOrdersBalanceAndTurnovers.QuantityReceipt / SalesOrdersBalanceAndTurnovers.Products.ConversionRate
	END AS ReportReceipt,
	CASE
		WHEN SalesOrdersBalanceAndTurnovers.Products.ConversionRate = 0
			THEN SalesOrdersBalanceAndTurnovers.QuantityExpense
		ELSE SalesOrdersBalanceAndTurnovers.QuantityExpense / SalesOrdersBalanceAndTurnovers.Products.ConversionRate
	END AS ReportExpense,
	SalesOrdersBalanceAndTurnovers.SalesOrder.OrderState AS OrderState,
	SalesOrdersBalanceAndTurnovers.SalesOrder.OrderState.OrderStatus AS OrderStatus,
	SalesOrdersBalanceAndTurnovers.SalesOrder.Closed AS Closed,
	DocSalesOrder.Contract AS Contract,
	DocSalesOrder.Contract AS ContractForExternalUser,
	DocSalesOrder.Counterparty AS Counterparty
FROM
	AccumulationRegister.SalesOrders.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS SalesOrdersBalanceAndTurnovers
		LEFT JOIN Document.SalesOrder AS DocSalesOrder
		ON SalesOrdersBalanceAndTurnovers.SalesOrder = DocSalesOrder.Ref</query>
	</dataSet>
	<totalField>
		<dataPath>QuantityClosingBalance</dataPath>
		<expression>SUM(QuantityClosingBalance)</expression>
	</totalField>
	<totalField>
		<dataPath>QuantityOpeningBalance</dataPath>
