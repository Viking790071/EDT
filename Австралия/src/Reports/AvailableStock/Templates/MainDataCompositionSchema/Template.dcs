		<field xsi:type="DataSetFieldField">
			<dataPath>InventoryAccountType</dataPath>
			<field>InventoryAccountType</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	ReservedProductsBalances.Company AS Company,
	ReservedProductsBalances.StructuralUnit AS StructuralUnit,
	ReservedProductsBalances.Products AS Products,
	ReservedProductsBalances.Characteristic AS Characteristic,
	ReservedProductsBalances.Batch AS Batch,
	ReservedProductsBalances.SalesOrder AS SalesOrder,
	ReservedProductsBalances.QuantityBalance AS QuantityBalance
INTO TemporaryTableReserves
FROM
	AccumulationRegister.ReservedProducts.Balance(, SalesOrder &lt;&gt; UNDEFINED) AS ReservedProductsBalances
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	InventoryInWarehousesBalance.Company AS Company,
	InventoryInWarehousesBalance.StructuralUnit AS StructuralUnit,
	InventoryInWarehousesBalance.Products AS Products,
	InventoryInWarehousesBalance.Characteristic AS Characteristic,
	InventoryInWarehousesBalance.Batch AS Batch,
	InventoryInWarehousesBalance.Cell AS Cell,
	InventoryInWarehousesBalance.QuantityBalance AS CellQuantityBalance
INTO TemporaryTableInventoryInWarehousesBalance
FROM
	AccumulationRegister.InventoryInWarehouses.Balance AS InventoryInWarehousesBalance
WHERE
	&amp;StorageBins
{WHERE
	InventoryInWarehousesBalance.Company.*,
	InventoryInWarehousesBalance.StructuralUnit.*,
	InventoryInWarehousesBalance.Products.*}
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	InventoryBalances.Company AS Company,
	InventoryBalances.StructuralUnit AS StructuralUnit,
	InventoryBalances.Products AS Products,
	InventoryBalances.Characteristic AS Characteristic,
	InventoryBalances.Batch AS Batch,
	InventoryBalances.InventoryAccountType AS InventoryAccountType,
	InventoryBalances.QuantityBalance AS Balance,
	0 AS Reserve
INTO TemporaryTableBalanceAndReserves
FROM
	AccumulationRegister.Inventory.Balance(
			,
			VALUETYPE(StructuralUnit) = TYPE(Catalog.BusinessUnits)
				AND StructuralUnit &lt;&gt; VALUE(Catalog.BusinessUnits.DropShipping)
				AND InventoryAccountType IN (VALUE(Enum.InventoryAccountTypes.InventoryOnHand), VALUE(Enum.InventoryAccountTypes.ThirdPartyInventory),
											VALUE(Enum.InventoryAccountTypes.GoodsInTransit), VALUE(Enum.InventoryAccountTypes.CustomerOwnedComponents),
											VALUE(Enum.InventoryAccountTypes.CustomerOwnedFinishedProducts))) AS InventoryBalances

UNION ALL

SELECT
	Reserves.Company,
	Reserves.StructuralUnit,
	Reserves.Products,
	Reserves.Characteristic,
	Reserves.Batch,
	NULL,
	0,
	Reserves.QuantityBalance
FROM
	TemporaryTableReserves AS Reserves
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	BalanceAndReserves.Company AS Company,
	BalanceAndReserves.StructuralUnit AS StructuralUnit,
	BalanceAndReserves.Products AS Products,
	BalanceAndReserves.Characteristic AS Characteristic,
	BalanceAndReserves.Batch AS Batch,
	SUM(BalanceAndReserves.Balance) AS Balance,
	SUM(BalanceAndReserves.Reserve) AS Reserve,
	BalanceAndReserves.InventoryAccountType AS InventoryAccountType
INTO TemporaryTableBalanceReserves
FROM
	TemporaryTableBalanceAndReserves AS BalanceAndReserves

GROUP BY
	BalanceAndReserves.Company,
	BalanceAndReserves.StructuralUnit,
	BalanceAndReserves.Products,
	BalanceAndReserves.Characteristic,
	BalanceAndReserves.Batch,
	BalanceAndReserves.InventoryAccountType
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	BalanceReserves.Company AS Company,
	BalanceReserves.StructuralUnit AS StructuralUnit,
	BalanceReserves.Products AS Products,
	BalanceReserves.Characteristic AS Characteristic,
	BalanceReserves.Batch AS Batch,
	NULL AS SalesOrder,
	"TotalInAllWarehouses" AS TotalByWarehouses,
	BalanceReserves.Balance AS Balance,
	0 AS Reserve,
	BalanceReserves.Balance - BalanceReserves.Reserve AS Free,
	BalanceReserves.InventoryAccountType AS InventoryAccountType
INTO TemporaryTableAvailableStock
FROM
	TemporaryTableBalanceReserves AS BalanceReserves

UNION ALL

SELECT
	Reserves.Company,
	Reserves.StructuralUnit,
	Reserves.Products,
	Reserves.Characteristic,
	Reserves.Batch,
	Reserves.SalesOrder,
	"TotalInAllWarehouses",
	0,
	Reserves.QuantityBalance,
	0,
	VALUE(Enum.InventoryMovementTypes.EmptyRef)
FROM
	TemporaryTableReserves AS Reserves
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TemporaryTableAvailableStock.Company AS Company,
	TemporaryTableAvailableStock.StructuralUnit AS StructuralUnit,
	TemporaryTableAvailableStock.Products AS Products,
	TemporaryTableAvailableStock.Characteristic AS Characteristic,
	TemporaryTableAvailableStock.Batch AS Batch,
	CatalogProductsBatches.BatchNumber AS BatchNumber,
	CatalogProductsBatches.ExpirationDate AS ExpirationDate,
	DATEDIFF(&amp;PeriodForExpiration, CatalogProductsBatches.ExpirationDate, DAY) AS DaysTillExpiration,
	CASE
		WHEN CatalogProducts.ShelfLife &gt; 0
				AND DATEDIFF(&amp;PeriodForExpiration, CatalogProductsBatches.ExpirationDate, DAY) &gt;= 0
				AND DATEDIFF(&amp;PeriodForExpiration, CatalogProductsBatches.ExpirationDate, DAY) / CatalogProducts.ShelfLife &lt; 1
			THEN 100 - DATEDIFF(&amp;PeriodForExpiration, CatalogProductsBatches.ExpirationDate, DAY) / CatalogProducts.ShelfLife * 100
		ELSE 0
	END AS ExpirationPercent,
	TemporaryTableAvailableStock.SalesOrder AS SalesOrder,
	TemporaryTableAvailableStock.TotalByWarehouses AS TotalByWarehouses,
	TemporaryTableAvailableStock.Balance AS Balance,
	TemporaryTableAvailableStock.Reserve AS Reserve,
	TemporaryTableAvailableStock.Free AS Free,
	CASE
		WHEN CatalogProducts.ConversionRate &lt;&gt; 0
			THEN TemporaryTableAvailableStock.Balance / CatalogProducts.ConversionRate
		ELSE TemporaryTableAvailableStock.Balance
	END AS ReportBalance,
	CASE
		WHEN CatalogProducts.ConversionRate &lt;&gt; 0
			THEN TemporaryTableAvailableStock.Reserve / CatalogProducts.ConversionRate
		ELSE TemporaryTableAvailableStock.Reserve
	END AS ReportReserve,
	CASE
		WHEN CatalogProducts.ConversionRate &lt;&gt; 0
			THEN TemporaryTableAvailableStock.Free / CatalogProducts.ConversionRate
		ELSE TemporaryTableAvailableStock.Free
	END AS ReportFree,
	0 AS Cell,
	0 AS CellBalance,
	0 AS ReportCellBalance,
	TemporaryTableAvailableStock.InventoryAccountType AS InventoryAccountType
FROM
	TemporaryTableAvailableStock AS TemporaryTableAvailableStock
		INNER JOIN Catalog.Products AS CatalogProducts
		ON TemporaryTableAvailableStock.Products = CatalogProducts.Ref
		LEFT JOIN Catalog.ProductsBatches AS CatalogProductsBatches
		ON TemporaryTableAvailableStock.Batch = CatalogProductsBatches.Ref

UNION ALL

SELECT
	TemporaryTableInventoryInWarehousesBalance.Company,
	TemporaryTableInventoryInWarehousesBalance.StructuralUnit,
	TemporaryTableInventoryInWarehousesBalance.Products,
	TemporaryTableInventoryInWarehousesBalance.Characteristic,
	TemporaryTableInventoryInWarehousesBalance.Batch,
	CatalogProductsBatches.BatchNumber,
	CatalogProductsBatches.ExpirationDate,
	DATEDIFF(&amp;PeriodForExpiration, CatalogProductsBatches.ExpirationDate, DAY),
	CASE
		WHEN CatalogProducts.ShelfLife &gt; 0
				AND DATEDIFF(&amp;PeriodForExpiration, CatalogProductsBatches.ExpirationDate, DAY) &gt;= 0
				AND DATEDIFF(&amp;PeriodForExpiration, CatalogProductsBatches.ExpirationDate, DAY) / CatalogProducts.ShelfLife &lt; 1
			THEN 100 - DATEDIFF(&amp;PeriodForExpiration, CatalogProductsBatches.ExpirationDate, DAY) / CatalogProducts.ShelfLife * 100
		ELSE 0
	END,
	NULL,
	"TotalInAllWarehouses",
	0,
	0,
	0,
	NULL,
	NULL,
	NULL,
	TemporaryTableInventoryInWarehousesBalance.Cell,
	TemporaryTableInventoryInWarehousesBalance.CellQuantityBalance,
	CASE
		WHEN CatalogProducts.ConversionRate &lt;&gt; 0
			THEN TemporaryTableInventoryInWarehousesBalance.CellQuantityBalance / CatalogProducts.ConversionRate
		ELSE TemporaryTableInventoryInWarehousesBalance.CellQuantityBalance
	END,
	VALUE(Enum.InventoryMovementTypes.EmptyRef)
FROM
	TemporaryTableInventoryInWarehousesBalance AS TemporaryTableInventoryInWarehousesBalance
		INNER JOIN Catalog.Products AS CatalogProducts
		ON TemporaryTableInventoryInWarehousesBalance.Products = CatalogProducts.Ref
		LEFT JOIN Catalog.ProductsBatches AS CatalogProductsBatches
		ON TemporaryTableInventoryInWarehousesBalance.Batch = CatalogProductsBatches.Ref</query>
	</dataSet>
