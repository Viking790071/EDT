INTO TT_SalesOrdersWithBalancesCumulative
FROM
	TT_SalesOrdersWithBalances AS TT_SalesOrders
		INNER JOIN TT_SalesOrdersWithBalances AS TT_SalesOrdersCumulative
		ON TT_SalesOrders.Company = TT_SalesOrdersCumulative.Company
			AND TT_SalesOrders.SalesOrder = TT_SalesOrdersCumulative.SalesOrder
			AND TT_SalesOrders.Products = TT_SalesOrdersCumulative.Products
			AND TT_SalesOrders.Characteristic = TT_SalesOrdersCumulative.Characteristic
			AND TT_SalesOrders.ShipmentDate &gt;= TT_SalesOrdersCumulative.ShipmentDate

GROUP BY
	TT_SalesOrders.SalesOrder,
	TT_SalesOrders.ShipmentDate,
	TT_SalesOrders.IsMaxShipmentDate,
	TT_SalesOrders.Counterparty,
	TT_SalesOrders.Characteristic,
	TT_SalesOrders.Company,
	TT_SalesOrders.Products,
	TT_SalesOrders.ConversionRate,
	TT_SalesOrders.Quantity,
	TT_SalesOrders.QuantityBalance
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_SalesOrdersCumulative.Company AS Company,
	TT_SalesOrdersCumulative.SalesOrder AS SalesOrder,
	TT_SalesOrdersCumulative.Products AS Products,
	TT_SalesOrdersCumulative.ConversionRate AS ConversionRate,
	TT_SalesOrdersCumulative.Characteristic AS Characteristic,
	TT_SalesOrdersCumulative.Quantity AS Quantity,
	TT_SalesOrdersCumulative.QuantityBalance AS QuantityBalance,
	TT_SalesOrdersCumulative.ShipmentDate AS ShipmentDate,
	TT_SalesOrdersCumulative.Counterparty AS Counterparty,
	CASE
		WHEN TT_ReservesBalances.Quantity IS NULL
			THEN 0
		WHEN TT_ReservesBalances.Quantity &gt; TT_SalesOrdersCumulative.QuantityBalanceCumulative
				AND NOT TT_SalesOrdersCumulative.IsMaxShipmentDate
			THEN TT_SalesOrdersCumulative.QuantityBalance
		ELSE TT_ReservesBalances.Quantity - (TT_SalesOrdersCumulative.QuantityBalanceCumulative - TT_SalesOrdersCumulative.QuantityBalance)
	END AS QuantityReserve,
	ISNULL(TT_GoodsInvoicedNotShippedBalances.Quantity, 0) AS NotShippedQuantityBalance
INTO TT_SalesOrdersWithBalancesAndReserves
FROM
	TT_SalesOrdersWithBalancesCumulative AS TT_SalesOrdersCumulative
		LEFT JOIN TT_ReservesBalances AS TT_ReservesBalances
		ON TT_SalesOrdersCumulative.Company = TT_ReservesBalances.Company
			AND TT_SalesOrdersCumulative.SalesOrder = TT_ReservesBalances.SalesOrder
			AND TT_SalesOrdersCumulative.Products = TT_ReservesBalances.Products
			AND TT_SalesOrdersCumulative.Characteristic = TT_ReservesBalances.Characteristic
			AND (TT_ReservesBalances.Quantity &gt; TT_SalesOrdersCumulative.QuantityBalanceCumulative - TT_SalesOrdersCumulative.QuantityBalance)
		LEFT JOIN TT_GoodsInvoicedNotShippedBalances AS TT_GoodsInvoicedNotShippedBalances
		ON TT_SalesOrdersCumulative.Company = TT_GoodsInvoicedNotShippedBalances.Company
			AND TT_SalesOrdersCumulative.SalesOrder = TT_GoodsInvoicedNotShippedBalances.SalesOrder
			AND TT_SalesOrdersCumulative.Products = TT_GoodsInvoicedNotShippedBalances.Products
			AND TT_SalesOrdersCumulative.Characteristic = TT_GoodsInvoicedNotShippedBalances.Characteristic
;

