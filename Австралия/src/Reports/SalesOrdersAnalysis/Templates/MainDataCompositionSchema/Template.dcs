	TT_SalesOrders.Counterparty AS Counterparty,
	SUM(TT_SalesOrdersCumulative.Quantity) AS QuantityCumulative
INTO TT_SalesOrdersCumulative
FROM
	TT_SalesOrders AS TT_SalesOrders
		INNER JOIN TT_SalesOrders AS TT_SalesOrdersCumulative
		ON TT_SalesOrders.Company = TT_SalesOrdersCumulative.Company
			AND TT_SalesOrders.SalesOrder = TT_SalesOrdersCumulative.SalesOrder
			AND TT_SalesOrders.Products = TT_SalesOrdersCumulative.Products
			AND TT_SalesOrders.Characteristic = TT_SalesOrdersCumulative.Characteristic
			AND TT_SalesOrders.ShipmentDate &lt;= TT_SalesOrdersCumulative.ShipmentDate

GROUP BY
	TT_SalesOrders.SalesOrder,
	TT_SalesOrders.ShipmentDate,
	TT_SalesOrders.Counterparty,
	TT_SalesOrders.Characteristic,
	TT_SalesOrders.Company,
	TT_SalesOrders.Products,
	TT_SalesOrders.ConversionRate,
	TT_SalesOrders.Quantity
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_SalesOrdersCumulative.Company AS Company,
	TT_SalesOrdersCumulative.SalesOrder AS SalesOrder,
	TT_SalesOrdersCumulative.Products AS Products,
	TT_SalesOrdersCumulative.ConversionRate AS ConversionRate,
	TT_SalesOrdersCumulative.Characteristic AS Characteristic,
	TT_SalesOrdersCumulative.Quantity AS Quantity,
	TT_SalesOrdersCumulative.ShipmentDate AS ShipmentDate,
	TT_SalesOrdersCumulative.ShipmentDate = TT_SalesOrdersMaxShipmentDates.ShipmentDate AS IsMaxShipmentDate,
	TT_SalesOrdersCumulative.Counterparty AS Counterparty,
	CASE
		WHEN TT_SalesOrdersBalances.Quantity IS NULL
			THEN 0
		WHEN TT_SalesOrdersBalances.Quantity &gt; TT_SalesOrdersCumulative.QuantityCumulative
			THEN TT_SalesOrdersCumulative.Quantity
		ELSE TT_SalesOrdersBalances.Quantity - (TT_SalesOrdersCumulative.QuantityCumulative - TT_SalesOrdersCumulative.Quantity)
	END AS QuantityBalance
INTO TT_SalesOrdersWithBalances
FROM
	TT_SalesOrdersCumulative AS TT_SalesOrdersCumulative
		LEFT JOIN TT_SalesOrdersBalances AS TT_SalesOrdersBalances
		ON TT_SalesOrdersCumulative.Company = TT_SalesOrdersBalances.Company
			AND TT_SalesOrdersCumulative.SalesOrder = TT_SalesOrdersBalances.SalesOrder
			AND TT_SalesOrdersCumulative.Products = TT_SalesOrdersBalances.Products
			AND TT_SalesOrdersCumulative.Characteristic = TT_SalesOrdersBalances.Characteristic
			AND (TT_SalesOrdersBalances.Quantity &gt; TT_SalesOrdersCumulative.QuantityCumulative - TT_SalesOrdersCumulative.Quantity)
		LEFT JOIN TT_SalesOrdersMaxShipmentDates AS TT_SalesOrdersMaxShipmentDates
		ON TT_SalesOrdersCumulative.Company = TT_SalesOrdersMaxShipmentDates.Company
			AND TT_SalesOrdersCumulative.SalesOrder = TT_SalesOrdersMaxShipmentDates.SalesOrder
			AND TT_SalesOrdersCumulative.Products = TT_SalesOrdersMaxShipmentDates.Products
			AND TT_SalesOrdersCumulative.Characteristic = TT_SalesOrdersMaxShipmentDates.Characteristic
;

