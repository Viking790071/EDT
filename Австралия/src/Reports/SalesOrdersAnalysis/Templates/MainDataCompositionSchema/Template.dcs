	TT_SalesOrders.ConversionRate AS ConversionRate,
	TT_SalesOrders.Characteristic AS Characteristic,
	TT_SalesOrders.ShipmentDate AS ShipmentDate,
	TT_SalesOrders.Counterparty AS Counterparty,
	TT_SalesOrders.QuantityBalance - TT_SalesOrders.QuantityReserve AS QuantityReplenish,
	SUM(TT_SalesOrdersCumulative.QuantityBalance - TT_SalesOrdersCumulative.QuantityReserve) AS QuantityReplenishCumulative
INTO TT_SalesOrdersReplenishCumulative
FROM
	TT_SalesOrdersWithBalancesAndReserves AS TT_SalesOrders
		INNER JOIN TT_SalesOrdersWithBalancesAndReserves AS TT_SalesOrdersCumulative
		ON TT_SalesOrders.Company = TT_SalesOrdersCumulative.Company
			AND TT_SalesOrders.SalesOrder = TT_SalesOrdersCumulative.SalesOrder
			AND TT_SalesOrders.Products = TT_SalesOrdersCumulative.Products
			AND TT_SalesOrders.Characteristic = TT_SalesOrdersCumulative.Characteristic
			AND TT_SalesOrders.ShipmentDate &gt;= TT_SalesOrdersCumulative.ShipmentDate

GROUP BY
	TT_SalesOrders.SalesOrder,
	TT_SalesOrders.ShipmentDate,
	TT_SalesOrders.Counterparty,
	TT_SalesOrders.Characteristic,
	TT_SalesOrders.Company,
	TT_SalesOrders.Products,
	TT_SalesOrders.ConversionRate,
	TT_SalesOrders.Quantity,
	TT_SalesOrders.QuantityBalance,
	TT_SalesOrders.QuantityReserve,
	TT_SalesOrders.QuantityBalance - TT_SalesOrders.QuantityReserve
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_Replenishment.Company AS Company,
	TT_Replenishment.SalesOrder AS SalesOrder,
	TT_Replenishment.Products AS Products,
	TT_Replenishment.Characteristic AS Characteristic,
	TT_Replenishment.ReceiptDate AS ReceiptDate,
	TT_Replenishment.PurchaseOrders AS PurchaseOrders,
	TT_Replenishment.ProductionOrders AS ProductionOrders,
	TT_Replenishment.KitOrders AS KitOrders,
	TT_Replenishment.PurchaseOrders + TT_Replenishment.ProductionOrders + TT_Replenishment.KitOrders AS QuantityReplenishment,
	SUM(TT_ReplenishmentCumulative.PurchaseOrders + TT_ReplenishmentCumulative.ProductionOrders + TT_ReplenishmentCumulative.KitOrders) AS QuantityReplenishmentCumulative
INTO TT_ReplenishmentCumulative
FROM
	TT_Replenishment AS TT_Replenishment
		INNER JOIN TT_Replenishment AS TT_ReplenishmentCumulative
		ON TT_Replenishment.Company = TT_ReplenishmentCumulative.Company
			AND TT_Replenishment.SalesOrder = TT_ReplenishmentCumulative.SalesOrder
			AND TT_Replenishment.Products = TT_ReplenishmentCumulative.Products
			AND TT_Replenishment.Characteristic = TT_ReplenishmentCumulative.Characteristic
			AND TT_Replenishment.ReceiptDate &gt;= TT_ReplenishmentCumulative.ReceiptDate

GROUP BY
	TT_Replenishment.Company,
	TT_Replenishment.SalesOrder,
	TT_Replenishment.Characteristic,
	TT_Replenishment.Products,
	TT_Replenishment.ReceiptDate,
	TT_Replenishment.PurchaseOrders,
	TT_Replenishment.ProductionOrders,
	TT_Replenishment.KitOrders,
	TT_Replenishment.PurchaseOrders + TT_Replenishment.ProductionOrders + TT_Replenishment.KitOrders
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_SalesOrdersReplenishCumulative.Company AS Company,
	TT_SalesOrdersReplenishCumulative.SalesOrder AS SalesOrder,
	TT_SalesOrdersReplenishCumulative.Counterparty AS Counterparty,
	TT_SalesOrdersReplenishCumulative.Products AS Products,
	TT_SalesOrdersReplenishCumulative.ConversionRate AS ConversionRate,
	TT_SalesOrdersReplenishCumulative.Characteristic AS Characteristic,
	TT_SalesOrdersReplenishCumulative.ShipmentDate AS ShipmentDate,
	TT_ReplenishmentCumulative.ReceiptDate AS ReceiptDate,
	CASE
		WHEN TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders &gt; TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative - TT_SalesOrdersReplenishCumulative.QuantityReplenish
			THEN CASE
					WHEN TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative &gt; TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders
						THEN TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders
					ELSE TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative
				END - CASE
					WHEN TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative - TT_SalesOrdersReplenishCumulative.QuantityReplenish &gt; TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment
						THEN TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative - TT_SalesOrdersReplenishCumulative.QuantityReplenish
					ELSE TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment
				END
		ELSE 0
	END AS KitOrders,
	CASE
		WHEN TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders + TT_ReplenishmentCumulative.ProductionOrders &gt; TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative - TT_SalesOrdersReplenishCumulative.QuantityReplenish
				AND TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders &lt; TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative
			THEN CASE
					WHEN TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative &gt; TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders + TT_ReplenishmentCumulative.ProductionOrders
						THEN TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders + TT_ReplenishmentCumulative.ProductionOrders
					ELSE TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative
				END - CASE
					WHEN TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative - TT_SalesOrdersReplenishCumulative.QuantityReplenish &gt; TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders
						THEN TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative - TT_SalesOrdersReplenishCumulative.QuantityReplenish
					ELSE TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders
				END
		ELSE 0
	END AS ProductionOrders,
	CASE
		WHEN TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders + TT_ReplenishmentCumulative.ProductionOrders &lt; TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative
			THEN CASE
					WHEN TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative &gt; TT_ReplenishmentCumulative.QuantityReplenishmentCumulative
						THEN TT_ReplenishmentCumulative.QuantityReplenishmentCumulative
					ELSE TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative
				END - CASE
					WHEN TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative - TT_SalesOrdersReplenishCumulative.QuantityReplenish &gt; TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders + TT_ReplenishmentCumulative.ProductionOrders
						THEN TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative - TT_SalesOrdersReplenishCumulative.QuantityReplenish
					ELSE TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment + TT_ReplenishmentCumulative.KitOrders + TT_ReplenishmentCumulative.ProductionOrders
				END
		ELSE 0
	END AS PurchaseOrders
INTO TT_AllocatedReplenishment
FROM
	TT_SalesOrdersReplenishCumulative AS TT_SalesOrdersReplenishCumulative
		INNER JOIN TT_ReplenishmentCumulative AS TT_ReplenishmentCumulative
		ON TT_SalesOrdersReplenishCumulative.Company = TT_ReplenishmentCumulative.Company
			AND TT_SalesOrdersReplenishCumulative.SalesOrder = TT_ReplenishmentCumulative.SalesOrder
			AND TT_SalesOrdersReplenishCumulative.Products = TT_ReplenishmentCumulative.Products
			AND TT_SalesOrdersReplenishCumulative.Characteristic = TT_ReplenishmentCumulative.Characteristic
			AND (TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative - TT_SalesOrdersReplenishCumulative.QuantityReplenish &lt; TT_ReplenishmentCumulative.QuantityReplenishmentCumulative)
			AND (TT_SalesOrdersReplenishCumulative.QuantityReplenishCumulative &gt; TT_ReplenishmentCumulative.QuantityReplenishmentCumulative - TT_ReplenishmentCumulative.QuantityReplenishment)
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_AllocatedReplenishment.Company AS Company,
	TT_AllocatedReplenishment.SalesOrder AS SalesOrder,
	TT_AllocatedReplenishment.Products AS Products,
	TT_AllocatedReplenishment.Characteristic AS Characteristic,
	TT_AllocatedReplenishment.ReceiptDate AS ReceiptDate,
	SUM(TT_AllocatedReplenishment.KitOrders) AS KitOrders,
	SUM(TT_AllocatedReplenishment.ProductionOrders) AS ProductionOrders,
	SUM(TT_AllocatedReplenishment.PurchaseOrders) AS PurchaseOrders
INTO TT_AllocatedReplenishmentGrouped
FROM
	TT_AllocatedReplenishment AS TT_AllocatedReplenishment

GROUP BY
	TT_AllocatedReplenishment.SalesOrder,
	TT_AllocatedReplenishment.Products,
	TT_AllocatedReplenishment.ConversionRate,
	TT_AllocatedReplenishment.Characteristic,
	TT_AllocatedReplenishment.ReceiptDate,
	TT_AllocatedReplenishment.Company
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_Replenishment.Company AS Company,
	TT_Replenishment.SalesOrder AS SalesOrder,
	TT_Replenishment.Products AS Products,
	TT_Replenishment.Characteristic AS Characteristic,
	TT_Replenishment.ReceiptDate AS ReceiptDate,
	TT_Replenishment.PurchaseOrders - ISNULL(TT_AllocatedReplenishmentGrouped.PurchaseOrders, 0) AS PurchaseOrders,
	TT_Replenishment.ProductionOrders - ISNULL(TT_AllocatedReplenishmentGrouped.ProductionOrders, 0) AS ProductionOrders,
	TT_Replenishment.KitOrders - ISNULL(TT_AllocatedReplenishmentGrouped.KitOrders, 0) AS KitOrders
INTO TT_UnallocatedReplenishment
FROM
	TT_Replenishment AS TT_Replenishment
		LEFT JOIN TT_AllocatedReplenishmentGrouped AS TT_AllocatedReplenishmentGrouped
		ON TT_Replenishment.Company = TT_AllocatedReplenishmentGrouped.Company
			AND TT_Replenishment.SalesOrder = TT_AllocatedReplenishmentGrouped.SalesOrder
			AND TT_Replenishment.Products = TT_AllocatedReplenishmentGrouped.Products
			AND TT_Replenishment.Characteristic = TT_AllocatedReplenishmentGrouped.Characteristic
			AND TT_Replenishment.ReceiptDate = TT_AllocatedReplenishmentGrouped.ReceiptDate
WHERE
	(TT_Replenishment.PurchaseOrders - ISNULL(TT_AllocatedReplenishmentGrouped.PurchaseOrders, 0) &gt; 0
			OR TT_Replenishment.ProductionOrders - ISNULL(TT_AllocatedReplenishmentGrouped.ProductionOrders, 0) &gt; 0
			OR TT_Replenishment.KitOrders - ISNULL(TT_AllocatedReplenishmentGrouped.KitOrders, 0) &gt; 0)
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_SalesOrders.Company AS Company,
	TT_SalesOrders.SalesOrder AS SalesOrder,
	TT_SalesOrders.Counterparty AS Counterparty,
	TT_SalesOrders.Products AS Products,
	TT_SalesOrders.ConversionRate AS ConversionRate,
	TT_SalesOrders.Characteristic AS Characteristic,
	TT_SalesOrders.ShipmentDate AS ShipmentDate,
	NULL AS ReceiptDate,
	NULL AS ReceiptDateGroupField,
	TT_SalesOrders.Quantity AS Ordered,
	TT_SalesOrders.Quantity / TT_SalesOrders.ConversionRate AS ReportOrdered,
	TT_SalesOrders.QuantityBalance + TT_SalesOrders.NotShippedQuantityBalance AS Remaining,
	(TT_SalesOrders.QuantityBalance + TT_SalesOrders.NotShippedQuantityBalance) / TT_SalesOrders.ConversionRate AS ReportRemaining,
	TT_SalesOrders.Quantity - TT_SalesOrders.QuantityBalance - TT_SalesOrders.NotShippedQuantityBalance AS Shipped,
	(TT_SalesOrders.Quantity - TT_SalesOrders.QuantityBalance - TT_SalesOrders.NotShippedQuantityBalance) / TT_SalesOrders.ConversionRate AS ReportShipped,
	TT_SalesOrders.QuantityReserve AS Reserved,
	TT_SalesOrders.QuantityReserve / TT_SalesOrders.ConversionRate AS ReportReserved,
	0 AS KitOrders,
	0 AS ReportKitOrders,
	0 AS ProductionOrders,
	0 AS ReportProductionOrders,
	0 AS PurchaseOrders,
	0 AS ReportPurchaseOrders
FROM
	TT_SalesOrdersWithBalancesAndReserves AS TT_SalesOrders

UNION ALL

SELECT
	TT_AllocatedReplenishment.Company,
	TT_AllocatedReplenishment.SalesOrder,
	TT_AllocatedReplenishment.Counterparty,
	TT_AllocatedReplenishment.Products,
	TT_AllocatedReplenishment.ConversionRate,
	TT_AllocatedReplenishment.Characteristic,
	TT_AllocatedReplenishment.ShipmentDate,
	TT_AllocatedReplenishment.ReceiptDate,
	TT_AllocatedReplenishment.ReceiptDate,
	0,
	0,
	0,
	0,
	0,
	0,
	0,
	0,
	TT_AllocatedReplenishment.KitOrders,
	TT_AllocatedReplenishment.KitOrders / TT_AllocatedReplenishment.ConversionRate,
	TT_AllocatedReplenishment.ProductionOrders,
	TT_AllocatedReplenishment.ProductionOrders / TT_AllocatedReplenishment.ConversionRate,
	TT_AllocatedReplenishment.PurchaseOrders,
	TT_AllocatedReplenishment.PurchaseOrders / TT_AllocatedReplenishment.ConversionRate
FROM
	TT_AllocatedReplenishment AS TT_AllocatedReplenishment

UNION ALL

SELECT
	TT_SalesOrdersMaxShipmentDates.Company,
	TT_SalesOrdersMaxShipmentDates.SalesOrder,
	TT_SalesOrdersMaxShipmentDates.Counterparty,
	TT_SalesOrdersMaxShipmentDates.Products,
	TT_SalesOrdersMaxShipmentDates.ConversionRate,
	TT_SalesOrdersMaxShipmentDates.Characteristic,
	TT_SalesOrdersMaxShipmentDates.ShipmentDate,
	TT_UnallocatedReplenishment.ReceiptDate,
	TT_UnallocatedReplenishment.ReceiptDate,
	0,
	0,
	0,
	0,
	0,
	0,
	0,
	0,
	TT_UnallocatedReplenishment.KitOrders,
	TT_UnallocatedReplenishment.KitOrders / TT_SalesOrdersMaxShipmentDates.ConversionRate,
	TT_UnallocatedReplenishment.ProductionOrders,
	TT_UnallocatedReplenishment.ProductionOrders / TT_SalesOrdersMaxShipmentDates.ConversionRate,
	TT_UnallocatedReplenishment.PurchaseOrders,
	TT_UnallocatedReplenishment.PurchaseOrders / TT_SalesOrdersMaxShipmentDates.ConversionRate
FROM
	TT_SalesOrdersMaxShipmentDates AS TT_SalesOrdersMaxShipmentDates
		INNER JOIN TT_UnallocatedReplenishment AS TT_UnallocatedReplenishment
		ON TT_SalesOrdersMaxShipmentDates.Company = TT_UnallocatedReplenishment.Company
			AND TT_SalesOrdersMaxShipmentDates.SalesOrder = TT_UnallocatedReplenishment.SalesOrder
			AND TT_SalesOrdersMaxShipmentDates.Products = TT_UnallocatedReplenishment.Products
