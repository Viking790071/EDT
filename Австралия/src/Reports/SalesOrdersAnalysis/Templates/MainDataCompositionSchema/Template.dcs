		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	SalesOrders.Company AS Company,
	SalesOrders.SalesOrder AS SalesOrder,
	SalesOrders.Products AS Products,
	CASE
		WHEN CatalogProducts.ConversionRate = 0
			THEN 1
		ELSE CatalogProducts.ConversionRate
	END AS ConversionRate,
	SalesOrders.Characteristic AS Characteristic,
	SUM(SalesOrders.Quantity) AS Quantity,
	SalesOrders.ShipmentDate AS ShipmentDate,
	DocSalesOrder.Counterparty AS Counterparty
INTO TT_SalesOrders
FROM
	AccumulationRegister.SalesOrders AS SalesOrders
		INNER JOIN Document.SalesOrder AS DocSalesOrder
		ON SalesOrders.Recorder = DocSalesOrder.Ref
		LEFT JOIN Catalog.Products AS CatalogProducts
		ON SalesOrders.Products = CatalogProducts.Ref
WHERE
	SalesOrders.Active
	AND SalesOrders.RecordType = VALUE(AccumulationRecordType.Receipt)
	AND SalesOrders.Quantity &gt; 0
	AND NOT DocSalesOrder.Closed

GROUP BY
	SalesOrders.Company,
	SalesOrders.SalesOrder,
	SalesOrders.Products,
	CASE
		WHEN CatalogProducts.ConversionRate = 0
			THEN 1
		ELSE CatalogProducts.ConversionRate
	END,
	SalesOrders.Characteristic,
	SalesOrders.ShipmentDate,
	DocSalesOrder.Counterparty
;

