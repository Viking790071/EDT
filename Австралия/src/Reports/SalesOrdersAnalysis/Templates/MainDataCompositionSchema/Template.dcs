////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_SalesOrders.Company AS Company,
	TT_SalesOrders.SalesOrder AS SalesOrder,
	TT_SalesOrders.Products AS Products,
	TT_SalesOrders.ConversionRate AS ConversionRate,
	TT_SalesOrders.Characteristic AS Characteristic,
	TT_SalesOrders.ShipmentDate AS ShipmentDate,
