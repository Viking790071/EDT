	Inventory.CostObject AS CostObject,
	ISNULL(Inventory.CostObject.ProductionOrder, VALUE(Document.ProductionOrder.EmptyRef)) AS ProductionOrder,
	Inventory.CorrIncomeAndExpenseItem AS IncomeAndExpenseItem,
