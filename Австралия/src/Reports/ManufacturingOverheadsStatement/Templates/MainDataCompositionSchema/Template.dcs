SELECT
	Inventory.Period AS Period,
	Inventory.Company AS Company,
	Inventory.Recorder AS Recorder,
	Inventory.Products AS Products,
	Inventory.StructuralUnit AS StructuralUnit,
	Inventory.CostObject AS CostObject,
	Inventory.IncomeAndExpenseItem AS IncomeAndExpenseItem,
	ISNULL(Inventory.CostObject.ProductionOrder, VALUE(Document.ProductionOrder.EmptyRef)) AS ProductionOrder,
	SUM(CASE
			WHEN Inventory.RecordType = VALUE(AccumulationRecordType.Receipt)
				THEN Inventory.Amount
			ELSE -Inventory.Amount
		END) AS AmountDiffCOGS
INTO TT_PreDiffsCOGS
FROM
	AccumulationRegister.Inventory AS Inventory
WHERE
	Inventory.Company = &amp;Company
	AND Inventory.Recorder REFS Document.MonthEndClosing
	AND Inventory.InventoryAccountType = VALUE(Enum.InventoryAccountTypes.ManufacturingOverheads)
	AND Inventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod

GROUP BY
	Inventory.Period,
	Inventory.Company,
	Inventory.Recorder,
	Inventory.Products,
	Inventory.StructuralUnit,
	Inventory.CostObject,
	Inventory.IncomeAndExpenseItem,
	ISNULL(Inventory.CostObject.ProductionOrder, VALUE(Document.ProductionOrder.EmptyRef))
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_PreDiffsCOGS.Company AS Company,
	TT_PreDiffsCOGS.Recorder AS Recorder,
	TT_PreDiffsCOGS.Products AS Products,
	TT_PreDiffsCOGS.StructuralUnit AS StructuralUnit,
	TT_PreDiffsCOGS.CostObject AS CostObject,
	TT_PreDiffsCOGS.IncomeAndExpenseItem AS IncomeAndExpenseItem,
	TT_PreDiffsCOGS.ProductionOrder AS ProductionOrder,
	-TT_PreDiffsCOGS.AmountDiffCOGS - SUM(TT_Plan.PlanAmount) AS AmountDiffCOGS
INTO TT_PreDiffsCOGSWithPlan
FROM
	TT_PreDiffsCOGS AS TT_PreDiffsCOGS
		INNER JOIN TT_Plan AS TT_Plan
		ON TT_PreDiffsCOGS.Company = TT_Plan.Company
			AND TT_PreDiffsCOGS.StructuralUnit = TT_Plan.StructuralUnit
			AND TT_PreDiffsCOGS.IncomeAndExpenseItem = TT_Plan.IncomeAndExpenseItem
			AND (TT_Plan.Period BETWEEN BEGINOFPERIOD(TT_PreDiffsCOGS.Period, MONTH) AND TT_PreDiffsCOGS.Period)

GROUP BY
	TT_PreDiffsCOGS.Company,
	TT_PreDiffsCOGS.Recorder,
	TT_PreDiffsCOGS.Products,
	TT_PreDiffsCOGS.StructuralUnit,
	TT_PreDiffsCOGS.CostObject,
	TT_PreDiffsCOGS.IncomeAndExpenseItem,
	TT_PreDiffsCOGS.ProductionOrder,
	TT_PreDiffsCOGS.AmountDiffCOGS
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_PreDiffsCOGS.Company AS Company,
	TT_PreDiffsCOGS.Recorder AS Recorder,
	TT_PreDiffsCOGS.Products AS Products,
	TT_PreDiffsCOGS.StructuralUnit AS StructuralUnit,
	TT_PreDiffsCOGS.CostObject AS CostObject,
	TT_PreDiffsCOGS.IncomeAndExpenseItem AS IncomeAndExpenseItem,
	TT_PreDiffsCOGS.ProductionOrder AS ProductionOrder,
	ISNULL(TT_PreDiffsCOGSWithPlan.AmountDiffCOGS, TT_PreDiffsCOGS.AmountDiffCOGS) AS AmountDiffCOGS
INTO TT_DiffsCOGS
FROM
	TT_PreDiffsCOGS AS TT_PreDiffsCOGS
		INNER JOIN InformationRegister.AccountingPolicy AS AccountingPolicy
		ON TT_PreDiffsCOGS.Company = AccountingPolicy.Company
			AND (AccountingPolicy.UnderOverAllocatedOverheadsSetting &lt;&gt; VALUE(Enum.UnderOverAllocatedOverheadsSettings.AdjustedAllocationRate))
		INNER JOIN (SELECT
			MAX(AccountingPolicy.Period) AS MaxPeriod,
			TT_PreDiffsCOGS.Company AS Company,
			TT_PreDiffsCOGS.Period AS Period
		FROM
			TT_PreDiffsCOGS AS TT_PreDiffsCOGS
				INNER JOIN InformationRegister.AccountingPolicy AS AccountingPolicy
				ON TT_PreDiffsCOGS.Company = AccountingPolicy.Company
					AND TT_PreDiffsCOGS.Period &gt;= AccountingPolicy.Period
		
		GROUP BY
			TT_PreDiffsCOGS.Company,
			TT_PreDiffsCOGS.Period) AS PeriodSelect
		ON (AccountingPolicy.Company = PeriodSelect.Company)
			AND (AccountingPolicy.Period = PeriodSelect.MaxPeriod)
			AND TT_PreDiffsCOGS.Period = PeriodSelect.Period
		LEFT JOIN TT_PreDiffsCOGSWithPlan AS TT_PreDiffsCOGSWithPlan
		ON TT_PreDiffsCOGS.Recorder = TT_PreDiffsCOGSWithPlan.Recorder
			AND TT_PreDiffsCOGS.Products = TT_PreDiffsCOGSWithPlan.Products
			AND TT_PreDiffsCOGS.StructuralUnit = TT_PreDiffsCOGSWithPlan.StructuralUnit
			AND TT_PreDiffsCOGS.CostObject = TT_PreDiffsCOGSWithPlan.CostObject
			AND TT_PreDiffsCOGS.IncomeAndExpenseItem = TT_PreDiffsCOGSWithPlan.IncomeAndExpenseItem
;

