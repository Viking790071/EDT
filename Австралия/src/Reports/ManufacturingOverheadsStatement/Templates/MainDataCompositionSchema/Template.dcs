	AND NOT Inventory.Recorder REFS Document.MonthEndClosing
	AND Inventory.Company = &amp;Company
	AND Inventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod

GROUP BY
	Inventory.Company,
	Inventory.Products,
	Inventory.StructuralUnit,
	Inventory.CostObject,
	Inventory.IncomeAndExpenseItem,
	Inventory.Recorder,
	ISNULL(Inventory.CostObject.ProductionOrder, VALUE(Document.ProductionOrder.EmptyRef))
;

