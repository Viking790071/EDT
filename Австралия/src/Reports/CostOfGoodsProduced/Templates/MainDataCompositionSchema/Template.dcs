			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	InventoryReg.Products AS ComponentProduct,
	InventoryReg.Characteristic AS ComponentCharacteristic,
	InventoryReg.Ownership AS ComponentOwnership,
	SUM(InventoryReg.Quantity) AS ComponentQuantity,
	InventoryReg.Recorder AS Recorder,
	InventoryReg.GLAccount AS GLAccount,
	InventoryReg.IncomeAndExpenseItem AS IncomeAndExpenseItem,
	InventoryReg.InventoryAccountType AS InventoryAccountType
INTO TT_DisassemblyQuantity
FROM
	AccumulationRegister.Inventory AS InventoryReg
		INNER JOIN Document.Manufacturing AS Manufacturing
		ON InventoryReg.Recorder = Manufacturing.Ref
WHERE
	InventoryReg.RecordType = VALUE(AccumulationRecordType.Expense)
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN InventoryReg.Period &gt;= &amp;BeginOfPeriod
			ELSE InventoryReg.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END
	AND Manufacturing.OperationKind = VALUE(Enum.OperationTypesProduction.Disassembly)

GROUP BY
	InventoryReg.Products,
	InventoryReg.Characteristic,
	InventoryReg.Ownership,
	InventoryReg.Recorder,
	InventoryReg.GLAccount,
	InventoryReg.IncomeAndExpenseItem,
	InventoryReg.InventoryAccountType
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	Manufacturing.StructuralUnit AS Manufacturer,
	InventoryReg.ProductsCorr AS FinishedProduct,
	InventoryReg.CharacteristicCorr AS FinishedProductVariant,
	InventoryReg.OwnershipCorr AS FinishedProductOwnership,
	InventoryReg.ProductsCorr.MeasurementUnit AS FinishedProductUnit,
	0 AS FinishedQuantity,
	InventoryReg.Products AS ComponentProduct,
	InventoryReg.Characteristic AS ComponentCharacteristic,
	InventoryReg.Ownership AS ComponentOwnership,
	TT_DisassemblyQuantity.ComponentQuantity AS ComponentQuantity,
	SUM(InventoryReg.Amount) AS ComponentAmount,
	InventoryReg.Products.MeasurementUnit AS ComponentUnit,
	InventoryReg.Recorder AS Recorder,
	Manufacturing.Project AS Project,
	InventoryReg.Company AS Company,
	VALUE(Enum.OperationTypesProductionOrder.Disassembly) AS OperationKind,
	Manufacturing.PointInTime AS PointInTime
FROM
	AccumulationRegister.Inventory AS InventoryReg
		INNER JOIN Document.Manufacturing AS Manufacturing
		ON InventoryReg.Recorder = Manufacturing.Ref
		INNER JOIN TT_DisassemblyQuantity AS TT_DisassemblyQuantity
		ON InventoryReg.Recorder = TT_DisassemblyQuantity.Recorder
			AND InventoryReg.Products = TT_DisassemblyQuantity.ComponentProduct
			AND InventoryReg.Characteristic = TT_DisassemblyQuantity.ComponentCharacteristic
			AND InventoryReg.Ownership = TT_DisassemblyQuantity.ComponentOwnership
			AND (InventoryReg.InventoryAccountType = TT_DisassemblyQuantity.InventoryAccountType)
			AND (InventoryReg.GLAccount = TT_DisassemblyQuantity.GLAccount)
			AND (InventoryReg.IncomeAndExpenseItem = TT_DisassemblyQuantity.IncomeAndExpenseItem)
WHERE
	InventoryReg.RecordType = VALUE(AccumulationRecordType.Expense)
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN InventoryReg.Period &gt;= &amp;BeginOfPeriod
			ELSE InventoryReg.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END
	AND Manufacturing.OperationKind = VALUE(Enum.OperationTypesProduction.Disassembly)

GROUP BY
	Manufacturing.StructuralUnit,
	InventoryReg.ProductsCorr,
	InventoryReg.CharacteristicCorr,
	InventoryReg.OwnershipCorr,
	InventoryReg.ProductsCorr.MeasurementUnit,
	InventoryReg.Products,
	InventoryReg.Characteristic,
	InventoryReg.Ownership,
	InventoryReg.Products.MeasurementUnit,
	InventoryReg.Recorder,
	Manufacturing.Project,
	InventoryReg.Company,
	Manufacturing.PointInTime,
	TT_DisassemblyQuantity.ComponentQuantity</query>
		</item>
	</dataSet>
