			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	Manufacturing.StructuralUnit AS Manufacturer,
	InventoryReg.ProductsCorr AS FinishedProduct,
	InventoryReg.CharacteristicCorr AS FinishedProductVariant,
	InventoryReg.OwnershipCorr AS FinishedProductOwnership,
	InventoryReg.ProductsCorr.MeasurementUnit AS FinishedProductUnit,
	0 AS FinishedQuantity,
	InventoryReg.Products AS ComponentProduct,
	InventoryReg.Characteristic AS ComponentCharacteristic,
	InventoryReg.Ownership AS ComponentOwnership,
	InventoryReg.Quantity AS ComponentQuantity,
	InventoryReg.Amount AS ComponentAmount,
	InventoryReg.Products.MeasurementUnit AS ComponentUnit,
	InventoryReg.Recorder AS Recorder,
	Manufacturing.Project AS Project,
	InventoryReg.Company AS Company,
	VALUE(Enum.OperationTypesProductionOrder.Assembly) AS OperationKind,
	Manufacturing.PointInTime AS PointInTime
FROM
	AccumulationRegister.Inventory AS InventoryReg
		INNER JOIN Document.Manufacturing AS Manufacturing
		ON InventoryReg.Recorder = Manufacturing.Ref
WHERE
	InventoryReg.RecordType = VALUE(AccumulationRecordType.Expense)
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN InventoryReg.Period &gt;= &amp;BeginOfPeriod
			ELSE InventoryReg.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END
	AND Manufacturing.OperationKind = VALUE(Enum.OperationTypesProduction.Assembly)</query>
		</item>
