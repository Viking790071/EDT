			<query>SELECT ALLOWED
	Inventory.ProductsCorr AS FinishedProduct,
	Inventory.CharacteristicCorr AS FinishedProductVariant,
	Inventory.OwnershipCorr AS FinishedProductOwnership,
	Inventory.Products AS ComponentProduct,
	Inventory.Characteristic AS ComponentCharacteristic,
	Inventory.Ownership AS ComponentOwnership,
	CASE
		WHEN Inventory.RecordType = VALUE(AccumulationRecordType.Expense)
			THEN Inventory.Quantity
		ELSE -Inventory.Quantity
	END AS ComponentQuantity,
	CASE
		WHEN Inventory.RecordType = VALUE(AccumulationRecordType.Expense)
			THEN Inventory.Amount
		ELSE -Inventory.Amount
	END AS ComponentAmount,
	Inventory.Recorder AS Document,
	Inventory.CostObject AS CostObject,
	Inventory.Company AS Company,
	Inventory.PointInTime AS PointInTime,
	ProductsCatalog.MeasurementUnit AS Unit
INTO ComponentsTT
FROM
	AccumulationRegister.Inventory AS Inventory
		INNER JOIN Catalog.Products AS ProductsCatalog
		ON Inventory.Products = ProductsCatalog.Ref
WHERE
	Inventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
	AND Inventory.CostObject &lt;&gt; VALUE(Catalog.CostObjects.EmptyRef)
	AND Inventory.ProductsCorr &lt;&gt; UNDEFINED
	AND Inventory.ProductsCorr &lt;&gt; Inventory.Products

UNION ALL

SELECT
	RegInventory.ProductsCorr,
	RegInventory.CharacteristicCorr,
	RegInventory.OwnershipCorr,
	RegInventory.Products,
	RegInventory.Characteristic,
	RegInventory.Ownership,
	CASE
		WHEN RegInventory.RecordType = VALUE(AccumulationRecordType.Expense)
			THEN RegInventory.Quantity
		ELSE -RegInventory.Quantity
	END,
	CASE
		WHEN RegInventory.RecordType = VALUE(AccumulationRecordType.Expense)
			THEN RegInventory.Amount
		ELSE -RegInventory.Amount
	END,
	RegInventory.Recorder,
	DocWIP.CostObject,
	RegInventory.Company,
	RegInventory.PointInTime,
	ProductsCatalog.MeasurementUnit
FROM
	AccumulationRegister.Inventory AS RegInventory
		INNER JOIN Catalog.Products AS ProductsCatalog
		ON RegInventory.Products = ProductsCatalog.Ref
		INNER JOIN Document.Manufacturing AS DocManufacturing
		ON RegInventory.Recorder = DocManufacturing.Ref
		LEFT JOIN Document.ManufacturingOperation AS DocWIP
		ON (DocManufacturing.WorkInProgress = DocWIP.Ref)
WHERE
	RegInventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
	AND RegInventory.FixedCost
	AND RegInventory.ProductsCorr &lt;&gt; UNDEFINED
	AND RegInventory.ProductsCorr &lt;&gt; RegInventory.Products
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	RegInventory.Products AS FinishedProduct,
	RegInventory.Characteristic AS FinishedProductVariant,
	RegInventory.Ownership AS FinishedProductOwnership,
	RegInventory.ProductsCorr AS ComponentProduct,
	RegInventory.CharacteristicCorr AS ComponentCharacteristic,
	RegInventory.OwnershipCorr AS ComponentOwnership,
	CASE
		WHEN InventoryForQty.RecordType = VALUE(AccumulationRecordType.Expense)
			THEN InventoryForQty.Quantity
		ELSE -InventoryForQty.Quantity
	END AS ComponentQuantity,
	CASE
		WHEN InventoryForQty.RecordType = VALUE(AccumulationRecordType.Expense)
			THEN InventoryForQty.Amount
		ELSE -InventoryForQty.Amount
	END AS ComponentAmount,
	RegInventory.Recorder AS Document,
	DocWIP.CostObject AS CostObject,
	RegInventory.Company AS Company,
	RegInventory.PointInTime AS PointInTime,
	ProductsCatalog.MeasurementUnit AS Unit
INTO ByProductsTT
FROM
	AccumulationRegister.Inventory AS RegInventory
		INNER JOIN Catalog.Products AS ProductsCatalog
		ON RegInventory.ProductsCorr = ProductsCatalog.Ref
		LEFT JOIN AccumulationRegister.Inventory AS InventoryForQty
		ON RegInventory.Recorder = InventoryForQty.Recorder
			AND RegInventory.ProductsCorr = InventoryForQty.Products
			AND RegInventory.CharacteristicCorr = InventoryForQty.Characteristic
		INNER JOIN Document.Manufacturing AS DocManufacturing
		ON RegInventory.Recorder = DocManufacturing.Ref
		LEFT JOIN Document.ManufacturingOperation AS DocWIP
		ON (DocManufacturing.WorkInProgress = DocWIP.Ref)
WHERE
	RegInventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
	AND RegInventory.ProductsCorr &lt;&gt; UNDEFINED
	AND RegInventory.FixedCost
	AND RegInventory.ProductsCorr &lt;&gt; RegInventory.Products
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	Inventory.ProductsCorr AS FinishedProduct,
	Inventory.CharacteristicCorr AS FinishedProductVariant,
	Inventory.OwnershipCorr AS FinishedProductOwnership,
	Inventory.Products AS ComponentProduct,
	Inventory.Characteristic AS ComponentCharacteristic,
	Inventory.Ownership AS ComponentOwnership,
	Inventory.Quantity * ManufacturingActivities.StandardTimeInUOM AS ComponentQuantity,
	CASE
		WHEN Inventory.RecordType = VALUE(AccumulationRecordType.Expense)
			THEN Inventory.Amount
		ELSE -Inventory.Amount
	END AS ComponentAmount,
	Inventory.Recorder AS Document,
	Inventory.CostObject AS CostObject,
	Inventory.Company AS Company,
	Inventory.PointInTime AS PointInTime,
	ManufacturingActivities.TimeUOM AS TimeUOM
INTO OperationsTT
FROM
	AccumulationRegister.Inventory AS Inventory
		INNER JOIN Catalog.ManufacturingActivities AS ManufacturingActivities
		ON Inventory.Products = ManufacturingActivities.Ref
WHERE
	Inventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
	AND Inventory.CostObject &lt;&gt; VALUE(Catalog.CostObjects.EmptyRef)
	AND Inventory.ProductsCorr &lt;&gt; UNDEFINED
	AND Inventory.ProductsCorr &lt;&gt; Inventory.Products
;

////////////////////////////////////////////////////////////////////////////////
SELECT DISTINCT
	ComponentsTT.FinishedProduct AS FinishedProduct,
	ComponentsTT.FinishedProductVariant AS FinishedProductVariant,
	ComponentsTT.FinishedProductOwnership AS FinishedProductOwnership,
	ComponentsTT.Document AS Document,
	ComponentsTT.CostObject AS CostObject
INTO AllConsumptionTT
FROM
	ComponentsTT AS ComponentsTT

UNION ALL

SELECT
	ByProductsTT.FinishedProduct,
	ByProductsTT.FinishedProductVariant,
	ByProductsTT.FinishedProductOwnership,
	ByProductsTT.Document,
	ByProductsTT.CostObject
FROM
	ByProductsTT AS ByProductsTT

UNION ALL

SELECT DISTINCT
	OperationsTT.FinishedProduct,
	OperationsTT.FinishedProductVariant,
	OperationsTT.FinishedProductOwnership,
	OperationsTT.Document,
	OperationsTT.CostObject
FROM
	OperationsTT AS OperationsTT
;

////////////////////////////////////////////////////////////////////////////////
SELECT DISTINCT
	AllConsumptionTT.FinishedProduct AS FinishedProduct,
	AllConsumptionTT.FinishedProductVariant AS FinishedProductVariant,
	AllConsumptionTT.FinishedProductOwnership AS FinishedProductOwnership,
	AllConsumptionTT.Document AS Document,
	AllConsumptionTT.CostObject AS CostObject
INTO AllConsumptionGroupedTT
FROM
	AllConsumptionTT AS AllConsumptionTT
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	Inventory.Products AS FinishedProduct,
	Inventory.Characteristic AS FinishedProductVariant,
	Inventory.Ownership AS FinishedProductOwnership,
	SUM(CASE
			WHEN Inventory.RecordType = VALUE(AccumulationRecordType.Expense)
				THEN -Inventory.Quantity
			ELSE Inventory.Quantity
		END) AS FinishedQuantity,
	Inventory.Recorder AS Document,
	AllConsumptionGroupedTT.CostObject AS CostObject,
	Inventory.Company AS Company,
	ProductsCatalog.MeasurementUnit AS FinishedProductUnit,
	Inventory.PointInTime AS PointInTime,
	Inventory.LineNumber AS LineNumber
INTO FinishedTT
FROM
	AccumulationRegister.Inventory AS Inventory
		INNER JOIN AllConsumptionGroupedTT AS AllConsumptionGroupedTT
		ON Inventory.Recorder = AllConsumptionGroupedTT.Document
			AND Inventory.Products = AllConsumptionGroupedTT.FinishedProduct
			AND Inventory.Characteristic = AllConsumptionGroupedTT.FinishedProductVariant
			AND Inventory.Ownership = AllConsumptionGroupedTT.FinishedProductOwnership
		LEFT JOIN Catalog.Products AS ProductsCatalog
		ON Inventory.Products = ProductsCatalog.Ref

GROUP BY
	Inventory.Products,
	Inventory.Characteristic,
	Inventory.Recorder,
	AllConsumptionGroupedTT.CostObject,
	Inventory.Company,
	ProductsCatalog.MeasurementUnit,
	Inventory.PointInTime,
	Inventory.LineNumber,
	Inventory.Ownership
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	ProductionOrderDocument.StructuralUnit AS Manufacturer,
	ComponentsTT.FinishedProduct AS FinishedProduct,
	ComponentsTT.FinishedProductVariant AS FinishedProductVariant,
	ComponentsTT.FinishedProductOwnership AS FinishedProductOwnership,
	NULL AS FinishedProductUnit,
	NULL AS FinishedQuantity,
	ComponentsTT.ComponentProduct AS ComponentProduct,
	ComponentsTT.ComponentCharacteristic AS ComponentCharacteristic,
	ComponentsTT.ComponentOwnership AS ComponentOwnership,
	ComponentsTT.ComponentQuantity AS ComponentQuantity,
	ComponentsTT.ComponentAmount AS ComponentAmount,
	ComponentsTT.Unit AS ComponentUnit,
	ComponentsTT.Document AS Recorder,
	ComponentsTT.CostObject AS CostObject,
	CostObjects.Project AS Project,
	ComponentsTT.Company AS Company,
	ProductionOrderDocument.OperationKind AS OperationKind,
	ComponentsTT.PointInTime AS PointInTime,
	NULL AS OperationUnit,
	NULL AS OperationQuantity,
	NULL AS OperationAmount
FROM
	ComponentsTT AS ComponentsTT
		LEFT JOIN Catalog.CostObjects AS CostObjects
			LEFT JOIN Document.ProductionOrder AS ProductionOrderDocument
			ON CostObjects.ProductionOrder = ProductionOrderDocument.Ref
		ON ComponentsTT.CostObject = CostObjects.Ref

UNION ALL

SELECT
	ProductionOrderDocument.StructuralUnit,
	OperationsTT.FinishedProduct,
	OperationsTT.FinishedProductVariant,
	OperationsTT.FinishedProductOwnership,
	NULL,
	NULL,
	OperationsTT.ComponentProduct,
	NULL,
	VALUE(Catalog.InventoryOwnership.EmptyRef),
	NULL,
	NULL,
	NULL,
	OperationsTT.Document,
	OperationsTT.CostObject,
	CostObjects.Project,
	OperationsTT.Company,
	ProductionOrderDocument.OperationKind,
	OperationsTT.PointInTime,
	OperationsTT.TimeUOM,
	OperationsTT.ComponentQuantity,
	OperationsTT.ComponentAmount
FROM
	OperationsTT AS OperationsTT
		LEFT JOIN Catalog.CostObjects AS CostObjects
			LEFT JOIN Document.ProductionOrder AS ProductionOrderDocument
			ON CostObjects.ProductionOrder = ProductionOrderDocument.Ref
		ON OperationsTT.CostObject = CostObjects.Ref

UNION ALL

SELECT
	ProductionOrderDocument.StructuralUnit,
	FinishedTT.FinishedProduct,
	FinishedTT.FinishedProductVariant,
	FinishedTT.FinishedProductOwnership,
	FinishedTT.FinishedProductUnit,
	SUM(FinishedTT.FinishedQuantity),
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	FinishedTT.Document,
	FinishedTT.CostObject,
	CostObjects.Project,
	FinishedTT.Company,
	ProductionOrderDocument.OperationKind,
	FinishedTT.PointInTime,
	NULL,
	NULL,
	NULL
FROM
	FinishedTT AS FinishedTT
		LEFT JOIN Catalog.CostObjects AS CostObjects
			LEFT JOIN Document.ProductionOrder AS ProductionOrderDocument
			ON CostObjects.ProductionOrder = ProductionOrderDocument.Ref
		ON FinishedTT.CostObject = CostObjects.Ref

GROUP BY
	ProductionOrderDocument.StructuralUnit,
	FinishedTT.FinishedProduct,
	FinishedTT.FinishedProductVariant,
	FinishedTT.FinishedProductUnit,
	FinishedTT.Document,
	FinishedTT.CostObject,
	CostObjects.Project,
	FinishedTT.Company,
	ProductionOrderDocument.OperationKind,
	FinishedTT.PointInTime,
	FinishedTT.FinishedProductOwnership

UNION ALL

SELECT
	ProductionOrderDocument.StructuralUnit,
	ByProductsTT.FinishedProduct,
	ByProductsTT.FinishedProductVariant,
	ByProductsTT.FinishedProductOwnership,
	NULL,
	NULL,
	ByProductsTT.ComponentProduct,
	ByProductsTT.ComponentCharacteristic,
	ByProductsTT.ComponentOwnership,
	ByProductsTT.ComponentQuantity,
	ByProductsTT.ComponentAmount,
	ByProductsTT.Unit,
	ByProductsTT.Document,
	ByProductsTT.CostObject,
	CostObjects.Project,
	ByProductsTT.Company,
	ProductionOrderDocument.OperationKind,
	ByProductsTT.PointInTime,
	NULL,
	NULL,
	NULL
FROM
	ByProductsTT AS ByProductsTT
		LEFT JOIN Catalog.CostObjects AS CostObjects
			LEFT JOIN Document.ProductionOrder AS ProductionOrderDocument
			ON CostObjects.ProductionOrder = ProductionOrderDocument.Ref
		ON ByProductsTT.CostObject = CostObjects.Ref</query>
