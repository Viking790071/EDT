			<query>SELECT ALLOWED
	Manufacturing.StructuralUnit AS Manufacturer,
	ProductRelease.Products AS FinishedProduct,
	ProductRelease.Characteristic AS FinishedProductVariant,
	ProductRelease.Ownership AS FinishedProductOwnership,
	ProductRelease.Products.MeasurementUnit AS FinishedProductUnit,
	ProductRelease.Quantity AS FinishedQuantity,
	ProductRelease.Company AS Company,
	ProductRelease.Recorder AS Recorder,
	CASE
		WHEN Manufacturing.OperationKind = VALUE(Enum.OperationTypesProduction.Assembly)
			THEN VALUE(Enum.OperationTypesProductionOrder.Assembly)
		ELSE VALUE(Enum.OperationTypesProductionOrder.Disassembly)
	END AS OperationKind,
	ProductRelease.PointInTime AS PointInTime,
	Manufacturing.Project AS Project
FROM
	AccumulationRegister.ProductRelease AS ProductRelease
		INNER JOIN Document.Manufacturing AS Manufacturing
		ON ProductRelease.Recorder = Manufacturing.Ref
WHERE
	ProductRelease.Quantity &lt;&gt; 0
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN ProductRelease.Period &gt;= &amp;BeginOfPeriod
			ELSE ProductRelease.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END
