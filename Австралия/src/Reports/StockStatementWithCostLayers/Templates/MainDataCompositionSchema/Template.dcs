		<dataPath>CostOfUnits</dataPath>
		<expression>CASE WHEN SUM(QuantityClosingBalance) = 0 OR SUM(QuantityClosingBalance) IS NULL THEN 0 ELSE SUM(AmountClosingBalance) / SUM(QuantityClosingBalance) END</expression>
		<group>Products</group>
	</totalField>
	<totalField>
		<dataPath>ReportClosingBalance</dataPath>
		<expression>EvalExpression("Sum(ReportOpeningBalance)", , , "Current", "Current") + EvalExpression("Sum(ReportReceipt - ReportExpense)", , , "Current", "Current")</expression>
	</totalField>
	<totalField>
		<dataPath>ReportExpense</dataPath>
		<expression>Sum(ReportExpense)</expression>
	</totalField>
	<totalField>
		<dataPath>ReportOpeningBalance</dataPath>
		<expression>Sum(ReportOpeningBalance)</expression>
	</totalField>
	<totalField>
		<dataPath>ReportReceipt</dataPath>
		<expression>Sum(ReportReceipt)</expression>
	</totalField>
	<totalField>
		<dataPath>ReportTurnover</dataPath>
		<expression>Sum(ReportTurnover)</expression>
	</totalField>
