		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN InventoryCostLayer.Recorder = UNDEFINED
			THEN NULL
		ELSE InventoryCostLayer.Recorder
	END AS Recorder,
	InventoryCostLayer.SecondPeriod AS SecondPeriod,
	InventoryCostLayer.MinutePeriod AS MinutePeriod,
	InventoryCostLayer.HourPeriod AS HourPeriod,
	InventoryCostLayer.DayPeriod AS DayPeriod,
	InventoryCostLayer.WeekPeriod AS WeekPeriod,
	InventoryCostLayer.TenDaysPeriod AS TenDaysPeriod,
	InventoryCostLayer.MonthPeriod AS MonthPeriod,
	InventoryCostLayer.QuarterPeriod AS QuarterPeriod,
	InventoryCostLayer.YearPeriod AS YearPeriod,
	InventoryCostLayer.Company AS Company,
	InventoryCostLayer.PresentationCurrency AS PresentationCurrency,
	InventoryCostLayer.StructuralUnit AS StructuralUnit,
	InventoryCostLayer.Products AS Products,
	InventoryCostLayer.Characteristic AS Characteristic,
	InventoryCostLayer.Batch AS Batch,
	InventoryCostLayer.Ownership AS Ownership,
	InventoryCostLayer.CostLayer AS CostLayer,
	InventoryCostLayer.QuantityOpeningBalance AS QuantityOpeningBalance,
	InventoryCostLayer.QuantityClosingBalance AS QuantityClosingBalance,
	InventoryCostLayer.QuantityTurnover AS QuantityTurnover,
	InventoryCostLayer.QuantityReceipt AS QuantityReceipt,
	InventoryCostLayer.QuantityExpense AS QuantityExpense,
	InventoryCostLayer.AmountOpeningBalance AS AmountOpeningBalance,
	InventoryCostLayer.AmountClosingBalance AS AmountClosingBalance,
	InventoryCostLayer.AmountTurnover AS AmountTurnover,
	InventoryCostLayer.AmountReceipt AS AmountReceipt,
	InventoryCostLayer.AmountExpense AS AmountExpense,
	ProductsRef.MeasurementUnit AS ProductsMeasurementUnit,
	CASE
		WHEN ProductsRef.ConversionRate = 0
			THEN InventoryCostLayer.QuantityOpeningBalance
		ELSE InventoryCostLayer.QuantityOpeningBalance / ProductsRef.ConversionRate
	END AS ReportOpeningBalance,
	CASE
		WHEN ProductsRef.ConversionRate = 0
			THEN InventoryCostLayer.QuantityClosingBalance
		ELSE InventoryCostLayer.QuantityClosingBalance / ProductsRef.ConversionRate
	END AS ReportClosingBalance,
	CASE
		WHEN ProductsRef.ConversionRate = 0
			THEN InventoryCostLayer.QuantityTurnover
		ELSE InventoryCostLayer.QuantityTurnover / ProductsRef.ConversionRate
	END AS ReportTurnover,
	CASE
		WHEN ProductsRef.ConversionRate = 0
			THEN InventoryCostLayer.QuantityReceipt
		ELSE InventoryCostLayer.QuantityReceipt / ProductsRef.ConversionRate
	END AS ReportReceipt,
	CASE
		WHEN ProductsRef.ConversionRate = 0
			THEN InventoryCostLayer.QuantityExpense
		ELSE InventoryCostLayer.QuantityExpense / ProductsRef.ConversionRate
	END AS ReportExpense,
	ProductsRef.ReportUOM AS ReportUOM,
	InventoryCostLayer.InventoryAccountType AS InventoryAccountType
INTO CostsLayers
FROM
	AccumulationRegister.InventoryCostLayer.BalanceAndTurnovers(, , Auto, , InventoryAccountType IN (VALUE(Enum.InventoryAccountTypes.InventoryOnHand), VALUE(Enum.InventoryAccountTypes.ThirdPartyInventory), VALUE(Enum.InventoryAccountTypes.ComponentsForSubcontractor), VALUE(Enum.InventoryAccountTypes.CustomerOwnedComponents))) AS InventoryCostLayer
		LEFT JOIN Catalog.Products AS ProductsRef
		ON InventoryCostLayer.Products = ProductsRef.Ref

UNION ALL

SELECT
	CASE
		WHEN LandedCosts.Recorder = UNDEFINED
			THEN NULL
		ELSE LandedCosts.Recorder
	END,
	LandedCosts.SecondPeriod,
	LandedCosts.MinutePeriod,
	LandedCosts.HourPeriod,
	LandedCosts.DayPeriod,
	LandedCosts.WeekPeriod,
	LandedCosts.TenDaysPeriod,
	LandedCosts.MonthPeriod,
	LandedCosts.QuarterPeriod,
	LandedCosts.YearPeriod,
	LandedCosts.Company,
	LandedCosts.PresentationCurrency,
	LandedCosts.StructuralUnit,
	LandedCosts.Products,
	LandedCosts.Characteristic,
	LandedCosts.Batch,
	LandedCosts.Ownership,
	LandedCosts.CostLayer,
	0,
	0,
	0,
	0,
	0,
	LandedCosts.AmountOpeningBalance,
	LandedCosts.AmountClosingBalance,
	LandedCosts.AmountTurnover,
	LandedCosts.AmountReceipt,
	LandedCosts.AmountExpense,
	ProductsRef.MeasurementUnit,
	0,
	0,
	0,
	0,
	0,
	ProductsRef.ReportUOM,
	LandedCosts.InventoryAccountType
FROM
	AccumulationRegister.LandedCosts.BalanceAndTurnovers(, , Auto, , InventoryAccountType IN (VALUE(Enum.InventoryAccountTypes.InventoryOnHand), VALUE(Enum.InventoryAccountTypes.ThirdPartyInventory), VALUE(Enum.InventoryAccountTypes.ComponentsForSubcontractor), VALUE(Enum.InventoryAccountTypes.CustomerOwnedComponents))) AS LandedCosts
		LEFT JOIN Catalog.Products AS ProductsRef
		ON LandedCosts.Products = ProductsRef.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	CostsLayers.Recorder AS Recorder,
	CostsLayers.SecondPeriod AS SecondPeriod,
	CostsLayers.MinutePeriod AS MinutePeriod,
	CostsLayers.HourPeriod AS HourPeriod,
	CostsLayers.DayPeriod AS DayPeriod,
	CostsLayers.WeekPeriod AS WeekPeriod,
	CostsLayers.TenDaysPeriod AS TenDaysPeriod,
	CostsLayers.MonthPeriod AS MonthPeriod,
	CostsLayers.QuarterPeriod AS QuarterPeriod,
	CostsLayers.YearPeriod AS YearPeriod,
	CostsLayers.Company AS Company,
	CostsLayers.PresentationCurrency AS PresentationCurrency,
	CostsLayers.StructuralUnit AS StructuralUnit,
	CostsLayers.Products AS Products,
	CostsLayers.Characteristic AS Characteristic,
	CostsLayers.Batch AS Batch,
	CostsLayers.Ownership AS Ownership,
	CostsLayers.CostLayer AS CostLayer,
	SUM(CostsLayers.QuantityOpeningBalance) AS QuantityOpeningBalance,
	SUM(CostsLayers.QuantityClosingBalance) AS QuantityClosingBalance,
	SUM(CostsLayers.QuantityTurnover) AS QuantityTurnover,
	SUM(CostsLayers.QuantityReceipt) AS QuantityReceipt,
	SUM(CostsLayers.QuantityExpense) AS QuantityExpense,
	SUM(CostsLayers.AmountOpeningBalance) AS AmountOpeningBalance,
	SUM(CostsLayers.AmountClosingBalance) AS AmountClosingBalance,
	SUM(CostsLayers.AmountTurnover) AS AmountTurnover,
	SUM(CostsLayers.AmountReceipt) AS AmountReceipt,
	SUM(CostsLayers.AmountExpense) AS AmountExpense,
	CostsLayers.ProductsMeasurementUnit AS ProductsMeasurementUnit,
	CostsLayers.ReportOpeningBalance AS ReportOpeningBalance,
	CostsLayers.ReportClosingBalance AS ReportClosingBalance,
	CostsLayers.ReportTurnover AS ReportTurnover,
	CostsLayers.ReportReceipt AS ReportReceipt,
	CostsLayers.ReportExpense AS ReportExpense,
	CostsLayers.ReportUOM AS ReportUOM,
	CostsLayers.InventoryAccountType AS InventoryAccountType
FROM
	CostsLayers AS CostsLayers

GROUP BY
	CostsLayers.Recorder,
	CostsLayers.SecondPeriod,
	CostsLayers.MinutePeriod,
	CostsLayers.HourPeriod,
	CostsLayers.DayPeriod,
	CostsLayers.WeekPeriod,
	CostsLayers.TenDaysPeriod,
	CostsLayers.MonthPeriod,
	CostsLayers.QuarterPeriod,
	CostsLayers.YearPeriod,
	CostsLayers.Company,
	CostsLayers.StructuralUnit,
	CostsLayers.Products,
	CostsLayers.Characteristic,
	CostsLayers.Batch,
	CostsLayers.Ownership,
	CostsLayers.CostLayer,
	CostsLayers.ProductsMeasurementUnit,
	CostsLayers.ReportOpeningBalance,
	CostsLayers.ReportClosingBalance,
	CostsLayers.ReportTurnover,
	CostsLayers.ReportReceipt,
	CostsLayers.ReportExpense,
	CostsLayers.ReportUOM,
	CostsLayers.PresentationCurrency,
	CostsLayers.InventoryAccountType</query>
