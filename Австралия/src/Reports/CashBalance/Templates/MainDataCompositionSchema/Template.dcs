	<template>
		<name>Template1</name>
		<template xmlns:dcsat="http://v8.1c.ru/8.1/data-composition-system/area-template" xsi:type="dcsat:AreaTemplate">
			<dcsat:item xsi:type="dcsat:TableRow">
				<dcsat:tableCell>
					<dcsat:item xsi:type="dcsat:Field">
						<dcsat:value xsi:type="dcscor:Parameter">DynamicPeriod</dcsat:value>
					</dcsat:item>
				</dcsat:tableCell>
			</dcsat:item>
		</template>
		<parameter xmlns:dcsat="http://v8.1c.ru/8.1/data-composition-system/area-template" xsi:type="dcsat:ExpressionAreaTemplateParameter">
			<dcsat:name>DynamicPeriod</dcsat:name>
			<dcsat:expression>CASE 
WHEN &amp;Periodicity = VALUE(Enum.Periodicity.Day) 
 THEN Format(DynamicPeriod, "DF=dd.MM.yy") 
 WHEN &amp;Periodicity = VALUE(Enum.Periodicity.Week)
 THEN Format(DynamicPeriod, "DF=dd.MM.yy") 
 WHEN &amp;Periodicity = VALUE(Enum.Periodicity.TenDays)
 THEN Format(DynamicPeriod, "DF=dd.MM.yy")
 WHEN &amp;Periodicity = VALUE(Enum.Periodicity.Month)
 THEN Format(DynamicPeriod, "DF = 'MMM yy'")
 WHEN &amp;Periodicity = VALUE(Enum.Periodicity.Quarter)
 THEN Format(DynamicPeriod, "DF = 'q ""app."" yy'")
 WHEN &amp;Periodicity = VALUE(Enum.Periodicity.HalfYear)
 THEN Format(DynamicPeriod, "DF = 'MM.yy'")
 WHEN &amp;Periodicity = VALUE(Enum.Periodicity.Year)
 THEN Format(DynamicPeriod, "DF = 'yyyy'")
END</dcsat:expression>
		</parameter>
	</template>
