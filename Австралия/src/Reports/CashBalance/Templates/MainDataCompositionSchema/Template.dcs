			<field xsi:type="DataSetFieldField">
				<dataPath>Recorder</dataPath>
				<field>Recorder</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>Amount</dataPath>
				<field>Amount</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>AmountCur</dataPath>
				<field>AmountCur</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>IndexOfSet</dataPath>
				<field>IndexOfSet</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>PaymentMethod</dataPath>
				<field>PaymentMethod</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>PresentationCurrency</dataPath>
				<field>PresentationCurrency</field>
			</field>
			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	1 AS IndexOfSet,
	CASE
		WHEN CashAssets.Recorder = UNDEFINED
			THEN NULL
		ELSE CashAssets.Recorder
	END AS Recorder,
	CashAssets.Company AS Company,
	CashAssets.PresentationCurrency AS PresentationCurrency,
	CashAssets.PaymentMethod AS PaymentMethod,
	CashAssets.BankAccountPettyCash AS BankAccountPettyCash,
	CashAssets.Currency AS Currency,
	CashAssets.Item AS Item,
	0 AS AmountOpeningBalance,
	0 AS AmountClosingBalance,
	0 AS AmountCurOpeningBalance,
	0 AS AmountCurClosingBalance,
	CASE
		WHEN CashAssets.RecordType = VALUE(AccumulationRecordType.Receipt)
			THEN CashAssets.Amount
		ELSE 0
	END AS AmountReceipt,
	CASE
		WHEN CashAssets.RecordType = VALUE(AccumulationRecordType.Expense)
			THEN CashAssets.Amount
		ELSE 0
	END AS AmountExpense,
	CASE
		WHEN CashAssets.RecordType = VALUE(AccumulationRecordType.Receipt)
			THEN CashAssets.AmountCur
		ELSE 0
	END AS AmountCurReceipt,
	CASE
		WHEN CashAssets.RecordType = VALUE(AccumulationRecordType.Expense)
			THEN CashAssets.AmountCur
		ELSE 0
	END AS AmountCurExpense,
	CASE
		WHEN CashAssets.RecordType = VALUE(AccumulationRecordType.Receipt)
			THEN CashAssets.Amount
		ELSE -CashAssets.Amount
	END AS Amount,
	CASE
		WHEN CashAssets.RecordType = VALUE(AccumulationRecordType.Receipt)
			THEN CashAssets.AmountCur
		ELSE -CashAssets.AmountCur
	END AS AmountCur,
	BEGINOFPERIOD(CashAssets.Period, DAY) AS DayPeriod,
	BEGINOFPERIOD(CashAssets.Period, WEEK) AS WeekPeriod,
	BEGINOFPERIOD(CashAssets.Period, TENDAYS) AS TenDaysPeriod,
	BEGINOFPERIOD(CashAssets.Period, MONTH) AS MonthPeriod,
	BEGINOFPERIOD(CashAssets.Period, QUARTER) AS QuarterPeriod,
	BEGINOFPERIOD(CashAssets.Period, YEAR) AS YearPeriod,
	BEGINOFPERIOD(CashAssets.Period, MINUTE) AS HourPeriod,
	BEGINOFPERIOD(CashAssets.Period, HOUR) AS MinutePeriod,
	CashAssets.Period AS SecondPeriod
FROM
	AccumulationRegister.CashAssets AS CashAssets
WHERE
	CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN CashAssets.Period &gt;= &amp;BeginOfPeriod
			ELSE CashAssets.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END</query>
		</item>
	</dataSet>
