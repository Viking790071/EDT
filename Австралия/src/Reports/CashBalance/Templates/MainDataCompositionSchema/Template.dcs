					<dcscom:dimension>true</dcscom:dimension>
				</role>
			</field>
			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	0 AS IndexOfSet,
	CASE
		WHEN CashAssetsBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE CashAssetsBalanceAndTurnovers.Recorder
	END AS Recorder,
	CashAssetsBalanceAndTurnovers.Company AS Company,
	CashAssetsBalanceAndTurnovers.PresentationCurrency AS PresentationCurrency,
	CashAssetsBalanceAndTurnovers.PaymentMethod AS PaymentMethod,
	CashAssetsBalanceAndTurnovers.BankAccountPettyCash AS BankAccountPettyCash,
	CashAssetsBalanceAndTurnovers.Currency AS Currency,
	CashAssetsBalanceAndTurnovers.AmountOpeningBalance AS AmountOpeningBalance,
	CashAssetsBalanceAndTurnovers.AmountClosingBalance AS AmountClosingBalance,
	CashAssetsBalanceAndTurnovers.AmountCurOpeningBalance AS AmountCurOpeningBalance,
	CashAssetsBalanceAndTurnovers.AmountCurClosingBalance AS AmountCurClosingBalance,
	0 AS AmountReceipt,
	0 AS AmountExpense,
	0 AS AmountCurReceipt,
	0 AS AmountCurExpense,
	CashAssetsBalanceAndTurnovers.DayPeriod AS DayPeriod,
	CashAssetsBalanceAndTurnovers.WeekPeriod AS WeekPeriod,
	CashAssetsBalanceAndTurnovers.TenDaysPeriod AS TenDaysPeriod,
	CashAssetsBalanceAndTurnovers.MonthPeriod AS MonthPeriod,
	CashAssetsBalanceAndTurnovers.QuarterPeriod AS QuarterPeriod,
	CashAssetsBalanceAndTurnovers.HalfYearPeriod AS HalfYearPeriod,
	CashAssetsBalanceAndTurnovers.YearPeriod AS YearPeriod,
	CashAssetsBalanceAndTurnovers.SecondPeriod AS SecondPeriod,
	CashAssetsBalanceAndTurnovers.MinutePeriod AS MinutePeriod,
	CashAssetsBalanceAndTurnovers.HourPeriod AS HourPeriod,
	0 AS Amount,
	0 AS AmountCur
FROM
	AccumulationRegister.CashAssets.BalanceAndTurnovers(, , Auto, , ) AS CashAssetsBalanceAndTurnovers</query>
