		<query>SELECT ALLOWED
	OrdersByFulfillmentMethodTurnovers.SalesOrder AS SalesOrder,
	OrdersByFulfillmentMethodTurnovers.PurchaseOrder AS PurchaseOrder,
	OrdersByFulfillmentMethodTurnovers.Customer AS Customer,
	OrdersByFulfillmentMethodTurnovers.Supplier AS Supplier,
	OrdersByFulfillmentMethodTurnovers.Products AS Products,
	OrdersByFulfillmentMethodTurnovers.ShippingAddress AS ShippingAddress,
	OrdersByFulfillmentMethodTurnovers.Characteristic AS Characteristic,
	OrdersByFulfillmentMethodTurnovers.FulfillmentMethod AS FulfillmentMethod,
	OrdersByFulfillmentMethodTurnovers.QuantityTurnover AS Quantity
INTO PreTable_OrdersByFulfillmentMethodTurnovers
FROM
	AccumulationRegister.OrdersByFulfillmentMethod.Turnovers AS OrdersByFulfillmentMethodTurnovers
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	PreTable_PurchaseOrdersByFulfillment.SalesOrder AS SalesOrder,
	PreTable_PurchaseOrdersByFulfillment.PurchaseOrder AS PurchaseOrder,
	PreTable_PurchaseOrdersByFulfillment.Products AS Products,
	PreTable_PurchaseOrdersByFulfillment.Characteristic AS Characteristic,
	PreTable_PurchaseOrdersByFulfillment.FulfillmentMethod AS FulfillmentMethod,
	PreTable_PurchaseOrdersByFulfillment.Quantity AS Quantity,
	PreTable_PurchaseOrdersByFulfillment.Customer AS Customer,
	PreTable_PurchaseOrdersByFulfillment.Supplier AS Supplier,
	PreTable_PurchaseOrdersByFulfillment.ShippingAddress AS ShippingAddress,
	CatalogProducts.MeasurementUnit.Presentation AS MeasurementUnit,
	CASE
		WHEN OrderPayments.Amount IS NULL
			THEN "NA"
		WHEN OrderPayments.Amount - OrderPayments.AdvanceAmount - OrderPayments.PaymentAmount = 0
			THEN "Paid"
		WHEN OrderPayments.AdvanceAmount + OrderPayments.PaymentAmount = 0
			THEN "NotPaid"
		WHEN OrderPayments.Amount - OrderPayments.AdvanceAmount - OrderPayments.PaymentAmount &gt; 0
			THEN "PaidInPart"
		ELSE "NotPaid"
	END AS PaymentStatusSalesOrder,
	PurchaseOrdersStatuses.StatusHistory AS StatusHistory,
	PurchaseOrdersStatuses.Status AS Status
FROM
	PreTable_OrdersByFulfillmentMethodTurnovers AS PreTable_PurchaseOrdersByFulfillment
		LEFT JOIN InformationRegister.OrderPayments AS OrderPayments
		ON (OrderPayments.Quote = PreTable_PurchaseOrdersByFulfillment.SalesOrder)
		LEFT JOIN InformationRegister.PurchaseOrdersStatuses.SliceLast({(&amp;EndOfPeriod)}, ) AS PurchaseOrdersStatuses
		ON PreTable_PurchaseOrdersByFulfillment.PurchaseOrder = PurchaseOrdersStatuses.PurchaseOrder
		LEFT JOIN Catalog.Products AS CatalogProducts
		ON PreTable_PurchaseOrdersByFulfillment.Products = CatalogProducts.Ref</query>
	</dataSet>
