	<totalField>
		<dataPath>AvailableStock</dataPath>
		<expression>Sum(AvailableStock)</expression>
	</totalField>
	<totalField>
		<dataPath>Required</dataPath>
		<expression>Sum(Required)</expression>
	</totalField>
	<totalField>
		<dataPath>Consumed</dataPath>
		<expression>Sum(Consumed)</expression>
	</totalField>
	<totalField>
		<dataPath>BalanceToProduce</dataPath>
		<expression>CASE WHEN Sum(TotalToProduce) - Sum(Produced) &gt; 0 THEN Sum(TotalToProduce) - Sum(Produced) ELSE 0 END</expression>
	</totalField>
	<totalField>
		<dataPath>TotalToProduce</dataPath>
		<expression>Sum(TotalToProduce)</expression>
	</totalField>
	<totalField>
		<dataPath>Produced</dataPath>
		<expression>Sum(Produced)</expression>
	</totalField>
	<totalField>
		<dataPath>RequiredStockBOM</dataPath>
		<expression>CASE WHEN Sum(Required) - Sum(Consumed) &gt; 0 THEN Sum(Required) - Sum(Consumed) ELSE 0 END</expression>
	</totalField>
	<totalField>
		<dataPath>RequiredStockWIP</dataPath>
		<expression>CASE WHEN Sum(Required) - Sum(Consumed) - Sum(TotalToProduce) + Sum(Produced) &gt; 0 THEN Sum(Required) - Sum(Consumed) - Sum(TotalToProduce) + Sum(Produced) ELSE 0 END</expression>
	</totalField>
	<totalField>
		<dataPath>Shortage</dataPath>
		<expression>CASE WHEN Sum(AvailableStock) &gt; 0
THEN 
	CASE WHEN (Sum(Required) - Sum(Consumed) - Sum(TotalToProduce) + Sum(Produced) - Sum(AvailableStock) - Sum(Reserved) - Sum(Ordered)) &gt; 0
	THEN (Sum(Required) - Sum(Consumed) - Sum(TotalToProduce) + Sum(Produced) - Sum(AvailableStock) - Sum(Reserved) - Sum(Ordered))
	ELSE 0 END
ELSE Sum(Required) - Sum(Consumed) - Sum(TotalToProduce)  - Sum(Reserved) - Sum(Ordered) + Sum(Produced) END</expression>
	</totalField>
	<totalField>
		<dataPath>Reserved</dataPath>
		<expression>Sum(Reserved)</expression>
	</totalField>
	<totalField>
		<dataPath>Ordered</dataPath>
		<expression>Sum(Ordered)</expression>
	</totalField>
