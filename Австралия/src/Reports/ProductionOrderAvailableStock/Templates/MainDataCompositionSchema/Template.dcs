		<dataSource>AvailableTable</dataSource>
		<query>SELECT ALLOWED
	ProductionOrderInventory.Products AS Products,
	ProductionOrderInventory.Characteristic AS Characteristic,
	ProductionOrderInventory.Quantity * ISNULL(UOM.Factor, 1) AS Required,
	0 AS Consumed
INTO TT_AssemblyBOMProducts
FROM
	Document.ProductionOrder.Inventory AS ProductionOrderInventory
		INNER JOIN Document.ProductionOrder AS ProductionOrder
		ON ProductionOrderInventory.Ref = ProductionOrder.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON ProductionOrderInventory.MeasurementUnit = UOM.Ref
WHERE
	ProductionOrderInventory.Ref = &amp;ProductionOrder
	AND ProductionOrder.OperationKind = VALUE(Enum.OperationTypesProductionOrder.Assembly)
	AND &amp;ComponentsSourceFilter = "BOM"

UNION ALL

SELECT
	ManufacturingInventory.Products,
	ManufacturingInventory.Characteristic,
	0,
	ManufacturingInventory.Quantity * ISNULL(UOM.Factor, 1)
FROM
	Document.Manufacturing.Inventory AS ManufacturingInventory
		INNER JOIN (SELECT DISTINCT
			Manufacturing.Ref AS Ref
		FROM
			Document.Manufacturing AS Manufacturing
				INNER JOIN Document.ProductionOrder AS ProductionOrder
				ON Manufacturing.BasisDocument = ProductionOrder.Ref
		WHERE
			ProductionOrder.Ref = &amp;ProductionOrder
			AND ProductionOrder.OperationKind = VALUE(Enum.OperationTypesProductionOrder.Assembly)
			AND Manufacturing.Posted) AS ProductionAssemblyDocs
		ON ManufacturingInventory.Ref = ProductionAssemblyDocs.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON ManufacturingInventory.MeasurementUnit = UOM.Ref
WHERE
	&amp;ComponentsSourceFilter = "BOM"
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	ProductionOrderProducts.Products AS Products,
	ProductionOrderProducts.Characteristic AS Characteristic,
	ProductionOrderProducts.Quantity * ISNULL(UOM.Factor, 1) AS Required,
	0 AS Consumed
INTO TT_DisassemblyBOMProducts
FROM
	Document.ProductionOrder.Products AS ProductionOrderProducts
		INNER JOIN Document.ProductionOrder AS ProductionOrder
		ON ProductionOrderProducts.Ref = ProductionOrder.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON ProductionOrderProducts.MeasurementUnit = UOM.Ref
WHERE
	ProductionOrderProducts.Ref = &amp;ProductionOrder
	AND ProductionOrder.OperationKind = VALUE(Enum.OperationTypesProductionOrder.Disassembly)
	AND &amp;ComponentsSourceFilter = "BOM"

UNION ALL

SELECT
	ManufacturingProducts.Products,
	ManufacturingProducts.Characteristic,
	0,
	ManufacturingProducts.Quantity * ISNULL(UOM.Factor, 1)
FROM
	Document.Manufacturing.Products AS ManufacturingProducts
		INNER JOIN (SELECT DISTINCT
			Manufacturing.Ref AS Ref
		FROM
			Document.Manufacturing AS Manufacturing
				INNER JOIN Document.ProductionOrder AS ProductionOrder
				ON Manufacturing.BasisDocument = ProductionOrder.Ref
		WHERE
			ProductionOrder.Ref = &amp;ProductionOrder
			AND ProductionOrder.OperationKind = VALUE(Enum.OperationTypesProductionOrder.Disassembly)
			AND Manufacturing.Posted) AS ProductionDisassemblyDocs
		ON ManufacturingProducts.Ref = ProductionDisassemblyDocs.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON ManufacturingProducts.MeasurementUnit = UOM.Ref
WHERE
	&amp;ComponentsSourceFilter = "BOM"
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	ProductionOrderProducts.Products AS Products,
	ProductionOrderProducts.Characteristic AS Characteristic,
	SUM(ProductionOrderProducts.Quantity * ISNULL(UOM.Factor, 1)) AS Quantity,
	ProductionOrderProducts.Specification AS Specification
INTO ProductionOrderProducts
FROM
	Document.ProductionOrder.Products AS ProductionOrderProducts
		INNER JOIN Document.ProductionOrder AS ProductionOrder
		ON ProductionOrderProducts.Ref = ProductionOrder.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON ProductionOrderProducts.MeasurementUnit = UOM.Ref
WHERE
	ProductionOrder.Ref = &amp;ProductionOrder
	AND ProductionOrder.OperationKind = VALUE(Enum.OperationTypesProductionOrder.Production)
	AND &amp;ComponentsSourceFilter = "BOM"

GROUP BY
	ProductionOrderProducts.Products,
	ProductionOrderProducts.Characteristic,
	ProductionOrderProducts.Specification
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	BillsOfMaterialsContent.Products AS Products,
	BillsOfMaterialsContent.Characteristic AS Characteristic,
	CASE
		WHEN BillsOfMaterials.Quantity &gt; 1
			THEN CASE
					WHEN BillsOfMaterialsContent.CalculationMethod = VALUE(Enum.BOMContentCalculationMethod.Proportional)
						THEN CAST(BillsOfMaterialsContent.Quantity / BillsOfMaterials.Quantity AS NUMBER(15, 4))
					ELSE (CAST(BillsOfMaterialsContent.Quantity / BillsOfMaterials.Quantity - 0.5 AS NUMBER(15, 0))) + 1
				END
		ELSE BillsOfMaterialsContent.Quantity
	END * ProductionOrderProducts.Quantity * ISNULL(UOM.Factor, 1) AS Quantity,
	BillsOfMaterialsContent.Specification AS Specification,
	BillsOfMaterialsContent.ManufacturedInProcess AS ManufacturedInProcess
INTO TT_ProductionBOMProductsLevel_1
FROM
	ProductionOrderProducts AS ProductionOrderProducts
		LEFT JOIN Catalog.BillsOfMaterials AS BillsOfMaterials
		ON ProductionOrderProducts.Specification = BillsOfMaterials.Ref
		LEFT JOIN Catalog.BillsOfMaterials.Content AS BillsOfMaterialsContent
		ON ProductionOrderProducts.Specification = BillsOfMaterialsContent.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON (BillsOfMaterialsContent.MeasurementUnit = UOM.Ref)
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	BillsOfMaterialsContent.Products AS Products,
	BillsOfMaterialsContent.Characteristic AS Characteristic,
	CASE
		WHEN BillsOfMaterials.Quantity &gt; 1
			THEN CASE
					WHEN BillsOfMaterialsContent.CalculationMethod = VALUE(Enum.BOMContentCalculationMethod.Proportional)
						THEN CAST(BillsOfMaterialsContent.Quantity / BillsOfMaterials.Quantity AS NUMBER(15, 4))
					ELSE (CAST(BillsOfMaterialsContent.Quantity / BillsOfMaterials.Quantity - 0.5 AS NUMBER(15, 0))) + 1
				END
		ELSE BillsOfMaterialsContent.Quantity
	END * TT_ProductionBOMProductsLevel_1.Quantity * ISNULL(UOM.Factor, 1) AS Quantity,
	BillsOfMaterialsContent.Specification AS Specification,
	BillsOfMaterialsContent.ManufacturedInProcess AS ManufacturedInProcess
INTO TT_ProductionBOMProductsLevel_2
FROM
	TT_ProductionBOMProductsLevel_1 AS TT_ProductionBOMProductsLevel_1
		LEFT JOIN Catalog.BillsOfMaterials AS BillsOfMaterials
		ON TT_ProductionBOMProductsLevel_1.Specification = BillsOfMaterials.Ref
		LEFT JOIN Catalog.BillsOfMaterials.Content AS BillsOfMaterialsContent
		ON TT_ProductionBOMProductsLevel_1.Specification = BillsOfMaterialsContent.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON (BillsOfMaterialsContent.MeasurementUnit = UOM.Ref)
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	BillsOfMaterialsContent.Products AS Products,
	BillsOfMaterialsContent.Characteristic AS Characteristic,
	CASE
		WHEN BillsOfMaterials.Quantity &gt; 1
			THEN CASE
					WHEN BillsOfMaterialsContent.CalculationMethod = VALUE(Enum.BOMContentCalculationMethod.Proportional)
						THEN CAST(BillsOfMaterialsContent.Quantity / BillsOfMaterials.Quantity AS NUMBER(15, 4))
					ELSE (CAST(BillsOfMaterialsContent.Quantity / BillsOfMaterials.Quantity - 0.5 AS NUMBER(15, 0))) + 1
				END
		ELSE BillsOfMaterialsContent.Quantity
	END * TT_ProductionBOMProductsLevel_2.Quantity * ISNULL(UOM.Factor, 1) AS Quantity,
	BillsOfMaterialsContent.ManufacturedInProcess AS ManufacturedInProcess
INTO TT_ProductionBOMProductsLevel_3
FROM
	TT_ProductionBOMProductsLevel_2 AS TT_ProductionBOMProductsLevel_2
		LEFT JOIN Catalog.BillsOfMaterials AS BillsOfMaterials
		ON TT_ProductionBOMProductsLevel_2.Specification = BillsOfMaterials.Ref
		LEFT JOIN Catalog.BillsOfMaterials.Content AS BillsOfMaterialsContent
		ON TT_ProductionBOMProductsLevel_2.Specification = BillsOfMaterialsContent.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON (BillsOfMaterialsContent.MeasurementUnit = UOM.Ref)
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TT_ProductionBOMProductsLevel_1.Products AS Products,
	TT_ProductionBOMProductsLevel_1.Characteristic AS Characteristic,
	SUM(TT_ProductionBOMProductsLevel_1.Quantity) AS Required,
	0 AS Consumed
INTO TT_ProductionBOMProducts
FROM
	TT_ProductionBOMProductsLevel_1 AS TT_ProductionBOMProductsLevel_1
WHERE
	TT_ProductionBOMProductsLevel_1.ManufacturedInProcess = FALSE

GROUP BY
	TT_ProductionBOMProductsLevel_1.Products,
	TT_ProductionBOMProductsLevel_1.Characteristic

UNION ALL

SELECT
	TT_ProductionBOMProductsLevel_2.Products,
	TT_ProductionBOMProductsLevel_2.Characteristic,
	SUM(TT_ProductionBOMProductsLevel_2.Quantity),
	0
FROM
	TT_ProductionBOMProductsLevel_2 AS TT_ProductionBOMProductsLevel_2
WHERE
	TT_ProductionBOMProductsLevel_2.ManufacturedInProcess = FALSE

GROUP BY
	TT_ProductionBOMProductsLevel_2.Products,
	TT_ProductionBOMProductsLevel_2.Characteristic

UNION ALL

SELECT
	TT_ProductionBOMProductsLevel_3.Products,
	TT_ProductionBOMProductsLevel_3.Characteristic,
	SUM(TT_ProductionBOMProductsLevel_3.Quantity),
	0
FROM
	TT_ProductionBOMProductsLevel_3 AS TT_ProductionBOMProductsLevel_3
WHERE
	TT_ProductionBOMProductsLevel_3.ManufacturedInProcess = FALSE

GROUP BY
	TT_ProductionBOMProductsLevel_3.Products,
	TT_ProductionBOMProductsLevel_3.Characteristic

UNION ALL

SELECT
	WorkInProgress.Products,
	WorkInProgress.Characteristic,
	0,
	WorkInProgress.Quantity
FROM
	Document.ManufacturingOperation AS ManufacturingOperation
		INNER JOIN AccumulationRegister.WorkInProgress AS WorkInProgress
		ON (ManufacturingOperation.BasisDocument = &amp;ProductionOrder)
			AND ManufacturingOperation.Ref = WorkInProgress.Recorder
			AND (VALUETYPE(WorkInProgress.Products) = TYPE(Catalog.Products))
WHERE
	&amp;ComponentsSourceFilter = "BOM"
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	ManufacturingOperation.Ref AS WIP,
	ManufacturingOperation.ReleaseRequired AS ReleaseRequired
INTO TT_ProductionWIP_AllWIPs
FROM
	Document.ManufacturingOperation AS ManufacturingOperation
WHERE
	ManufacturingOperation.BasisDocument = &amp;ProductionOrder
	AND ManufacturingOperation.Posted
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TT_ProductionWIP_AllWIPs.WIP AS WIP,
	TT_ProductionWIP_AllWIPs.ReleaseRequired AS ReleaseRequired
INTO TT_ProductionWIP_WIPs
FROM
	TT_ProductionWIP_AllWIPs AS TT_ProductionWIP_AllWIPs
WHERE
	&amp;ComponentsSourceFilter = "WIP"
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	ManufacturingOperation.Products AS Products,
	ManufacturingOperation.Characteristic AS Characteristic,
	ManufacturingOperation.Quantity * ISNULL(UOM.Factor, 1) AS TotalToProduce,
	0 AS Required,
	0 AS Consumed,
	0 AS Produced
INTO TT_ProductionWIPProducts
FROM
	TT_ProductionWIP_WIPs AS TT_ProductionWIP_WIPs
		INNER JOIN Document.ManufacturingOperation AS ManufacturingOperation
		ON TT_ProductionWIP_WIPs.WIP = ManufacturingOperation.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON (ManufacturingOperation.MeasurementUnit = UOM.Ref)
WHERE
	NOT TT_ProductionWIP_WIPs.ReleaseRequired

UNION ALL

SELECT
	ManufacturingOperationInventory.Products,
	ManufacturingOperationInventory.Characteristic,
	0,
	ManufacturingOperationInventory.Quantity * ISNULL(UOM.Factor, 1),
	0,
	0
FROM
	TT_ProductionWIP_WIPs AS TT_ProductionWIP_WIPs
		INNER JOIN Document.ManufacturingOperation.Inventory AS ManufacturingOperationInventory
		ON TT_ProductionWIP_WIPs.WIP = ManufacturingOperationInventory.Ref
		LEFT JOIN Catalog.UOM AS UOM
		ON (ManufacturingOperationInventory.MeasurementUnit = UOM.Ref)

UNION ALL

SELECT
	WorkInProgress.Products,
	WorkInProgress.Characteristic,
	0,
	0,
	WorkInProgress.Quantity,
	0
FROM
	TT_ProductionWIP_WIPs AS TT_ProductionWIP_WIPs
		LEFT JOIN AccumulationRegister.WorkInProgress AS WorkInProgress
		ON TT_ProductionWIP_WIPs.WIP = WorkInProgress.Recorder
			AND (VALUETYPE(WorkInProgress.Products) = TYPE(Catalog.Products))

UNION ALL

SELECT
	WorkInProgressStatement.Products,
	WorkInProgressStatement.Characteristic,
	0,
	0,
	0,
	WorkInProgressStatement.Quantity
FROM
	AccumulationRegister.WorkInProgressStatement AS WorkInProgressStatement
WHERE
	WorkInProgressStatement.ProductionOrder = &amp;ProductionOrder
	AND WorkInProgressStatement.RecordType = VALUE(AccumulationRecordType.Expense)
	AND NOT (WorkInProgressStatement.Products, WorkInProgressStatement.Characteristic) IN
				(SELECT
					ProductionOrderProducts.Products,
					ProductionOrderProducts.Characteristic
				FROM
					Document.ProductionOrder.Products AS ProductionOrderProducts
				WHERE
					ProductionOrderProducts.Ref = &amp;ProductionOrder)
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_AssemblyBOMProducts.Products AS Products,
	TT_AssemblyBOMProducts.Characteristic AS Characteristic,
	TT_AssemblyBOMProducts.Required AS Required,
	TT_AssemblyBOMProducts.Consumed AS Consumed,
	0 AS TotalToProduce,
	0 AS Produced
INTO TT_Products
FROM
	TT_AssemblyBOMProducts AS TT_AssemblyBOMProducts

UNION ALL

SELECT
	TT_DisassemblyBOMProducts.Products,
	TT_DisassemblyBOMProducts.Characteristic,
	TT_DisassemblyBOMProducts.Required,
	TT_DisassemblyBOMProducts.Consumed,
	0,
	0
FROM
	TT_DisassemblyBOMProducts AS TT_DisassemblyBOMProducts

UNION ALL

SELECT
	TT_ProductionBOMProducts.Products,
	TT_ProductionBOMProducts.Characteristic,
	TT_ProductionBOMProducts.Required,
	TT_ProductionBOMProducts.Consumed,
	0,
	0
FROM
	TT_ProductionBOMProducts AS TT_ProductionBOMProducts

UNION ALL

SELECT
	TT_ProductionWIPProducts.Products,
	TT_ProductionWIPProducts.Characteristic,
	TT_ProductionWIPProducts.Required,
	TT_ProductionWIPProducts.Consumed,
	TT_ProductionWIPProducts.TotalToProduce,
	TT_ProductionWIPProducts.Produced
FROM
	TT_ProductionWIPProducts AS TT_ProductionWIPProducts
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	InventoryBalances.Company AS Company,
	InventoryBalances.StructuralUnit AS StructuralUnit,
	InventoryBalances.Products AS Products,
	InventoryBalances.Characteristic AS Characteristic,
	InventoryBalances.Ownership AS Ownership,
	InventoryBalances.QuantityBalance AS Balance,
	0 AS Reserve,
	InventoryBalances.Batch AS Batch,
	ProductsBatches.BatchNumber AS BatchNumber,
	ProductsBatches.ExpirationDate AS ExpirationDate
INTO TT_BalanceAndReserves
FROM
	AccumulationRegister.Inventory.Balance(
			,
			VALUETYPE(StructuralUnit) = TYPE(Catalog.BusinessUnits)
				AND StructuralUnit &lt;&gt; VALUE(Catalog.BusinessUnits.DropShipping)
				AND InventoryAccountType = VALUE(Enum.InventoryAccountTypes.InventoryOnHand)
				AND (Products, Characteristic) IN
					(SELECT
						TT_Products.Products AS Products,
						TT_Products.Characteristic AS Characteristic
					FROM
						TT_Products AS TT_Products)) AS InventoryBalances
		LEFT JOIN Catalog.ProductsBatches AS ProductsBatches
		ON InventoryBalances.Batch = ProductsBatches.Ref

UNION ALL

SELECT
	ReservedProductsBalances.Company,
	ReservedProductsBalances.StructuralUnit,
	ReservedProductsBalances.Products,
	ReservedProductsBalances.Characteristic,
	VALUE(Catalog.InventoryOwnership.EmptyRef),
	0,
	ReservedProductsBalances.QuantityBalance,
	ReservedProductsBalances.Batch,
	ProductsBatches.BatchNumber,
	ProductsBatches.ExpirationDate
FROM
	AccumulationRegister.ReservedProducts.Balance(
			,
			SalesOrder &lt;&gt; UNDEFINED
				AND (Products, Characteristic) IN
					(SELECT
						TT_Products.Products AS Products,
						TT_Products.Characteristic AS Characteristic
					FROM
						TT_Products AS TT_Products)) AS ReservedProductsBalances
		LEFT JOIN Catalog.ProductsBatches AS ProductsBatches
		ON ReservedProductsBalances.Batch = ProductsBatches.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TT_BalanceAndReserves.Company AS Company,
	TT_BalanceAndReserves.StructuralUnit AS StructuralUnit,
	TT_BalanceAndReserves.Products AS Products,
	TT_BalanceAndReserves.Characteristic AS Characteristic,
	TT_BalanceAndReserves.Ownership AS Ownership,
	"TotalInAllWarehouses" AS TotalByWarehouses,
	SUM(TT_BalanceAndReserves.Balance) AS Balance,
	SUM(TT_BalanceAndReserves.Reserve) AS Reserve,
	SUM(TT_BalanceAndReserves.Balance) - SUM(TT_BalanceAndReserves.Reserve) AS Free,
	TT_BalanceAndReserves.Batch AS Batch,
	TT_BalanceAndReserves.BatchNumber AS BatchNumber,
	TT_BalanceAndReserves.ExpirationDate AS ExpirationDate
INTO TT_Available
FROM
	TT_BalanceAndReserves AS TT_BalanceAndReserves

GROUP BY
	TT_BalanceAndReserves.Company,
	TT_BalanceAndReserves.StructuralUnit,
	TT_BalanceAndReserves.Products,
	TT_BalanceAndReserves.Characteristic,
	TT_BalanceAndReserves.Ownership,
	TT_BalanceAndReserves.BatchNumber,
	TT_BalanceAndReserves.Batch,
	TT_BalanceAndReserves.ExpirationDate
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	ReservedProductsBalance.Company AS Company,
	ReservedProductsBalance.Products AS Products,
	ReservedProductsBalance.Characteristic AS Characteristic,
	VALUE(Catalog.InventoryOwnership.EmptyRef) AS Ownership,
	ReservedProductsBalance.QuantityBalance AS Reserved,
	0 AS Ordered
INTO TT_ReservesAndBackorders
FROM
	AccumulationRegister.ReservedProducts.Balance(
			,
			(Products, Characteristic) IN
					(SELECT
						TT_Products.Products AS Products,
						TT_Products.Characteristic AS Characteristic
					FROM
						TT_Products AS TT_Products)
				AND SalesOrder IN
					(SELECT
						ProductionOrder.Ref AS Ref
					FROM
						Document.ProductionOrder AS ProductionOrder
					WHERE
						ProductionOrder.Ref = &amp;ProductionOrder
						AND ProductionOrder.OperationKind = VALUE(Enum.OperationTypesProductionOrder.Disassembly)
			
					UNION ALL
			
					SELECT
						ProductionOrder.Ref
					FROM
						Document.ProductionOrder AS ProductionOrder
					WHERE
						ProductionOrder.Ref = &amp;ProductionOrder
						AND ProductionOrder.OperationKind = VALUE(Enum.OperationTypesProductionOrder.Assembly))) AS ReservedProductsBalance

UNION ALL

SELECT
	ReservedProductsBalance.Company,
	ReservedProductsBalance.Products,
	ReservedProductsBalance.Characteristic,
	VALUE(Catalog.InventoryOwnership.EmptyRef),
	ReservedProductsBalance.QuantityBalance,
	0
FROM
	AccumulationRegister.ReservedProducts.Balance(
			,
			(Products, Characteristic) IN
					(SELECT
						TT_Products.Products AS Products,
						TT_Products.Characteristic AS Characteristic
					FROM
						TT_Products AS TT_Products)
				AND SalesOrder IN
					(SELECT
						TT_ProductionWIP_AllWIPs.WIP AS WIP
					FROM
						TT_ProductionWIP_AllWIPs AS TT_ProductionWIP_AllWIPs)) AS ReservedProductsBalance

UNION ALL

SELECT
	BackordersBalance.Company,
	BackordersBalance.Products,
	BackordersBalance.Characteristic,
	VALUE(Catalog.InventoryOwnership.EmptyRef),
	0,
	BackordersBalance.QuantityBalance
FROM
	AccumulationRegister.Backorders.Balance(
			,
			(Products, Characteristic) IN
					(SELECT
						TT_Products.Products AS Products,
						TT_Products.Characteristic AS Characteristic
					FROM
						TT_Products AS TT_Products)
				AND SalesOrder IN
					(SELECT
						ProductionOrder.Ref AS Ref
					FROM
						Document.ProductionOrder AS ProductionOrder
					WHERE
						ProductionOrder.Ref = &amp;ProductionOrder
						AND ProductionOrder.OperationKind = VALUE(Enum.OperationTypesProductionOrder.Disassembly)
			
					UNION ALL
			
					SELECT
						ProductionOrder.Ref
					FROM
						Document.ProductionOrder AS ProductionOrder
					WHERE
						ProductionOrder.Ref = &amp;ProductionOrder
						AND ProductionOrder.OperationKind = VALUE(Enum.OperationTypesProductionOrder.Assembly))) AS BackordersBalance

UNION ALL

SELECT
	BackordersBalance.Company,
	BackordersBalance.Products,
	BackordersBalance.Characteristic,
	VALUE(Catalog.InventoryOwnership.EmptyRef),
	0,
	BackordersBalance.QuantityBalance
FROM
	AccumulationRegister.Backorders.Balance(
			,
			(Products, Characteristic) IN
					(SELECT
						TT_Products.Products AS Products,
						TT_Products.Characteristic AS Characteristic
					FROM
						TT_Products AS TT_Products)
				AND SalesOrder IN
					(SELECT
						TT_ProductionWIP_AllWIPs.WIP AS WIP
					FROM
						TT_ProductionWIP_AllWIPs AS TT_ProductionWIP_AllWIPs)) AS BackordersBalance
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TT_Products.Products AS Products,
	TT_Products.Characteristic AS Characteristic,
	TT_Products.Required AS Required,
	TT_Products.Consumed AS Consumed,
	NULL AS Company,
	NULL AS StructuralUnit,
	NULL AS Ownership,
	"TotalInAllWarehouses" AS TotalByWarehouses,
	0 AS AvailableStock,
	TT_Products.TotalToProduce AS TotalToProduce,
	TT_Products.Produced AS Produced,
	NULL AS Batch,
	NULL AS BatchNumber,
	NULL AS ExpirationDate,
	0 AS Reserved,
	0 AS Ordered
FROM
	TT_Products AS TT_Products

UNION ALL

SELECT
	TT_Available.Products,
	TT_Available.Characteristic,
	0,
	0,
	TT_Available.Company,
	TT_Available.StructuralUnit,
	TT_Available.Ownership,
	TT_Available.TotalByWarehouses,
	TT_Available.Free,
	0,
	0,
	TT_Available.Batch,
	TT_Available.BatchNumber,
	TT_Available.ExpirationDate,
	0,
	0
FROM
	TT_Available AS TT_Available

UNION ALL

SELECT
	TT_ReservesAndBackorders.Products,
	TT_ReservesAndBackorders.Characteristic,
	0,
	0,
	TT_ReservesAndBackorders.Company,
	NULL,
	TT_ReservesAndBackorders.Ownership,
	"TotalInAllWarehouses",
	0,
	0,
	0,
	NULL,
	NULL,
	NULL,
	SUM(TT_ReservesAndBackorders.Reserved),
	SUM(TT_ReservesAndBackorders.Ordered)
FROM
	TT_ReservesAndBackorders AS TT_ReservesAndBackorders

GROUP BY
	TT_ReservesAndBackorders.Company,
	TT_ReservesAndBackorders.Products,
	TT_ReservesAndBackorders.Characteristic,
	TT_ReservesAndBackorders.Ownership</query>
