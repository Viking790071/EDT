		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	PurchaseOrdersBalanceAndTurnovers.Recorder AS RecorderStatement,
	PurchaseOrdersBalanceAndTurnovers.SecondPeriod AS SecondPeriodStatement,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder AS PurchaseOrderStatement,
	PurchaseOrdersBalanceAndTurnovers.Products AS ProductsStatement,
	PurchaseOrdersBalanceAndTurnovers.QuantityOpeningBalance AS QuantityOpeningBalanceStatement,
	PurchaseOrdersBalanceAndTurnovers.QuantityClosingBalance AS QuantityClosingBalanceStatement,
	PurchaseOrdersBalanceAndTurnovers.QuantityReceipt AS QuantityReceiptStatement,
	PurchaseOrdersBalanceAndTurnovers.QuantityExpense AS QuantityExpenseStatement
INTO TempTable_PurchaseOrderStatement
FROM
	AccumulationRegister.PurchaseOrders.BalanceAndTurnovers(, , Auto, RegisterRecords, PurchaseOrder = &amp;Order) AS PurchaseOrdersBalanceAndTurnovers
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TempTable_PurchaseOrderStatement.RecorderStatement AS RecorderStatement,
	TempTable_PurchaseOrderStatement.SecondPeriodStatement AS SecondPeriodStatement,
	TempTable_PurchaseOrderStatement.ProductsStatement AS ProductsStatement,
	TempTable_PurchaseOrderStatement.QuantityOpeningBalanceStatement AS QuantityOpeningBalanceStatement,
	TempTable_PurchaseOrderStatement.QuantityClosingBalanceStatement AS QuantityClosingBalanceStatement,
	TempTable_PurchaseOrderStatement.QuantityReceiptStatement AS QuantityReceiptStatement,
	TempTable_PurchaseOrderStatement.QuantityExpenseStatement AS QuantityExpenseStatement,
	PurchaseOrder.Counterparty AS CounterpartyStatement
FROM
	TempTable_PurchaseOrderStatement AS TempTable_PurchaseOrderStatement
		LEFT JOIN Document.PurchaseOrder AS PurchaseOrder
		ON TempTable_PurchaseOrderStatement.PurchaseOrderStatement = PurchaseOrder.Ref</query>
