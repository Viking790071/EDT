			<query>SELECT ALLOWED
	PaymentCalendar.Quote AS PurchaseOrder,
	PaymentCalendar.AmountTurnover AS AmountTurnover
INTO TemporaryTablePaymentCalendar
FROM
	AccumulationRegister.PaymentCalendar.Turnovers(
			,
			,
			,
			PaymentConfirmationStatus = VALUE(Enum.PaymentApprovalStatuses.Approved)
				AND Quote = &amp;Order) AS PaymentCalendar

UNION ALL

SELECT
	CAST(PaymentCalendar.Quote AS Document.ExpenditureRequest).BasisDocument,
	PaymentCalendar.AmountTurnover
FROM
	AccumulationRegister.PaymentCalendar.Turnovers(
			,
			,
			,
			PaymentConfirmationStatus = VALUE(Enum.PaymentApprovalStatuses.Approved)
				AND CAST(Quote AS Document.ExpenditureRequest).BasisDocument = &amp;Order) AS PaymentCalendar
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	PaymentCalendar.PurchaseOrder AS PurchaseOrder,
	SUM(-PaymentCalendar.AmountTurnover) AS PaymentPlan
INTO TemporaryTablePlannedPayment
FROM
	TemporaryTablePaymentCalendar AS PaymentCalendar

GROUP BY
	PaymentCalendar.PurchaseOrder
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder AS SalesOrder,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder.DocumentCurrency AS DocumentCurrency,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder.OrderState AS OrderState,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder.Counterparty AS Counterparty,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder.Contract AS Contract,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AmountTurnover, 0) AS DocumentAmount,
	ISNULL(Plannedpayment.PaymentPlan, 0) AS PaymentPlan,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) AS Prepayment,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) AS Payment,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) + ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) AS Paid,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) AS LeftToPay
FROM
	AccumulationRegister.PurchaseOrders.BalanceAndTurnovers(, , , , PurchaseOrder = &amp;Order) AS PurchaseOrdersBalanceAndTurnovers
		LEFT JOIN AccumulationRegister.InvoicesAndOrdersPayment.Turnovers(, , , Quote = &amp;Order) AS InvoicesAndOrdersPaymentTurnovers
		ON PurchaseOrdersBalanceAndTurnovers.PurchaseOrder = InvoicesAndOrdersPaymentTurnovers.Quote
		LEFT JOIN TemporaryTablePlannedPayment AS Plannedpayment
		ON PurchaseOrdersBalanceAndTurnovers.PurchaseOrder = Plannedpayment.PurchaseOrder</query>
