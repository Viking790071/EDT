				</v8:item>
				<v8:item>
					<v8:lang>es_ES</v8:lang>
					<v8:content>Pedido</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>es_CO</v8:lang>
					<v8:content>Pedido</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>tr</v8:lang>
					<v8:content>Sipariş edilmiş</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>it</v8:lang>
					<v8:content>Ordinati</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>de</v8:lang>
					<v8:content>Bestellt</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	SalesOrdersBalanceAndTurnovers.Company AS Company,
	SalesOrdersBalanceAndTurnovers.SalesOrder AS SalesOrder,
	SalesOrdersBalanceAndTurnovers.Products AS Products,
	SalesOrdersBalanceAndTurnovers.Characteristic AS Characteristic,
	SalesOrdersBalanceAndTurnovers.SalesOrder.Counterparty AS Counterparty,
	SalesOrdersBalanceAndTurnovers.QuantityReceipt AS Ordered,
	SalesOrdersBalanceAndTurnovers.QuantityExpense AS Shipped,
	SalesOrdersBalanceAndTurnovers.QuantityClosingBalance AS RemainingToShip,
	ISNULL(ReservedProductsBalances.QuantityBalance, 0) AS ReservedAtWarehouse,
	ISNULL(BackordersBalances.QuantityBalance, 0) AS PlacedInOrders,
	SalesOrdersBalanceAndTurnovers.QuantityClosingBalance - ISNULL(ReservedProductsBalances.QuantityBalance, 0) - ISNULL(BackordersBalances.QuantityBalance, 0) AS LeftToProvide,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AmountTurnover, 0) AS DocumentAmount,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) + ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) AS Paid,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) AS LeftToPay,
	CASE
		WHEN SalesOrdersBalanceAndConsolidatedTurnovers.QuantityReceipt = SalesOrdersBalanceAndConsolidatedTurnovers.QuantityClosingBalance
			THEN &amp;TextNotShipped
		WHEN SalesOrdersBalanceAndConsolidatedTurnovers.QuantityClosingBalance &lt;= 0
			THEN &amp;TextFullyShipped
		ELSE &amp;TextPartiallyShipped
	END AS StatusShipment,
	CASE
		WHEN ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) + ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) = 0
			THEN &amp;TextUnpaid
		WHEN ISNULL(InvoicesAndOrdersPaymentTurnovers.AmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) &lt;= 0
			THEN &amp;TextCompletelyPaid
		ELSE &amp;TextPartiallyPaid
	END AS PaymentState
	FROM
	AccumulationRegister.SalesOrders.BalanceAndTurnovers(, , , , ) AS SalesOrdersBalanceAndTurnovers
		LEFT JOIN AccumulationRegister.ReservedProducts.Balance(, SalesOrder &lt;&gt; UNDEFINED) AS ReservedProductsBalances
		ON SalesOrdersBalanceAndTurnovers.Products = ReservedProductsBalances.Products
			AND SalesOrdersBalanceAndTurnovers.Characteristic = ReservedProductsBalances.Characteristic
			AND SalesOrdersBalanceAndTurnovers.SalesOrder = ReservedProductsBalances.SalesOrder
		LEFT JOIN AccumulationRegister.Backorders.Balance(, ) AS BackordersBalances
		ON SalesOrdersBalanceAndTurnovers.Products = BackordersBalances.Products
			AND SalesOrdersBalanceAndTurnovers.Characteristic = BackordersBalances.Characteristic
			AND SalesOrdersBalanceAndTurnovers.SalesOrder = BackordersBalances.SalesOrder
		LEFT JOIN AccumulationRegister.InvoicesAndOrdersPayment.Turnovers(, , , Quote REFS Document.SalesOrder) AS InvoicesAndOrdersPaymentTurnovers
		ON SalesOrdersBalanceAndTurnovers.SalesOrder = InvoicesAndOrdersPaymentTurnovers.Quote
		INNER JOIN AccumulationRegister.SalesOrders.BalanceAndTurnovers(, , , , ) AS SalesOrdersBalanceAndConsolidatedTurnovers
		ON SalesOrdersBalanceAndTurnovers.SalesOrder = SalesOrdersBalanceAndConsolidatedTurnovers.SalesOrder
	WHERE
	(&amp;StatusShipment = "All orders"
			OR &amp;StatusShipment = CASE
				WHEN SalesOrdersBalanceAndConsolidatedTurnovers.QuantityReceipt = SalesOrdersBalanceAndConsolidatedTurnovers.QuantityClosingBalance
					THEN "NotShipped"
				WHEN SalesOrdersBalanceAndConsolidatedTurnovers.QuantityClosingBalance &lt;= 0
					THEN "Fully shipped"
				ELSE "Partially shipped"
			END)
	AND (&amp;PaymentState = "All orders"
			OR &amp;PaymentState = CASE
				WHEN ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) + ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) = 0
					THEN "Unpaid"
				WHEN ISNULL(InvoicesAndOrdersPaymentTurnovers.AmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) &lt;= 0
					THEN "Completely paid"
				ELSE "Partially paid"
				END)</query>
	</dataSet>
