			<query>SELECT ALLOWED
	CashFlowStatementBudgetTurnovers.LineNumber,
	CashFlowStatementBudgetTurnovers.Recorder AS Recorder,
	SECOND(CashFlowStatementBudgetTurnovers.Period) AS SecondPeriod,
	MINUTE(CashFlowStatementBudgetTurnovers.Period) AS MinutePeriod,
	HOUR(CashFlowStatementBudgetTurnovers.Period) AS HourPeriod,
	DAY(CashFlowStatementBudgetTurnovers.Period) AS DayPeriod,
	WEEK(CashFlowStatementBudgetTurnovers.Period) AS WeekPeriod,
	MONTH(CashFlowStatementBudgetTurnovers.Period) AS MonthPeriod,
	QUARTER(CashFlowStatementBudgetTurnovers.Period) AS QuarterPeriod,
	YEAR(CashFlowStatementBudgetTurnovers.Period) AS YearPeriod,
	CashFlowStatementBudgetTurnovers.Company,
	CashFlowStatementBudgetTurnovers.PlanningPeriod,
	CashFlowStatementBudgetTurnovers.Currency,
	CashFlowStatementBudgetTurnovers.GLAccount,
	CashFlowStatementBudgetTurnovers.Item,
	CashFlowStatementBudgetTurnovers.AmountReceipt AS AmountReceiptTurnover,
	CashFlowStatementBudgetTurnovers.AmountReceiptCur AS AmountReceiptCurTurnover,
	CashFlowStatementBudgetTurnovers.AmountExpense AS AmountExpenseTurnover,
	CashFlowStatementBudgetTurnovers.AmountExpenseCur AS AmountExpenseCurTurnover
FROM
	AccumulationRegister.CashBudget AS CashFlowStatementBudgetTurnovers 
			WHERE
	(CashFlowStatementBudgetTurnovers.PlanningPeriod = &amp;PlanningPeriod
			OR &amp;PlanningPeriod = VALUE(Catalog.PlanningPeriods.EmptyRef))
{WHERE
	(CashFlowStatementBudgetTurnovers.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod)}			 </query>
