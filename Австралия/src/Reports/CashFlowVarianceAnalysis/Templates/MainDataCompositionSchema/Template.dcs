			<query>SELECT ALLOWED
	CashAssets.Recorder AS Recorder,
	CashAssets.Company AS Company,
	CashAssets.PaymentMethod AS PaymentMethod,
	CashAssets.BankAccountPettyCash AS BankAccountPettyCash,
	CashAssets.Currency AS Currency,
	CashAssets.Item AS Item,
	CashAssets.AmountCur AS AmountCurReceiptFact,
	CashAssets.Amount AS AmountReceiptFact,
	NULL AS AmountCurExpenseFact,
	NULL AS AmountExpenseFact,
	BEGINOFPERIOD(CashAssets.Period, DAY) AS DayPeriod,
	BEGINOFPERIOD(CashAssets.Period, WEEK) AS WeekPeriod,
	BEGINOFPERIOD(CashAssets.Period, TENDAYS) AS TenDaysPeriod,
	BEGINOFPERIOD(CashAssets.Period, MONTH) AS MonthPeriod,
	BEGINOFPERIOD(CashAssets.Period, QUARTER) AS QuarterPeriod,
	BEGINOFPERIOD(CashAssets.Period, YEAR) AS YearPeriod
FROM
	AccumulationRegister.CashAssets AS CashAssets
WHERE
	CashAssets.RecordType = VALUE(AccumulationRecordType.Receipt)
	AND CashAssets.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod

UNION ALL

SELECT
	CashAssets.Recorder,
	CashAssets.Company,
	CashAssets.PaymentMethod,
	CashAssets.BankAccountPettyCash,
	CashAssets.Currency,
	CashAssets.Item,
	NULL,
	NULL,
	CashAssets.AmountCur,
	CashAssets.Amount,
	BEGINOFPERIOD(CashAssets.Period, DAY),
	BEGINOFPERIOD(CashAssets.Period, WEEK),
	BEGINOFPERIOD(CashAssets.Period, TENDAYS),
	BEGINOFPERIOD(CashAssets.Period, MONTH),
	BEGINOFPERIOD(CashAssets.Period, QUARTER),
	BEGINOFPERIOD(CashAssets.Period, YEAR)
FROM
	AccumulationRegister.CashAssets AS CashAssets
WHERE
	CashAssets.RecordType = VALUE(AccumulationRecordType.Expense)
	AND CashAssets.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod</query>
