		<query>SELECT ALLOWED
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		WHEN AccountsReceivableBalancesAndTurnovers.AmountTurnover = 0
			THEN NULL
		ELSE AccountsReceivableBalancesAndTurnovers.Recorder
	END AS Recorder,
	AccountsReceivableBalancesAndTurnovers.SecondPeriod AS SecondPeriod,
	AccountsReceivableBalancesAndTurnovers.MinutePeriod AS MinutePeriod,
	AccountsReceivableBalancesAndTurnovers.HourPeriod AS HourPeriod,
	AccountsReceivableBalancesAndTurnovers.DayPeriod AS DayPeriod,
	AccountsReceivableBalancesAndTurnovers.WeekPeriod AS WeekPeriod,
	AccountsReceivableBalancesAndTurnovers.TenDaysPeriod AS TenDaysPeriod,
	AccountsReceivableBalancesAndTurnovers.MonthPeriod AS MonthPeriod,
	AccountsReceivableBalancesAndTurnovers.QuarterPeriod AS QuarterPeriod,
	AccountsReceivableBalancesAndTurnovers.HalfYearPeriod AS HalfYearPeriod,
	AccountsReceivableBalancesAndTurnovers.YearPeriod AS YearPeriod,
	AccountsReceivableBalancesAndTurnovers.Company AS Company,
	AccountsReceivableBalancesAndTurnovers.SettlementsType AS SettlementsType,
	AccountsReceivableBalancesAndTurnovers.Counterparty AS Counterparty,
	AccountsReceivableBalancesAndTurnovers.Contract AS Contract,
	AccountsReceivableBalancesAndTurnovers.Contract AS ContractForExternalUser,
	AccountsReceivableBalancesAndTurnovers.Document AS Document,
	AccountsReceivableBalancesAndTurnovers.Order AS Order,
	AccountsReceivableBalancesAndTurnovers.Contract.SettlementsCurrency AS Currency,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountOpeningBalance
		ELSE 0
	END AS AmountOpeningBalanceDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsReceivableBalancesAndTurnovers.AmountOpeningBalance
		ELSE 0
	END AS AmountOpeningBalanceAdvance,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountOpeningBalance
		ELSE 0
	END - CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsReceivableBalancesAndTurnovers.AmountOpeningBalance
		ELSE 0
	END AS AmountOpeningBalanceSettlements,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountClosingBalance
		ELSE 0
	END AS AmountClosingBalanceDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsReceivableBalancesAndTurnovers.AmountClosingBalance
		ELSE 0
	END AS AmountClosingBalanceAdvance,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN CASE
					WHEN AccountsReceivableBalancesAndTurnovers.AmountClosingBalance &gt; AccountsReceivableBalancesAndTurnovers.AmountForPaymentClosingBalance
						THEN AccountsReceivableBalancesAndTurnovers.AmountClosingBalance
					ELSE AccountsReceivableBalancesAndTurnovers.AmountForPaymentClosingBalance
				END
		ELSE 0
	END - CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsReceivableBalancesAndTurnovers.AmountClosingBalance
		ELSE 0
	END AS AmountClosingBalanceSettlements,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountReceipt
		ELSE 0
	END AS AmountReceiptDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsReceivableBalancesAndTurnovers.AmountReceipt
		ELSE 0
	END AS AmountReceiptAdvance,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountTurnover
		ELSE 0
	END AS AmountTurnoverDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsReceivableBalancesAndTurnovers.AmountTurnover
		ELSE 0
	END AS AmountAdvanceTurnover,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountExpense
		ELSE 0
	END AS AmountExpenseDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsReceivableBalancesAndTurnovers.AmountExpense
		ELSE 0
	END AS AmountExpenseAdvance,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountCurOpeningBalance
		ELSE 0
	END AS AmountCurOpeningBalanceDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsReceivableBalancesAndTurnovers.AmountCurOpeningBalance
		ELSE 0
	END AS AmountCurOpeningBalanceAdvance,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountCurOpeningBalance
		ELSE 0
	END - CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsReceivableBalancesAndTurnovers.AmountCurOpeningBalance
		ELSE 0
	END AS AmountCurOpeningBalanceSettlements,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountCurClosingBalance
		ELSE 0
	END AS AmountCurClosingBalanceDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsReceivableBalancesAndTurnovers.AmountCurClosingBalance
		ELSE 0
	END AS AmountCurClosingBalanceAdvance,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN CASE
					WHEN AccountsReceivableBalancesAndTurnovers.AmountCurClosingBalance &gt; AccountsReceivableBalancesAndTurnovers.AmountForPaymentCurClosingBalance
						THEN AccountsReceivableBalancesAndTurnovers.AmountCurClosingBalance
					ELSE AccountsReceivableBalancesAndTurnovers.AmountForPaymentCurClosingBalance
				END
		ELSE 0
	END - CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN -AccountsReceivableBalancesAndTurnovers.AmountCurClosingBalance
		ELSE 0
	END AS AmountCurClosingBalanceCalculations,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountCurTurnover
		ELSE 0
	END AS AmountCurTurnoverDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsReceivableBalancesAndTurnovers.AmountCurTurnover
		ELSE 0
	END AS CurAmountTurnoverAdvance,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountCurReceipt
		ELSE 0
	END AS AmountCurReceiptDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsReceivableBalancesAndTurnovers.AmountCurReceipt
		ELSE 0
	END AS AmountCurReceiptPrepayment,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountCurExpense
		ELSE 0
	END AS AmountCurExpenseDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Advance)
			THEN AccountsReceivableBalancesAndTurnovers.AmountCurExpense
		ELSE 0
	END AS AmountCurExpenseAdvance,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
				AND ISNULL(UsingPaymentTermsInDocuments.UsingPaymentTerms, FALSE)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentOpeningBalance
		ELSE 0
	END AS AmountForPaymentOpeningBalanceDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
				AND ISNULL(UsingPaymentTermsInDocuments.UsingPaymentTerms, FALSE)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentClosingBalance
		ELSE 0
	END AS AmountForPaymentClosingBalanceDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentReceipt
		ELSE 0
	END AS AmountForPaymentReceiptDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentTurnover
		ELSE 0
	END AS AmountForPaymentTurnoverDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentExpense
		ELSE 0
	END AS AmountForPaymentExpenseDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
				AND ISNULL(UsingPaymentTermsInDocuments.UsingPaymentTerms, FALSE)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentCurOpeningBalance
		ELSE 0
	END AS AmountForPaymentCurOpeningBalanceDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
				AND ISNULL(UsingPaymentTermsInDocuments.UsingPaymentTerms, FALSE)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentCurClosingBalance
		ELSE 0
	END AS AmountForPaymentCurClosingBalanceDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentCurReceipt
		ELSE 0
	END AS AmountForPaymentCurReceiptDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentCurTurnover
		ELSE 0
	END AS AmountForPaymentCurTurnoverDebt,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
			THEN AccountsReceivableBalancesAndTurnovers.AmountForPaymentCurExpense
		ELSE 0
	END AS AmountForPaymentCurExpenseDebt
FROM
	AccumulationRegister.AccountsReceivable.BalanceAndTurnovers(, , Auto, , ) AS AccountsReceivableBalancesAndTurnovers
		LEFT JOIN InformationRegister.UsingPaymentTermsInDocuments AS UsingPaymentTermsInDocuments
		ON AccountsReceivableBalancesAndTurnovers.Document = UsingPaymentTermsInDocuments.Document</query>
	</dataSet>
