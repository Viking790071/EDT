SELECT ALLOWED
	TempTable_BalanceAndTurnovers.Recorder AS Recorder,
	TempTable_BalanceAndTurnovers.SecondPeriod AS SecondPeriod,
	TempTable_BalanceAndTurnovers.MinutePeriod AS MinutePeriod,
