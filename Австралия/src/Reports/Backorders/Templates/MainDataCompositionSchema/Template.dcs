		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN BackordersBalancesAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE BackordersBalancesAndTurnovers.Recorder
	END AS Recorder,
	BackordersBalancesAndTurnovers.SecondPeriod AS SecondPeriod,
	BackordersBalancesAndTurnovers.MinutePeriod AS MinutePeriod,
	BackordersBalancesAndTurnovers.HourPeriod AS HourPeriod,
	BackordersBalancesAndTurnovers.DayPeriod AS DayPeriod,
	BackordersBalancesAndTurnovers.WeekPeriod AS WeekPeriod,
	BackordersBalancesAndTurnovers.TenDaysPeriod AS TenDaysPeriod,
	BackordersBalancesAndTurnovers.MonthPeriod AS MonthPeriod,
	BackordersBalancesAndTurnovers.QuarterPeriod AS QuarterPeriod,
	BackordersBalancesAndTurnovers.HalfYearPeriod AS HalfYearPeriod,
	BackordersBalancesAndTurnovers.YearPeriod AS YearPeriod,
	BackordersBalancesAndTurnovers.Company AS Company,
	BackordersBalancesAndTurnovers.SalesOrder AS SalesOrder,
	BackordersBalancesAndTurnovers.Products AS Products,
	BackordersBalancesAndTurnovers.Characteristic AS Characteristic,
	BackordersBalancesAndTurnovers.SupplySource AS SupplySource,
	BackordersBalancesAndTurnovers.QuantityOpeningBalance AS QuantityOpeningBalance,
	BackordersBalancesAndTurnovers.QuantityClosingBalance AS QuantityClosingBalance,
	BackordersBalancesAndTurnovers.QuantityTurnover AS QuantityTurnover,
	BackordersBalancesAndTurnovers.QuantityReceipt AS QuantityReceipt,
	BackordersBalancesAndTurnovers.QuantityExpense AS QuantityExpense
INTO PrepTempTable_BalanceAndTurnovers
FROM
	AccumulationRegister.Backorders.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS BackordersBalancesAndTurnovers
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	PrepTempTable_BalanceAndTurnovers.Recorder AS Recorder,
	PrepTempTable_BalanceAndTurnovers.SecondPeriod AS SecondPeriod,
	PrepTempTable_BalanceAndTurnovers.MinutePeriod AS MinutePeriod,
	PrepTempTable_BalanceAndTurnovers.HourPeriod AS HourPeriod,
	PrepTempTable_BalanceAndTurnovers.DayPeriod AS DayPeriod,
	PrepTempTable_BalanceAndTurnovers.WeekPeriod AS WeekPeriod,
	PrepTempTable_BalanceAndTurnovers.TenDaysPeriod AS TenDaysPeriod,
	PrepTempTable_BalanceAndTurnovers.MonthPeriod AS MonthPeriod,
	PrepTempTable_BalanceAndTurnovers.QuarterPeriod AS QuarterPeriod,
	PrepTempTable_BalanceAndTurnovers.HalfYearPeriod AS HalfYearPeriod,
	PrepTempTable_BalanceAndTurnovers.YearPeriod AS YearPeriod,
	PrepTempTable_BalanceAndTurnovers.Company AS Company,
	PrepTempTable_BalanceAndTurnovers.SalesOrder AS SalesOrder,
	PrepTempTable_BalanceAndTurnovers.Products AS Products,
	PrepTempTable_BalanceAndTurnovers.Characteristic AS Characteristic,
	PrepTempTable_BalanceAndTurnovers.SupplySource AS SupplySource,
	PrepTempTable_BalanceAndTurnovers.QuantityOpeningBalance AS QuantityOpeningBalance,
	PrepTempTable_BalanceAndTurnovers.QuantityClosingBalance AS QuantityClosingBalance,
	PrepTempTable_BalanceAndTurnovers.QuantityTurnover AS QuantityTurnover,
	PrepTempTable_BalanceAndTurnovers.QuantityReceipt AS QuantityReceipt,
	PrepTempTable_BalanceAndTurnovers.QuantityExpense AS QuantityExpense,
	CASE
		WHEN CatalogProducts.ConversionRate = 0
			THEN PrepTempTable_BalanceAndTurnovers.QuantityOpeningBalance
		ELSE PrepTempTable_BalanceAndTurnovers.QuantityOpeningBalance / CatalogProducts.ConversionRate
	END AS ReportOpeningBalance,
	CASE
		WHEN CatalogProducts.ConversionRate = 0
			THEN PrepTempTable_BalanceAndTurnovers.QuantityClosingBalance
		ELSE PrepTempTable_BalanceAndTurnovers.QuantityClosingBalance / CatalogProducts.ConversionRate
	END AS ReportClosingBalance,
	CASE
		WHEN CatalogProducts.ConversionRate = 0
			THEN PrepTempTable_BalanceAndTurnovers.QuantityTurnover
		ELSE PrepTempTable_BalanceAndTurnovers.QuantityTurnover / CatalogProducts.ConversionRate
	END AS ReportTurnover,
	CASE
		WHEN CatalogProducts.ConversionRate = 0
			THEN PrepTempTable_BalanceAndTurnovers.QuantityReceipt
		ELSE PrepTempTable_BalanceAndTurnovers.QuantityReceipt / CatalogProducts.ConversionRate
	END AS ReportReceipt,
	CASE
		WHEN CatalogProducts.ConversionRate = 0
			THEN PrepTempTable_BalanceAndTurnovers.QuantityExpense
		ELSE PrepTempTable_BalanceAndTurnovers.QuantityExpense / CatalogProducts.ConversionRate
	END AS ReportExpense,
	CatalogProducts.MeasurementUnit AS MeasurementUnit,
	CatalogProducts.ReportUOM AS ReportUOM,
	ISNULL(DocumentSalesOrder.Counterparty, DocumentWorkOrder.Counterparty) AS Counterparty,
	ISNULL(DocumentSalesOrder.Contract, DocumentWorkOrder.Contract) AS Contract,
	ISNULL(DocumentSalesOrder.Project, DocumentWorkOrder.Project) AS Project,
	ISNULL(DocumentSalesOrder.OrderState, DocumentWorkOrder.OrderState) AS OrderState,
	ISNULL(DocumentSalesOrder.Closed, DocumentWorkOrder.Closed) AS Closed
INTO TempTable_BalanceAndTurnovers
FROM
	PrepTempTable_BalanceAndTurnovers AS PrepTempTable_BalanceAndTurnovers
		LEFT JOIN Catalog.Products AS CatalogProducts
		ON PrepTempTable_BalanceAndTurnovers.Products = CatalogProducts.Ref
		LEFT JOIN Document.SalesOrder AS DocumentSalesOrder
		ON PrepTempTable_BalanceAndTurnovers.SalesOrder = DocumentSalesOrder.Ref
		LEFT JOIN Document.WorkOrder AS DocumentWorkOrder
		ON PrepTempTable_BalanceAndTurnovers.SalesOrder = DocumentWorkOrder.Ref
;

