	TempTable_BalanceAndTurnovers.Products AS Products,
	TempTable_BalanceAndTurnovers.Characteristic AS Characteristic,
	TempTable_BalanceAndTurnovers.SupplySource AS SupplySource,
	TempTable_BalanceAndTurnovers.QuantityOpeningBalance AS QuantityOpeningBalance,
	TempTable_BalanceAndTurnovers.QuantityClosingBalance AS QuantityClosingBalance,
	TempTable_BalanceAndTurnovers.QuantityTurnover AS QuantityTurnover,
	TempTable_BalanceAndTurnovers.QuantityReceipt AS QuantityReceipt,
	TempTable_BalanceAndTurnovers.QuantityExpense AS QuantityExpense,
	TempTable_BalanceAndTurnovers.ReportOpeningBalance AS ReportOpeningBalance,
	TempTable_BalanceAndTurnovers.ReportClosingBalance AS ReportClosingBalance,
	TempTable_BalanceAndTurnovers.ReportTurnover AS ReportTurnover,
	TempTable_BalanceAndTurnovers.ReportReceipt AS ReportReceipt,
	TempTable_BalanceAndTurnovers.ReportExpense AS ReportExpense,
	TempTable_BalanceAndTurnovers.MeasurementUnit AS MeasurementUnit,
	TempTable_BalanceAndTurnovers.ReportUOM AS ReportUOM,
	TempTable_BalanceAndTurnovers.Counterparty AS Counterparty,
	TempTable_BalanceAndTurnovers.Contract AS Contract,
	TempTable_BalanceAndTurnovers.Project AS Project,
	TempTable_BalanceAndTurnovers.OrderState AS OrderState,
	TempTable_BalanceAndTurnovers.Closed AS Closed,
	ISNULL(SalesOrderStatuses.OrderStatus, WorkOrderStatuses.OrderStatus) AS OrderStatus
FROM
	TempTable_BalanceAndTurnovers AS TempTable_BalanceAndTurnovers
