		ON TempTable_BalanceAndTurnovers.OrderState = SalesOrderStatuses.Ref
		LEFT JOIN Catalog.WorkOrderStatuses AS WorkOrderStatuses
		ON TempTable_BalanceAndTurnovers.OrderState = WorkOrderStatuses.Ref</query>
