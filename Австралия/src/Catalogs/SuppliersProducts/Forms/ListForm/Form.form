    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT ALLOWED
	CatalogSuppliersProducts.Ref AS Ref,
	CatalogSuppliersProducts.DeletionMark AS DeletionMark,
	CatalogSuppliersProducts.Owner AS Owner,
	CatalogSuppliersProducts.Code AS Code,
	CatalogSuppliersProducts.Description AS Description,
	CatalogSuppliersProducts.Products AS Products,
	CatalogSuppliersProducts.Characteristic AS Characteristic,
	CatalogSuppliersProducts.SKU AS SKU,
	CatalogSuppliersProducts.DescriptionLanguage1 AS DescriptionLanguage1,
	CatalogSuppliersProducts.DescriptionLanguage2 AS DescriptionLanguage2,
	CatalogSuppliersProducts.DescriptionLanguage3 AS DescriptionLanguage3,
	CatalogSuppliersProducts.DescriptionLanguage4 AS DescriptionLanguage4,
	CatalogSuppliersProducts.AdditionalAttributes.(
		Ref AS Ref,
		LineNumber AS LineNumber,
		Property AS Property,
		Value AS Value,
		TextString AS TextString
	) AS AdditionalAttributes,
	CatalogSuppliersProducts.Predefined AS Predefined,
	CatalogSuppliersProducts.PredefinedDataName AS PredefinedDataName,
	ISNULL(CatalogProducts.ProductCrossReference, VALUE(Catalog.Products.EmptyRef)) = CatalogSuppliersProducts.Ref
		AND ISNULL(CatalogProducts.UseDefaultCrossReference, FALSE) AS IsDefault
FROM
	Catalog.SuppliersProducts AS CatalogSuppliersProducts
		LEFT JOIN Catalog.Products AS CatalogProducts
		ON CatalogSuppliersProducts.Products = CatalogProducts.Ref</queryText>
      <mainTable>Catalog.SuppliersProducts</mainTable>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
