    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CatalogBillsOfMaterials.Ref AS Ref,
	CatalogBillsOfMaterials.DeletionMark AS DeletionMark,
	CatalogBillsOfMaterials.Owner AS Owner,
	CatalogBillsOfMaterials.Code AS Code,
	CatalogBillsOfMaterials.Description AS Description,
	CatalogBillsOfMaterials.ProductCharacteristic AS ProductCharacteristic,
	CatalogBillsOfMaterials.NormalSpoilage AS NormalSpoilage,
	CatalogBillsOfMaterials.Quantity AS Quantity,
	CatalogBillsOfMaterials.UseRouting AS UseRouting,
	CatalogBillsOfMaterials.PlannedCost AS PlannedCost,
	CatalogBillsOfMaterials.Comment AS Comment,
	CatalogBillsOfMaterials.Status AS Status,
	CatalogBillsOfMaterials.ValidityStartDate AS ValidityStartDate,
	CatalogBillsOfMaterials.ValidityEndDate AS ValidityEndDate,
	CatalogBillsOfMaterials.OperationKind AS OperationKind,
	CatalogBillsOfMaterials.CommentLanguage1 AS CommentLanguage1,
	CatalogBillsOfMaterials.CommentLanguage2 AS CommentLanguage2,
	CatalogBillsOfMaterials.CommentLanguage3 AS CommentLanguage3,
	CatalogBillsOfMaterials.CommentLanguage4 AS CommentLanguage4,
	CatalogBillsOfMaterials.DescriptionLanguage1 AS DescriptionLanguage1,
	CatalogBillsOfMaterials.DescriptionLanguage2 AS DescriptionLanguage2,
	CatalogBillsOfMaterials.DescriptionLanguage3 AS DescriptionLanguage3,
	CatalogBillsOfMaterials.DescriptionLanguage4 AS DescriptionLanguage4,
	CatalogBillsOfMaterials.Predefined AS Predefined,
	CatalogBillsOfMaterials.PredefinedDataName AS PredefinedDataName,
	Products.Specification = CatalogBillsOfMaterials.Ref AS IsDefault,
	CatalogBillsOfMaterials.BillNumber AS BillNumber
FROM
	Catalog.BillsOfMaterials AS CatalogBillsOfMaterials
		LEFT JOIN Catalog.Products AS Products
		ON CatalogBillsOfMaterials.Owner = Products.Ref</queryText>
      <mainTable>Catalog.BillsOfMaterials</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
  </attributes>
