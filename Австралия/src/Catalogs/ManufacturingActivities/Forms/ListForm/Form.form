    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	ManufacturingActivities.Ref AS Ref
INTO TempTable_NestedSelect
FROM
	Catalog.ManufacturingActivities AS ManufacturingActivities
		LEFT JOIN Catalog.ManufacturingActivities.WorkCenterTypes AS ManufacturingActivitiesWorkCenterTypes
			INNER JOIN Catalog.CompanyResourceTypes AS CompanyResourceTypes
			ON ManufacturingActivitiesWorkCenterTypes.WorkcenterType = CompanyResourceTypes.Ref
		ON (ManufacturingActivitiesWorkCenterTypes.Ref = ManufacturingActivities.Ref)
WHERE
	&amp;UseBusinessUnitFilter
	AND (CompanyResourceTypes.BusinessUnit = &amp;BusinessUnit
			OR CompanyResourceTypes.BusinessUnit IS NULL)

GROUP BY
	ManufacturingActivities.Ref

INDEX BY
	Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	CatalogManufacturingActivities.Ref AS Ref,
	CatalogManufacturingActivities.Description AS Description,
	CatalogManufacturingActivities.StandardWorkload AS StandardWorkload,
	CatalogManufacturingActivities.CostPool AS CostPool,
	CatalogManufacturingActivities.TimeUOM AS TimeUOM,
	CatalogManufacturingActivities.StandardTimeInUOM AS StandardTimeInUOM,
	CatalogManufacturingActivities.OperationNumber AS OperationNumber,
	CatalogManufacturingActivities.Code AS Code
FROM
	Catalog.ManufacturingActivities AS CatalogManufacturingActivities
WHERE
	NOT &amp;UseBusinessUnitFilter

UNION ALL

SELECT
	CatalogManufacturingActivities.Ref,
	CatalogManufacturingActivities.Description,
	CatalogManufacturingActivities.StandardWorkload,
	CatalogManufacturingActivities.CostPool,
	CatalogManufacturingActivities.TimeUOM,
	CatalogManufacturingActivities.StandardTimeInUOM,
