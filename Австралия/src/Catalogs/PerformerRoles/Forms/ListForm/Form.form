    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CatalogPerformersRolesOverridable.Ref,
	CatalogPerformersRolesOverridable.DeletionMark,
	CatalogPerformersRolesOverridable.Predefined,
	CatalogPerformersRolesOverridable.Code,
	CatalogPerformersRolesOverridable.Description,
	CatalogPerformersRolesOverridable.UsedWithoutAddressingObjects,
	CatalogPerformersRolesOverridable.UsedByAddressingObjects,
	CatalogPerformersRolesOverridable.MainAddressingObjectTypes,
	CatalogPerformersRolesOverridable.AdditionalAddressingObjectTypes,
	CatalogPerformersRolesOverridable.Comment,
	CASE
		WHEN CatalogPerformersRolesOverridable.UsedByAddressingObjects
			THEN TRUE
		WHEN CatalogPerformersRolesOverridable.Ref IN
				(SELECT TOP 1
					InformationRegister.TaskPerformers.PerformerRole.Ref
				FROM
					InformationRegister.TaskPerformers
				WHERE
					InformationRegister.TaskPerformers.PerformerRole = CatalogPerformersRolesOverridable.Ref)
			THEN TRUE
		ELSE FALSE
	END AS HasPerformers,
	CatalogPerformersRolesOverridable.ExternalRole,
	CatalogPerformersRolesOverridable.BriefPresentation
FROM
	Catalog.PerformerRoles.Purpose AS PerformerRolesAssignmentOverridable
		LEFT JOIN Catalog.PerformerRoles AS CatalogPerformersRolesOverridable
		ON PerformerRolesAssignmentOverridable.Ref = CatalogPerformersRolesOverridable.Ref
WHERE
	PerformerRolesAssignmentOverridable.UsersType = Value(Catalog.Users.EmptyRef)</queryText>
      <mainTable>Catalog.PerformerRoles</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
