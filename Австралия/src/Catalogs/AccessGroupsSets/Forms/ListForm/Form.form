    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CatalogAccessGroupsSets.Ref AS Ref,
	CatalogAccessGroupsSets.DeletionMark AS DeletionMark,
	CatalogAccessGroupsSets.Description AS Description,
	CatalogAccessGroupsSets.ForExternalUsers AS ForExternalUsers,
	CatalogAccessGroupsSets.SetItemsType AS SetItemsType,
	VALUETYPE(CatalogAccessGroupsSets.SetItemsType) AS ItemsType,
	CatalogAccessGroupsSets.NotUsedSince AS NotUsedSince,
	CatalogAccessGroupsSets.User AS User,
	ISNULL(GroupsCountInSets.Count, 0) AS GroupsInSetCount,
	CASE
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.AccessGroups)
			THEN 1
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.UserGroups)
			THEN 2
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.Users)
			THEN 3
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.ExternalUsersGroups)
			THEN 4
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.ExternalUsers)
			THEN 5
		ELSE 0
	END AS OrderByType,
	CatalogAccessGroupsSets.Predefined AS Predefined,
	CatalogAccessGroupsSets.PredefinedDataName AS PredefinedDataName
FROM
	Catalog.AccessGroupsSets AS CatalogAccessGroupsSets
		LEFT JOIN (SELECT
			Folders.Ref AS Set,
			COUNT(Folders.Ref) AS Count
		FROM
			Catalog.AccessGroupsSets.Folders AS Folders
		
		GROUP BY
			Folders.Ref) AS GroupsCountInSets
		ON (GroupsCountInSets.Set = CatalogAccessGroupsSets.Ref)</queryText>
      <mainTable>Catalog.AccessGroupsSets</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
