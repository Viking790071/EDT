    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT ALLOWED
	CatalogMessagesTemplates.Ref,
	CatalogMessagesTemplates.Description,
	CASE
		WHEN CatalogMessagesTemplates.ForSMSMessages
			THEN &amp;SMSMessage
		ELSE &amp;Email
	END AS TemplateFor,
	CASE
		WHEN CatalogMessagesTemplates.InputOnBasisParameterTypeFullName = &quot;&quot;
			THEN &amp;Common
		ELSE CatalogMessagesTemplates.InputOnBasisParameterTypeFullName
	END AS Purpose,
	CatalogMessagesTemplates.TemplateOwner AS TemplateOwner,
	0 HasFiles
FROM
	Catalog.MessageTemplates AS CatalogMessagesTemplates
WHERE
	(&amp;Purpose = &quot;&quot;
		OR CatalogMessagesTemplates.Purpose = &amp;Purpose)
	AND (CatalogMessagesTemplates.TemplateOwner = Undefined
		OR CatalogMessagesTemplates.TemplateOwner = VALUE(Catalog.MetadataObjectIDs.EmptyRef)
		OR &amp;ShowContextTemplates)</queryText>
      <mainTable>Catalog.MessageTemplates</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
