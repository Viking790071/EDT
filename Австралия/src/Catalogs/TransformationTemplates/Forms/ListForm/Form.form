    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CatalogTransformationTemplates.Ref AS Ref,
	CatalogTransformationTemplates.DeletionMark AS DeletionMark,
	CatalogTransformationTemplates.Parent AS Parent,
	CatalogTransformationTemplates.IsFolder AS IsFolder,
	CatalogTransformationTemplates.Description AS Description,
	CatalogTransformationTemplates.Company AS Company,
	SourceMetadataObjectIDs.Synonym AS SourceChartOfAccountsName,
	ReceivingMetadataObjectIDs.Synonym AS ReceivingChartOfAccountsName,
	CatalogTransformationTemplates.SourceChartOfAccounts AS SourceChartOfAccounts,
	CatalogTransformationTemplates.ReceivingChartOfAccounts AS ReceivingChartOfAccounts
FROM
	Catalog.TransformationTemplates AS CatalogTransformationTemplates
		LEFT JOIN Catalog.MetadataObjectIDs AS ReceivingMetadataObjectIDs
		ON CatalogTransformationTemplates.ReceivingChartOfAccounts = ReceivingMetadataObjectIDs.Ref
		LEFT JOIN Catalog.MetadataObjectIDs AS SourceMetadataObjectIDs
		ON CatalogTransformationTemplates.SourceChartOfAccounts = SourceMetadataObjectIDs.Ref</queryText>
      <mainTable>Catalog.TransformationTemplates</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
  </attributes>
