    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.WorkInProgress</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	ProductionTask.Ref AS Ref,
	ProductionTask.Status AS Status,
	ProductionTask.Operation AS Operation,
	ProductionTask.WorkcenterType AS WorkcenterType,
	ProductionTask.Workcenter AS Workcenter,
	ProductionTask.Assignee AS Assignee,
	ProductionTask.StandardTimeInUOM AS StandardTimeInUOM,
	ProductionTask.TimeUOM AS TimeUOM,
	ManufacturingOperation.Products AS Products,
	ManufacturingOperation.Characteristic AS Characteristic,
	DocProductionOrder.Priority AS Priority,
	ProductionTask.StartDatePlanned AS StartDatePlanned,
	ProductionTask.EndDatePlanned AS EndDatePlanned,
	ManufacturingOperation.Ref AS WorkInProgress,
	DocProductionOrder.Ref AS ProductionOrder,
	ProductionTask.StructuralUnit AS StructuralUnit,
	ProductionTask.ComponentsList AS ComponentsList,
	ProductionTask.StartDate AS StartDate,
	ProductionTask.EndDate AS EndDate,
	ProductionTask.Date AS Date,
	ProductionTask.Number AS Number,
	ProductionTask.Posted AS Posted,
	ManufacturingOperation.ProductsList AS ProductsList,
	ProductionOperationsSequence.CanBeStarted AS CanBeStarted,
	ProductionTask.Comment AS Comment,
	ProductionTask.OperationQuantity AS OperationQuantity
FROM
	Document.ProductionTask AS ProductionTask
		LEFT JOIN Document.ManufacturingOperation AS ManufacturingOperation
			LEFT JOIN Document.ProductionOrder AS DocProductionOrder
			ON ManufacturingOperation.BasisDocument = DocProductionOrder.Ref
		ON ProductionTask.BasisDocument = ManufacturingOperation.Ref
		LEFT JOIN InformationRegister.ProductionOperationsSequence AS ProductionOperationsSequence
		ON ProductionTask.BasisDocument = ProductionOperationsSequence.WorkInProgress
			AND ProductionTask.Operation = ProductionOperationsSequence.Operation
			AND ProductionTask.ConnectionKey = ProductionOperationsSequence.ConnectionKey</queryText>
      <mainTable>Document.ProductionTask</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
