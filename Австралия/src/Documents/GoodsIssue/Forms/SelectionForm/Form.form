    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DocumentGoodsIssue.Ref AS Ref,
	DocumentGoodsIssue.DeletionMark AS DeletionMark,
	DocumentGoodsIssue.Number AS Number,
	DocumentGoodsIssue.Date AS Date,
	DocumentGoodsIssue.Posted AS Posted,
	DocumentGoodsIssue.Company AS Company,
	CASE
		WHEN DocumentGoodsIssue.OperationType = VALUE(Enum.OperationTypesGoodsIssue.IntraCommunityTransfer)
			THEN DocumentGoodsIssue.Company
		ELSE DocumentGoodsIssue.Counterparty
	END AS Counterparty,
	DocumentGoodsIssue.Department AS Department,
	DocumentGoodsIssue.Author AS Author,
	DocumentGoodsIssue.Responsible AS Responsible,
	DocumentGoodsIssue.Contract AS Contract,
	DocumentGoodsIssue.Order AS Order,
	DocumentGoodsIssue.SalesOrderPosition AS SalesOrderPosition,
	DocumentGoodsIssue.StructuralUnit AS StructuralUnit,
	DocumentGoodsIssue.Cell AS Cell,
	DocumentGoodsIssue.Comment AS Comment,
	DocumentGoodsIssue.PointInTime AS PointInTime,
	DocumentGoodsIssue.OperationType AS OperationType,
	CASE
		WHEN DocumentGoodsIssue.OperationType &lt;> VALUE(Enum.OperationTypesGoodsIssue.SaleToCustomer)
			THEN VALUE(Enum.GoodsDocumentStatuses.NotApplicable)
		WHEN DocumentGoodsIssue.Posted
			THEN ISNULL(GoodsDocumentsStatuses.Status, VALUE(Enum.GoodsDocumentStatuses.NotInvoiced))
		ELSE VALUE(Enum.GoodsDocumentStatuses.Draft)
	END AS Status
FROM
	Document.GoodsIssue AS DocumentGoodsIssue
		LEFT JOIN InformationRegister.GoodsDocumentsStatuses AS GoodsDocumentsStatuses
		ON (GoodsDocumentsStatuses.Document = DocumentGoodsIssue.Ref)
WHERE
	(&amp;OrderFilter = UNDEFINED
			OR DocumentGoodsIssue.Products.Order = &amp;OrderFilter
			OR DocumentGoodsIssue.Order = &amp;OrderFilter)</queryText>
