    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT ALLOWED
	DocumentReconciliationStatement.Ref AS Ref,
	DocumentReconciliationStatement.DeletionMark AS DeletionMark,
	DocumentReconciliationStatement.Number AS Number,
	DocumentReconciliationStatement.Date AS Date,
	DocumentReconciliationStatement.Posted AS Posted,
	DocumentReconciliationStatement.Company AS Company,
	DocumentReconciliationStatement.Counterparty AS Counterparty,
	DocumentReconciliationStatement.BeginOfPeriod AS BeginOfPeriod,
	DocumentReconciliationStatement.EndOfPeriod AS EndOfPeriod,
	DocumentReconciliationStatement.Status AS Status,
	DocumentReconciliationStatement.Responsible AS Responsible,
	DocumentReconciliationStatement.Comment AS Comment,
	DocumentReconciliationStatement.CounterpartyRepresentative AS CounterpartyRepresentative,
	DocumentReconciliationStatement.DocumentCurrency AS DocumentCurrency,
	DocumentReconciliationStatement.DescriptionContractsSelection AS DescriptionContractsSelection,
	DocumentReconciliationStatement.BalanceBeginPeriod AS BalanceBeginPeriod,
	DocumentReconciliationStatement.Author AS Author,
	DocumentReconciliationStatement.DocumentDescription AS DocumentDescription,
	DocumentReconciliationStatement.Contract AS Contract,
	DocumentReconciliationStatement.CompanyVATNumber AS CompanyVATNumber
FROM
	Document.ReconciliationStatement AS DocumentReconciliationStatement
		LEFT JOIN Catalog.CounterpartyContracts AS CounterpartyContracts
		ON DocumentReconciliationStatement.Contract = CounterpartyContracts.Ref
		LEFT JOIN Catalog.Counterparties AS Counterparties
		ON DocumentReconciliationStatement.Counterparty = Counterparties.Ref
WHERE
	DocumentReconciliationStatement.Counterparty = &amp;Counterparty
	AND NOT DocumentReconciliationStatement.DeletionMark
	AND CASE
			WHEN &amp;UseContractRestrictionsTurnOff
				THEN TRUE
			WHEN Counterparties.DoOperationsByContracts
					AND CounterpartyContracts.VisibleToExternalUsers
				THEN TRUE
			WHEN NOT Counterparties.DoOperationsByContracts
				THEN TRUE
			ELSE FALSE
		END</queryText>
      <mainTable>Document.ReconciliationStatement</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
