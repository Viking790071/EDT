    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DocumentOnlinePayment.Ref AS Ref,
	DocumentOnlinePayment.DeletionMark AS DeletionMark,
	DocumentOnlinePayment.Number AS Number,
	DocumentOnlinePayment.Date AS Date,
	DocumentOnlinePayment.Posted AS Posted,
	DocumentOnlinePayment.Company AS Company,
	DocumentOnlinePayment.CompanyVATNumber AS CompanyVATNumber,
	DocumentOnlinePayment.Comment AS Comment,
	DocumentOnlinePayment.Counterparty AS Counterparty,
	DocumentOnlinePayment.BasisDocument AS BasisDocument,
	DocumentOnlinePayment.OperationKind AS OperationKind,
	DocumentOnlinePayment.POSTerminal AS POSTerminal,
	DocumentOnlinePayment.CashCurrency AS CashCurrency,
	DocumentOnlinePayment.DocumentAmount + CASE
		WHEN ISNULL(POSTerminals.WithholdFeeOnPayout, FALSE)
			THEN 0
		ELSE DocumentOnlinePayment.FeeTotal
	END AS DocumentAmount,
	DocumentOnlinePayment.ChargeCardKind AS ChargeCardKind,
	DocumentOnlinePayment.ChargeCardNo AS ChargeCardNo,
	DocumentOnlinePayment.PaymentStatus AS PaymentStatus,
	DocumentOnlinePayment.FeePercent AS FeePercent,
	DocumentOnlinePayment.FeeAmount AS FeeAmount,
	DocumentOnlinePayment.FeeFixedPart AS FeeFixedPart,
	DocumentOnlinePayment.FeeTotal AS FeeTotal,
	DocumentOnlinePayment.Author AS Author,
	DocumentOnlinePayment.VATTaxation AS VATTaxation,
	DocumentOnlinePayment.PaymentDetails.(
		Ref AS Ref,
		LineNumber AS LineNumber,
		Contract AS Contract,
		AdvanceFlag AS AdvanceFlag,
		Document AS Document,
		SettlementsAmount AS SettlementsAmount,
		ExchangeRate AS ExchangeRate,
		Multiplicity AS Multiplicity,
		PaymentAmount AS PaymentAmount,
		VATRate AS VATRate,
		VATAmount AS VATAmount,
		PaymentDetails.Order AS Order,
		Item AS Item,
		AccountsReceivableGLAccount AS AccountsReceivableGLAccount,
		AdvancesReceivedGLAccount AS AdvancesReceivedGLAccount
	) AS PaymentDetails,
	DocumentOnlinePayment.AdditionalAttributes.(
		Ref AS Ref,
		LineNumber AS LineNumber,
		Property AS Property,
		Value AS Value,
		TextString AS TextString
	) AS AdditionalAttributes,
	DocumentOnlinePayment.PointInTime AS PointInTime
FROM
	Document.OnlinePayment AS DocumentOnlinePayment
		LEFT JOIN Catalog.POSTerminals AS POSTerminals
		ON DocumentOnlinePayment.POSTerminal = POSTerminals.Ref</queryText>
      <mainTable>Document.OnlinePayment</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
