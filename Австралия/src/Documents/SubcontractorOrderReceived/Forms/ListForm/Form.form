  <attributes>
    <name>List</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Closed</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Counterparty</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.OrderState</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.OrderStatus</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CASE
		WHEN DocumentSubcontractorOrder.Posted
				AND DocumentSubcontractorOrder.OrderState.OrderStatus &lt;> VALUE(Enum.OrderStatuses.Open)
			THEN CASE
					WHEN RunSchedule.Order IS NULL
							AND TimeSchedulePlan.Order IS NULL
						THEN CASE
								WHEN DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
										AND DocumentSubcontractorOrder.Closed
									THEN 2
								ELSE 0
							END
					ELSE CASE
							WHEN ISNULL(RunSchedule.Completed, 0) = 0
									AND ISNULL(TimeSchedulePlan.Completed, 0) = 0
								THEN CASE
										WHEN DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
												AND NOT DocumentSubcontractorOrder.Closed
											THEN CASE
													WHEN NOT RunSchedule.Order IS NULL
															AND RunSchedule.Period &lt; &amp;CurrentDateSession
														THEN 6
													WHEN NOT RunSchedule.Order IS NULL
															AND RunSchedule.Period = &amp;CurrentDateSession
														THEN 4
													ELSE 2
												END
										ELSE 2
									END
							ELSE CASE
									WHEN DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
											AND NOT DocumentSubcontractorOrder.Closed
										THEN CASE
												WHEN NOT RunSchedule.Order IS NULL
														AND RunSchedule.Period &lt; &amp;CurrentDateSession
													THEN 5
												WHEN NOT RunSchedule.Order IS NULL
														AND RunSchedule.Period = &amp;CurrentDateSession
													THEN 3
												ELSE 1
											END
									ELSE 1
								END
						END
				END
		ELSE 2
	END AS ShipmentPictureNumber,
	CASE
		WHEN SubcontractorInvoiceIssued.Ref IS NULL
			THEN 0
		ELSE 1
	END AS InvoicedPictureNumber,
	CASE
		WHEN DocumentSubcontractorOrder.Posted
			THEN CASE
					WHEN (DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
							OR ISNULL(PaymentEvent.Amount, 0) &lt;> 0)
							AND ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0) = 0
						THEN 2
					WHEN ISNULL(PaymentEvent.Amount, 0) &lt;= ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0)
						THEN CASE
								WHEN DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
											AND DocumentSubcontractorOrder.Closed
										OR PaymentEvent.Amount IS NULL
									THEN 2
								ELSE 0
							END
					ELSE 1
				END
		ELSE 2
	END AS PaymentPictureNumber,
	CASE
		WHEN DocumentSubcontractorOrder.Posted
				AND DocumentSubcontractorOrder.DocumentAmount > 0
			THEN CAST((ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0)) * 100 / DocumentSubcontractorOrder.DocumentAmount AS NUMBER(15, 2))
		ELSE 0
	END AS PaymentPercentage,
	CASE
		WHEN DocumentSubcontractorOrder.Posted
				AND NOT DocumentSubcontractorOrder.Closed
				AND DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
				AND NOT RunSchedule.Order IS NULL
				AND RunSchedule.Period &lt; &amp;CurrentDateSession
			THEN TRUE
		ELSE FALSE
	END AS PastPerformance,
	FALSE AS OverduePayment,
	CASE
		WHEN DocumentSubcontractorOrder.Posted
				AND NOT DocumentSubcontractorOrder.Closed
				AND DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
				AND NOT RunSchedule.Order IS NULL
				AND RunSchedule.Period = &amp;CurrentDateSession
			THEN TRUE
		ELSE FALSE
	END AS ForToday,
	CASE
		WHEN DocumentSubcontractorOrder.Posted
				AND NOT DocumentSubcontractorOrder.Closed
				AND DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
			THEN TRUE
		ELSE FALSE
	END AS OrderInProcess,
	DocumentSubcontractorOrder.Ref AS Ref,
	DocumentSubcontractorOrder.DeletionMark AS DeletionMark,
	DocumentSubcontractorOrder.Number AS Number,
	DocumentSubcontractorOrder.Date AS Date,
	DocumentSubcontractorOrder.Posted AS Posted,
	DocumentSubcontractorOrder.Company AS Company,
	DocumentSubcontractorOrder.Counterparty AS Counterparty,
	DocumentSubcontractorOrder.Contract AS Contract,
	DocumentSubcontractorOrder.DateRequired AS DateRequired,
	DocumentSubcontractorOrder.DocumentCurrency AS DocumentCurrency,
	DocumentSubcontractorOrder.AmountIncludesVAT AS AmountIncludesVAT,
	DocumentSubcontractorOrder.IncludeVATInPrice AS IncludeVATInPrice,
	DocumentSubcontractorOrder.ExchangeRate AS ExchangeRate,
	DocumentSubcontractorOrder.Multiplicity AS Multiplicity,
	DocumentSubcontractorOrder.StructuralUnit AS StructuralUnit,
	DocumentSubcontractorOrder.DocumentAmount AS DocumentAmount,
	DocumentSubcontractorOrder.Responsible AS Responsible,
	DocumentSubcontractorOrder.Comment AS Comment,
	DocumentSubcontractorOrder.OrderState AS OrderState,
	DocumentSubcontractorOrder.OrderState.OrderStatus AS OrderStateState,
	CASE
		WHEN (DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
				OR DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess))
				AND NOT DocumentSubcontractorOrder.Closed
			THEN &amp;StatusInProcess
		ELSE CASE
				WHEN DocumentSubcontractorOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Completed)
					THEN &amp;StatusCompleted
				ELSE &amp;StatusCanceled
			END
	END AS OrderStatus,
	DocumentSubcontractorOrder.DocumentTax AS DocumentTax,
	DocumentSubcontractorOrder.Closed AS Closed,
	DocumentSubcontractorOrder.Author AS Author,
	DocumentSubcontractorOrder.PointInTime AS PointInTime,
	ISNULL(PaymentEvent.Amount, 0) - ISNULL(PaymentEvent.PaymentAmount, 0) - ISNULL(PaymentEvent.AdvanceAmount, 0) AS ForPayment,
	ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0) AS PaymentAmountTurnover,
	DocumentSubcontractorOrder.DocumentAmount - DocumentSubcontractorOrder.DocumentTax AS AmountExclTax
FROM
	Document.SubcontractorOrderReceived AS DocumentSubcontractorOrder
		LEFT JOIN InformationRegister.OrderFulfillmentSchedule AS RunSchedule
		ON DocumentSubcontractorOrder.Ref = RunSchedule.Order
			AND (RunSchedule.Period &lt;= &amp;CurrentDateSession)
		LEFT JOIN InformationRegister.OrderFulfillmentSchedule AS TimeSchedulePlan
		ON DocumentSubcontractorOrder.Ref = TimeSchedulePlan.Order
			AND (TimeSchedulePlan.Period > &amp;CurrentDateSession)
		{LEFT JOIN InformationRegister.OrdersPaymentSchedule AS PaymentSchedule
		ON DocumentSubcontractorOrder.Ref = PaymentSchedule.Quote
			AND (PaymentSchedule.Period &lt;= &amp;CurrentDateSession)}
		LEFT JOIN InformationRegister.OrderPayments AS PaymentEvent
		ON DocumentSubcontractorOrder.Ref = PaymentEvent.Quote
		LEFT JOIN Document.SubcontractorInvoiceIssued AS SubcontractorInvoiceIssued
		ON DocumentSubcontractorOrder.Ref = SubcontractorInvoiceIssued.BasisDocument
			AND (SubcontractorInvoiceIssued.Posted)</queryText>
