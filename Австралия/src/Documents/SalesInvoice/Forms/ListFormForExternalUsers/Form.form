  <attributes>
    <name>List</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT ALLOWED
	DocumentSalesInvoice.Ref AS Ref,
	DocumentSalesInvoice.DeletionMark AS DeletionMark,
	DocumentSalesInvoice.Number AS Number,
	DocumentSalesInvoice.Date AS Date,
	DocumentSalesInvoice.Posted AS Posted,
	DocumentSalesInvoice.DocumentCurrency AS DocumentCurrency,
	DocumentSalesInvoice.Contract AS Contract,
	DocumentSalesInvoice.Company AS Company,
	DocumentSalesInvoice.StructuralUnit AS StructuralUnit,
	DocumentSalesInvoice.DocumentAmount AS DocumentAmount,
	DocumentSalesInvoice.DocumentTax AS DocumentTax,
	CASE
		WHEN DocumentSalesInvoice.OperationKind = VALUE(Enum.OperationTypesSalesInvoice.ZeroInvoice)
			THEN VALUE(Enum.InvoicesPaymentStatuses.EmptyRef)
		ELSE ISNULL(InvoicesPaymentStatuses.Status, VALUE(Enum.InvoicesPaymentStatuses.Unpaid))
	END AS PaymentStatus,
	DocumentSalesInvoice.DocumentAmount - DocumentSalesInvoice.DocumentTax AS AmountExclTax,
	DocumentSalesInvoice.Counterparty AS Counterparty
FROM
	Document.SalesInvoice AS DocumentSalesInvoice
		LEFT JOIN InformationRegister.InvoicesPaymentStatuses AS InvoicesPaymentStatuses
		ON (InvoicesPaymentStatuses.Document = DocumentSalesInvoice.Ref)
		LEFT JOIN Catalog.CounterpartyContracts AS CounterpartyContracts
		ON DocumentSalesInvoice.Contract = CounterpartyContracts.Ref
		LEFT JOIN Catalog.Counterparties AS Counterparties
		ON DocumentSalesInvoice.Counterparty = Counterparties.Ref
WHERE
	DocumentSalesInvoice.Counterparty = &amp;Counterparty
	AND NOT DocumentSalesInvoice.DeletionMark
	AND CASE
			WHEN &amp;UseContractRestrictionsTurnOff
				THEN TRUE
			WHEN Counterparties.DoOperationsByContracts
					AND CounterpartyContracts.VisibleToExternalUsers
				THEN TRUE
			WHEN NOT Counterparties.DoOperationsByContracts
				THEN TRUE
			ELSE FALSE
		END</queryText>
      <mainTable>Document.SalesInvoice</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
