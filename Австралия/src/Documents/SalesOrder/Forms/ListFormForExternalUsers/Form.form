    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT ALLOWED
	DocumentSalesOrder.Ref AS Ref,
	-SUM(ISNULL(RegisterSalesOrders.Quantity, 0)) AS Quantity
INTO TT_QuantityWithoutShipment
FROM
	Document.SalesOrder AS DocumentSalesOrder
		LEFT JOIN AccumulationRegister.SalesOrders AS RegisterSalesOrders
		ON (RegisterSalesOrders.SalesOrder = DocumentSalesOrder.Ref)
			AND (RegisterSalesOrders.Quantity &lt; 0)

GROUP BY
	DocumentSalesOrder.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	CASE
		WHEN DocumentSalesOrder.Posted
				AND DocumentSalesOrder.OrderState.OrderStatus &lt;> VALUE(Enum.OrderStatuses.Open)
			THEN CASE
					WHEN NOT DocumentSalesOrder.Closed
							AND ISNULL(SalesOrdersTurnovers.QuantityReceipt, 0) - ISNULL(SalesTurnovers.QuantityTurnover, 0) = 0
						THEN 0
					WHEN DocumentSalesOrder.Closed
							AND ISNULL(SalesOrdersTurnovers.QuantityReceipt, 0) + ISNULL(TT_QuantityWithoutShipment.Quantity, 0) - ISNULL(SalesTurnovers.QuantityTurnover, 0) = 0
							AND ISNULL(SalesTurnovers.QuantityTurnover, 0) > 0
						THEN 0
					WHEN ISNULL(SalesTurnovers.QuantityTurnover, 0) = 0
						THEN CASE
								WHEN NOT DocumentSalesOrder.Closed
									THEN CASE
											WHEN ISNULL(RunSchedule.Period, DocumentSalesOrder.ShipmentDate) &lt; &amp;CurrentDateSession
												THEN 6
											WHEN ISNULL(RunSchedule.Period, DocumentSalesOrder.ShipmentDate) = &amp;CurrentDateSession
												THEN 4
											ELSE 2
										END
								ELSE 2
							END
					ELSE CASE
							WHEN NOT DocumentSalesOrder.Closed
								THEN CASE
										WHEN ISNULL(RunSchedule.Period, DocumentSalesOrder.ShipmentDate) &lt; &amp;CurrentDateSession
											THEN 5
										WHEN ISNULL(RunSchedule.Period, DocumentSalesOrder.ShipmentDate) = &amp;CurrentDateSession
											THEN 3
										ELSE 1
									END
							ELSE 1
						END
				END
		ELSE 2
	END AS ShipmentPictureNumber,
	CASE
		WHEN DocumentSalesOrder.Posted
			THEN CASE
					WHEN (DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
							OR ISNULL(PaymentEvent.Amount, 0) &lt;> 0)
							AND ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0) = 0
						THEN CASE
								WHEN DocumentSalesOrder.SetPaymentTerms
										AND DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
										AND NOT DocumentSalesOrder.Closed
										AND NOT PaymentSchedule.Quote IS NULL
									THEN CASE
											WHEN PaymentSchedule.Period &lt; &amp;CurrentDateSession
												THEN 6
											WHEN PaymentSchedule.Period = &amp;CurrentDateSession
												THEN 4
											ELSE 2
										END
								ELSE 2
							END
					WHEN ISNULL(PaymentEvent.Amount, 0) &lt;= ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0)
						THEN CASE
								WHEN DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
											AND DocumentSalesOrder.Closed
										OR PaymentEvent.Amount IS NULL
									THEN 2
								ELSE 0
							END
					ELSE CASE
							WHEN DocumentSalesOrder.SetPaymentTerms
									AND DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
									AND NOT DocumentSalesOrder.Closed
									AND NOT PaymentSchedule.Quote IS NULL
								THEN CASE
										WHEN PaymentSchedule.Period &lt; &amp;CurrentDateSession
											THEN 5
										WHEN PaymentSchedule.Period = &amp;CurrentDateSession
											THEN 3
										ELSE 1
									END
							ELSE 1
						END
				END
		ELSE 2
	END AS PaymentPictureNumber,
	DocumentSalesOrder.Ref AS Ref,
	DocumentSalesOrder.DeletionMark AS DeletionMark,
	DocumentSalesOrder.Number AS Number,
	DocumentSalesOrder.Date AS Date,
	DocumentSalesOrder.Posted AS Posted,
	DocumentSalesOrder.DocumentCurrency AS DocumentCurrency,
	DocumentSalesOrder.OperationKind AS OperationKind,
	DocumentSalesOrder.ShipmentDate AS ShipmentDate,
	DocumentSalesOrder.Contract AS Contract,
	DocumentSalesOrder.Closed AS Closed,
	DocumentSalesOrder.Counterparty AS Counterparty,
	DocumentSalesOrder.Company AS Company,
	DocumentSalesOrder.OrderState AS OrderState,
	DocumentSalesOrder.OrderState.OrderStatus AS OrderStateState,
	CASE
		WHEN (DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
				OR DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess))
				AND NOT DocumentSalesOrder.Closed
			THEN &amp;StatusInProcess
		ELSE CASE
				WHEN DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Completed)
					THEN &amp;StatusCompleted
				ELSE &amp;StatusCanceled
			END
	END AS OrderStatus,
	DocumentSalesOrder.SalesStructuralUnit AS SalesStructuralUnit,
	DocumentSalesOrder.StructuralUnitReserve AS StructuralUnitReserve,
	DocumentSalesOrder.DocumentTax AS DocumentTax,
	DocumentSalesOrder.DocumentAmount AS DocumentAmount,
	DocumentSalesOrder.Cell AS Cell,
	DocumentSalesOrder.DocumentAmount - DocumentSalesOrder.DocumentTax AS AmountExclTax
FROM
	Document.SalesOrder AS DocumentSalesOrder
		LEFT JOIN InformationRegister.OrderFulfillmentSchedule AS RunSchedule
		ON DocumentSalesOrder.Ref = RunSchedule.Order
			AND (RunSchedule.Period &lt;= &amp;CurrentDateSession)
		{LEFT JOIN InformationRegister.OrdersPaymentSchedule AS PaymentSchedule
		ON DocumentSalesOrder.Ref = PaymentSchedule.Quote
			AND (PaymentSchedule.Period &lt;= &amp;CurrentDateSession)}
		LEFT JOIN InformationRegister.OrderPayments AS PaymentEvent
		ON DocumentSalesOrder.Ref = PaymentEvent.Quote
		LEFT JOIN AccumulationRegister.SalesOrders.Turnovers AS SalesOrdersTurnovers
		ON (SalesOrdersTurnovers.SalesOrder = DocumentSalesOrder.Ref)
		LEFT JOIN AccumulationRegister.Sales.Turnovers AS SalesTurnovers
		ON (SalesTurnovers.SalesOrder = DocumentSalesOrder.Ref)
		LEFT JOIN TT_QuantityWithoutShipment AS TT_QuantityWithoutShipment
		ON (TT_QuantityWithoutShipment.Ref = DocumentSalesOrder.Ref)
		LEFT JOIN Catalog.CounterpartyContracts AS CounterpartyContracts
		ON DocumentSalesOrder.Contract = CounterpartyContracts.Ref
		LEFT JOIN Catalog.Counterparties AS Counterparties
		ON DocumentSalesOrder.Counterparty = Counterparties.Ref
WHERE
	DocumentSalesOrder.Counterparty = &amp;Counterparty
	AND NOT DocumentSalesOrder.DeletionMark
	AND CASE
			WHEN &amp;UseContractRestrictionsTurnOff
				THEN TRUE
			WHEN Counterparties.DoOperationsByContracts
					AND CounterpartyContracts.VisibleToExternalUsers
				THEN TRUE
			WHEN NOT Counterparties.DoOperationsByContracts
				THEN TRUE
			ELSE FALSE
		END</queryText>
