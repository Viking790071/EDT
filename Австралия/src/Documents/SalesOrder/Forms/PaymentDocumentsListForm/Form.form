      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CASE
		WHEN DocumentSalesOrder.Posted
				AND DocumentSalesOrder.OrderState.OrderStatus &lt;> VALUE(Enum.OrderStatuses.Open)
			THEN CASE
					WHEN RunSchedule.Order IS NULL 
							AND TimeSchedulePlan.Order IS NULL 
						THEN CASE
								WHEN DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
										AND DocumentSalesOrder.Closed
									THEN 2
								ELSE 0
							END
					ELSE CASE
							WHEN ISNULL(RunSchedule.Completed, 0) = 0
									AND ISNULL(TimeSchedulePlan.Completed, 0) = 0
								THEN CASE
										WHEN DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
												AND Not DocumentSalesOrder.Closed
											THEN CASE
													WHEN Not RunSchedule.Order IS NULL 
																AND RunSchedule.Period &lt; &amp;CurrentDateSession
														THEN 6
													WHEN Not RunSchedule.Order IS NULL 
																AND RunSchedule.Period = &amp;CurrentDateSession
														THEN 4
													ELSE 2
												END
										ELSE 2
									END
							ELSE CASE
									WHEN DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
											AND Not DocumentSalesOrder.Closed
										THEN CASE
												WHEN Not RunSchedule.Order IS NULL 
															AND RunSchedule.Period &lt; &amp;CurrentDateSession
													THEN 5
												WHEN Not RunSchedule.Order IS NULL 
															AND RunSchedule.Period = &amp;CurrentDateSession
													THEN 3
												ELSE 1
											END
									ELSE 1
								END
						END
				END
		ELSE 2
	END AS ShipmentPictureNumber,
	CASE
		WHEN DocumentSalesOrder.Posted
			THEN CASE
					WHEN (DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
							OR ISNULL(PaymentEvent.Amount, 0) &lt;> 0)
							AND ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0) = 0
						THEN CASE
								WHEN DocumentSalesOrder.SetPaymentTerms
										AND DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
										AND Not DocumentSalesOrder.Closed
										AND Not PaymentSchedule.Quote IS NULL 
									THEN CASE
											WHEN PaymentSchedule.Period &lt; &amp;CurrentDateSession
												THEN 6
											WHEN PaymentSchedule.Period = &amp;CurrentDateSession
												THEN 4
											ELSE 2
										END
								ELSE 2
							END
					WHEN ISNULL(PaymentEvent.Amount, 0) &lt;= ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0)
						THEN CASE
								WHEN DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
										AND DocumentSalesOrder.Closed
									THEN 2
								ELSE 0
							END
					ELSE CASE
							WHEN DocumentSalesOrder.SetPaymentTerms
									AND DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
									AND Not DocumentSalesOrder.Closed
									AND Not PaymentSchedule.Quote IS NULL 
								THEN CASE
										WHEN PaymentSchedule.Period &lt; &amp;CurrentDateSession
											THEN 5
										WHEN PaymentSchedule.Period = &amp;CurrentDateSession
											THEN 3
										ELSE 1
									END
							ELSE 1
						END
				END
		ELSE 2
	END AS PaymentPictureNumber,
	DocumentSalesOrder.Ref,
	DocumentSalesOrder.Number,
	DocumentSalesOrder.Date,
	DocumentSalesOrder.Author,
	DocumentSalesOrder.DocumentCurrency,
	DocumentSalesOrder.ShipmentDate,
	DocumentSalesOrder.Contract,
	DocumentSalesOrder.Comment,
	DocumentSalesOrder.Counterparty,
	DocumentSalesOrder.Company AS Company,
	DocumentSalesOrder.Responsible,
	DocumentSalesOrder.OrderState,
	CASE
		WHEN (DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
				OR DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess))
				AND Not DocumentSalesOrder.Closed
			THEN &quot;In process&quot;
		ELSE CASE
				WHEN DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Completed)
					THEN &quot;Completed&quot;
				ELSE &quot;Canceled&quot;
			END
	END AS OrderStatus,
	DocumentSalesOrder.Start,
	DocumentSalesOrder.SalesStructuralUnit,
	DocumentSalesOrder.StructuralUnitReserve,
	DocumentSalesOrder.DocumentAmount,
	DocumentSalesOrder.Finish,
	DocumentSalesOrder.Cell,
	CASE
		WHEN RunSchedule.Order IS NULL 
				AND TimeSchedulePlan.Order IS NULL 
			THEN 0
		ELSE 1
	END AS ForShipment,
	ISNULL(PaymentEvent.Amount, 0) - ISNULL(PaymentEvent.PaymentAmount, 0) - ISNULL(PaymentEvent.AdvanceAmount, 0) AS ForPayment,
	CASE
		WHEN DocumentSalesOrder.Posted
				AND DocumentSalesOrder.DocumentAmount > 0
			THEN CAST((ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0)) * 100 / DocumentSalesOrder.DocumentAmount AS NUMBER(15, 2))
		ELSE 0
	END AS PaymentPercentage,
	ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0) AS Paid,
	DocumentSalesOrder.Counterparty.ContactPerson AS ContactPerson
FROM
	AccumulationRegister.SalesOrders.BalanceAndTurnovers(
			,
			,
			Auto,
			,
			Products.ProductsType = VALUE(Enum.ProductsTypes.InventoryItem)
				OR Products.ProductsType = VALUE(Enum.ProductsTypes.Service)
				OR Products.ProductsType = VALUE(Enum.ProductsTypes.Work)) AS SalesOrdersBalanceAndTurnovers
		LEFT JOIN Document.SalesOrder AS DocumentSalesOrder
		ON SalesOrdersBalanceAndTurnovers.SalesOrder = DocumentSalesOrder.Ref
		LEFT JOIN InformationRegister.OrderFulfillmentSchedule AS RunSchedule
		ON (DocumentSalesOrder.Ref = RunSchedule.Order)
			AND (RunSchedule.Period &lt;= &amp;CurrentDateSession)
		LEFT JOIN InformationRegister.OrderFulfillmentSchedule AS TimeSchedulePlan
		ON (DocumentSalesOrder.Ref = TimeSchedulePlan.Order)
			AND (TimeSchedulePlan.Period > &amp;CurrentDateSession)
		{LEFT JOIN InformationRegister.OrdersPaymentSchedule AS PaymentSchedule
		ON (DocumentSalesOrder.Ref = PaymentSchedule.Quote)
			AND (PaymentSchedule.Period &lt;= &amp;CurrentDateSession)}
		LEFT JOIN InformationRegister.OrderPayments AS PaymentEvent
		ON (DocumentSalesOrder.Ref = PaymentEvent.Quote)
WHERE
	(ISNULL(PaymentEvent.Amount, 0) - ISNULL(PaymentEvent.PaymentAmount, 0) - ISNULL(PaymentEvent.AdvanceAmount, 0) > 0
			OR DocumentSalesOrder.Ref IN (&amp;OrdersList))
		AND Not DocumentSalesOrder.Closed</queryText>
