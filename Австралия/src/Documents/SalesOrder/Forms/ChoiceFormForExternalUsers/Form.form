    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DocumentSalesOrder.Ref AS Ref,
	DocumentSalesOrder.DataVersion AS DataVersion,
	DocumentSalesOrder.DeletionMark AS DeletionMark,
	DocumentSalesOrder.Number AS Number,
	DocumentSalesOrder.Date AS Date,
	DocumentSalesOrder.Posted AS Posted,
	DocumentSalesOrder.Author AS Author,
	DocumentSalesOrder.BankAccount AS BankAccount,
	DocumentSalesOrder.DocumentCurrency AS DocumentCurrency,
	DocumentSalesOrder.OperationKind AS OperationKind,
	DocumentSalesOrder.WorkKind AS WorkKind,
	DocumentSalesOrder.DiscountMarkupKind AS DiscountMarkupKind,
	DocumentSalesOrder.PriceKind AS PriceKind,
	DocumentSalesOrder.ShipmentDate AS ShipmentDate,
	DocumentSalesOrder.Contract AS Contract,
	DocumentSalesOrder.Closed AS Closed,
	DocumentSalesOrder.SetPaymentTerms AS SetPaymentTerms,
	DocumentSalesOrder.PettyCash AS PettyCash,
	DocumentSalesOrder.Comment AS Comment,
	DocumentSalesOrder.Counterparty AS Counterparty,
	DocumentSalesOrder.Multiplicity AS Multiplicity,
	DocumentSalesOrder.ExchangeRate AS ExchangeRate,
	DocumentSalesOrder.IncludeVATInPrice AS IncludeVATInPrice,
	DocumentSalesOrder.Company AS Company,
	DocumentSalesOrder.Responsible AS Responsible,
	DocumentSalesOrder.Project AS Project,
	DocumentSalesOrder.Event AS Event,
	DocumentSalesOrder.OrderState AS OrderState,
	CASE
		WHEN (DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
				OR DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess))
				AND NOT DocumentSalesOrder.Closed
			THEN &amp;StatusInProcess
		ELSE CASE
				WHEN DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Completed)
					THEN &amp;StatusCompleted
				ELSE &amp;StatusCanceled
			END
	END AS OrderStatus,
	DocumentSalesOrder.Start AS Start,
	DocumentSalesOrder.SalesStructuralUnit AS SalesStructuralUnit,
	DocumentSalesOrder.StructuralUnitReserve AS StructuralUnitReserve,
	DocumentSalesOrder.VATTaxation AS VATTaxation,
	DocumentSalesOrder.AmountIncludesVAT AS AmountIncludesVAT,
	DocumentSalesOrder.DocumentTax AS DocumentTax,
	DocumentSalesOrder.DocumentAmount AS DocumentAmount,
	DocumentSalesOrder.CashAssetType AS CashAssetType,
	DocumentSalesOrder.Finish AS Finish,
	DocumentSalesOrder.Cell AS Cell,
	DocumentSalesOrder.WorkKindPosition AS WorkKindPosition,
	DocumentSalesOrder.ShipmentDatePosition AS ShipmentDatePosition,
	DocumentSalesOrder.UseProducts AS UseProducts,
	DocumentSalesOrder.UseConsumerMaterials AS UseConsumerMaterials,
	DocumentSalesOrder.UseMaterials AS UseMaterials,
	DocumentSalesOrder.UsePerformerSalaries AS UsePerformerSalaries,
	DocumentSalesOrder.ProductsList AS ProductsList,
	DocumentSalesOrder.PointInTime AS PointInTime,
	DocumentSalesOrder.DocumentAmount - DocumentSalesOrder.DocumentTax AS AmountExclTax,
	DocumentSalesOrder.SalesRep AS SalesRep
FROM
	Document.SalesOrder AS DocumentSalesOrder
		INNER JOIN Catalog.SalesOrderStatuses AS SalesOrderStatuses
		ON DocumentSalesOrder.OrderState = SalesOrderStatuses.Ref
		LEFT JOIN Catalog.CounterpartyContracts AS CounterpartyContracts
		ON DocumentSalesOrder.Contract = CounterpartyContracts.Ref
		LEFT JOIN Catalog.Counterparties AS Counterparties
		ON DocumentSalesOrder.Counterparty = Counterparties.Ref
WHERE
	DocumentSalesOrder.Counterparty = &amp;Counterparty
	AND NOT DocumentSalesOrder.DeletionMark
	AND CASE
			WHEN &amp;UseContractRestrictionsTurnOff
				THEN TRUE
			WHEN Counterparties.DoOperationsByContracts
					AND CounterpartyContracts.VisibleToExternalUsers
				THEN TRUE
			WHEN NOT Counterparties.DoOperationsByContracts
				THEN TRUE
			ELSE FALSE
		END</queryText>
      <mainTable>Document.SalesOrder</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
