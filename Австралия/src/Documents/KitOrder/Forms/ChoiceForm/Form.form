    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	KitOrder.Ref AS Ref,
	KitOrder.DataVersion AS DataVersion,
	KitOrder.DeletionMark AS DeletionMark,
	KitOrder.Number AS Number,
	KitOrder.Date AS Date,
	KitOrder.Posted AS Posted,
	KitOrder.OperationKind AS OperationKind,
	KitOrder.Company AS Company,
	KitOrder.SalesOrder AS SalesOrder,
	KitOrder.Start AS Start,
	KitOrder.Finish AS Finish,
	KitOrder.StructuralUnit AS StructuralUnit,
	KitOrder.Comment AS Comment,
	KitOrder.BasisDocument AS BasisDocument,
	KitOrder.OrderState AS OrderState,
	CASE
		WHEN (KitOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
				OR KitOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess))
				AND NOT KitOrder.Closed
			THEN &amp;StatusInProcess
		ELSE CASE
				WHEN KitOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Completed)
					THEN &amp;StatusCompleted
				ELSE &amp;StatusCanceled
			END
	END AS OrderStatus,
	KitOrder.Closed AS Closed,
	KitOrder.Responsible AS Responsible,
	KitOrder.Author AS Author,
	KitOrder.ProductsList AS ProductsList,
	KitOrder.Inventory.(
		Ref AS Ref,
		LineNumber AS LineNumber,
		Products AS Products,
		Characteristic AS Characteristic,
		Quantity AS Quantity,
		MeasurementUnit AS MeasurementUnit,
		Specification AS Specification
	) AS Inventory,
	KitOrder.PointInTime AS PointInTime
FROM
	Document.KitOrder AS KitOrder</queryText>
      <mainTable>Document.KitOrder</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
