  <attributes>
    <name>List</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DocumentPaymentExpense.Ref AS Ref,
	DocumentPaymentExpense.DeletionMark AS DeletionMark,
	DocumentPaymentExpense.Number AS Number,
	DocumentPaymentExpense.Date AS Date,
	DocumentPaymentExpense.Posted AS Posted,
	DocumentPaymentExpense.Company AS Company,
	DocumentPaymentExpense.Comment AS Comment,
	DocumentPaymentExpense.OperationKind AS OperationKind,
	DocumentPaymentExpense.BankAccount AS BankAccount,
	DocumentPaymentExpense.Item AS Item,
	DocumentPaymentExpense.BasisDocument AS BasisDocument,
	CASE
		WHEN DocumentPaymentExpense.OperationKind = VALUE(Enum.OperationTypesPaymentExpense.ToAdvanceHolder)
				OR DocumentPaymentExpense.OperationKind = VALUE(Enum.OperationTypesPaymentExpense.IssueLoanToEmployee)
			THEN DocumentPaymentExpense.AdvanceHolder
		WHEN DocumentPaymentExpense.OperationKind = VALUE(Enum.OperationTypesPaymentExpense.Other)
				OR DocumentPaymentExpense.OperationKind = VALUE(Enum.OperationTypesPaymentExpense.Taxes)
				OR DocumentPaymentExpense.OperationKind = VALUE(Enum.OperationTypesPaymentExpense.Salary)
			THEN DocumentPaymentExpense.Item
		ELSE DocumentPaymentExpense.Counterparty
	END AS Counterparty,
	DocumentPaymentExpense.AdvanceHolder AS AdvanceHolder,
	DocumentPaymentExpense.Document AS Document,
	DocumentPaymentExpense.DocumentAmount + CASE
		WHEN DocumentPaymentExpense.UseBankCharges
			THEN DocumentPaymentExpense.BankChargeAmount
		ELSE 0
	END AS DocumentAmount,
	DocumentPaymentExpense.CashCurrency AS CashCurrency,
	DocumentPaymentExpense.Correspondence AS Correspondence,
	DocumentPaymentExpense.TaxKind AS TaxKind,
	DocumentPaymentExpense.ExternalDocumentDate AS ExternalDocumentDate,
	DocumentPaymentExpense.ExternalDocumentNumber AS ExternalDocumentNumber,
	DocumentPaymentExpense.PaymentPurpose AS PaymentPurpose,
	DocumentPaymentExpense.BusinessLine AS BusinessLine,
	DocumentPaymentExpense.Department AS Department,
	DocumentPaymentExpense.Order AS Order,
	DocumentPaymentExpense.VATTaxation AS VATTaxation,
	DocumentPaymentExpense.CounterpartyAccount AS CounterpartyAccount,
	DocumentPaymentExpense.Author AS Author,
	DocumentPaymentExpense.PaymentDetails.(
		Ref AS Ref,
		LineNumber AS LineNumber,
		Contract AS Contract,
		AdvanceFlag AS AdvanceFlag,
		Document AS Document,
		SettlementsAmount AS SettlementsAmount,
		ExchangeRate AS ExchangeRate,
		Multiplicity AS Multiplicity,
		PaymentAmount AS PaymentAmount,
		VATRate AS VATRate,
		VATAmount AS VATAmount,
		PaymentDetails.Order AS Order,
		PlanningDocument AS PlanningDocument,
		TypeOfAmount AS TypeOfAmount,
		EPDAmount AS EPDAmount,
		SettlementsEPDAmount AS SettlementsEPDAmount,
		ExistsEPD AS ExistsEPD,
		AccountsPayableGLAccount AS AccountsPayableGLAccount,
		AdvancesPaidGLAccount AS AdvancesPaidGLAccount,
		AccountsReceivableGLAccount AS AccountsReceivableGLAccount,
		AdvancesReceivedGLAccount AS AdvancesReceivedGLAccount,
		DiscountReceivedGLAccount AS DiscountReceivedGLAccount,
		Item AS Item,
		Project AS Project
	) AS PaymentDetails,
	DocumentPaymentExpense.PayrollPayment.(
		Ref AS Ref,
		LineNumber AS LineNumber,
		Statement AS Statement,
		PaymentAmount AS PaymentAmount,
		PlanningDocument AS PlanningDocument
	) AS PayrollPayment,
	DocumentPaymentExpense.PointInTime AS PointInTime
FROM
	Document.PaymentExpense AS DocumentPaymentExpense</queryText>
      <mainTable>Document.PaymentExpense</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
