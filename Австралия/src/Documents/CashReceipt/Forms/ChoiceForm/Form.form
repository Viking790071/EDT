    <name>List</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DocumentCashReceipt.Ref,
	DocumentCashReceipt.DeletionMark,
	DocumentCashReceipt.Number,
	DocumentCashReceipt.Date,
	DocumentCashReceipt.Posted,
	DocumentCashReceipt.Company,
	DocumentCashReceipt.Comment,
	DocumentCashReceipt.OperationKind,
	DocumentCashReceipt.PettyCash,
	DocumentCashReceipt.Item,
	DocumentCashReceipt.AcceptedFrom,
	DocumentCashReceipt.Basis,
	DocumentCashReceipt.Application,
	CASE
		WHEN DocumentCashReceipt.OperationKind = VALUE(Enum.OperationTypesCashReceipt.FromAdvanceHolder)
				OR DocumentCashReceipt.OperationKind = VALUE(Enum.OperationTypesCashReceipt.LoanRepaymentByEmployee)
			THEN DocumentCashReceipt.AdvanceHolder
		WHEN DocumentCashReceipt.OperationKind = VALUE(Enum.OperationTypesCashReceipt.Other)
				OR DocumentCashReceipt.OperationKind = VALUE(Enum.OperationTypesCashReceipt.CurrencyPurchase)
			THEN DocumentCashReceipt.Item
		WHEN DocumentCashReceipt.OperationKind = VALUE(Enum.OperationTypesCashReceipt.RetailIncome)
			THEN DocumentCashReceipt.CashCR
		WHEN DocumentCashReceipt.OperationKind = VALUE(Enum.OperationTypesCashReceipt.RetailIncomeEarningAccounting)
			THEN DocumentCashReceipt.StructuralUnit
		ELSE DocumentCashReceipt.Counterparty
	END AS Counterparty,
	DocumentCashReceipt.AdvanceHolder,
	DocumentCashReceipt.Document,
	DocumentCashReceipt.DocumentAmount,
	DocumentCashReceipt.Correspondence,
	DocumentCashReceipt.CashCurrency,
	DocumentCashReceipt.BasisDocument,
	DocumentCashReceipt.CashCR,
	DocumentCashReceipt.StructuralUnit,
	DocumentCashReceipt.Department,
	DocumentCashReceipt.BusinessLine,
	DocumentCashReceipt.SalesSlipNumber,
	DocumentCashReceipt.VATTaxation,
	DocumentCashReceipt.Author,
	DocumentCashReceipt.AccountingAmount,
	DocumentCashReceipt.ExchangeRate,
	DocumentCashReceipt.Multiplicity,
	DocumentCashReceipt.LoanContract,
	DocumentCashReceipt.PaymentDetails,
	DocumentCashReceipt.AdditionalAttributes,
	DocumentCashReceipt.PointInTime
FROM
	Document.CashReceipt AS DocumentCashReceipt</queryText>
      <mainTable>Document.CashReceipt</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
  </attributes>
