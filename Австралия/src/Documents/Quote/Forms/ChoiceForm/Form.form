    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DocumentQuote.Ref AS Ref,
	DocumentQuote.DeletionMark AS DeletionMark,
	DocumentQuote.Number AS Number,
	DocumentQuote.Date AS Date,
	DocumentQuote.Posted AS Posted,
	DocumentQuote.Company AS Company,
	DocumentQuote.Comment AS Comment,
	DocumentQuote.Counterparty AS Counterparty,
	DocumentQuote.Contract AS Contract,
	DocumentQuote.DocumentCurrency AS DocumentCurrency,
	DocumentQuote.Department AS Department,
	DocumentQuote.Responsible AS Responsible,
	DocumentQuote.DocumentTax AS DocumentTax,
	DocumentQuote.DocumentAmount AS DocumentAmount,
	DocumentQuote.Author AS Author,
	CASE
		WHEN DocumentQuote.Posted
			THEN ISNULL(QuotationStatuses.Status, VALUE(Enum.QuotationStatuses.Sent))
		ELSE VALUE(Enum.QuotationStatuses.Draft)
	END AS Status,
	DocumentQuote.DocumentAmount - DocumentQuote.DocumentTax AS AmountExclTax,
	DocumentQuote.SalesRep AS SalesRep
FROM
	Document.Quote AS DocumentQuote
		LEFT JOIN InformationRegister.QuotationStatuses AS QuotationStatuses
		ON (QuotationStatuses.Document = DocumentQuote.Ref)</queryText>
      <mainTable>Document.Quote</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
  </attributes>
